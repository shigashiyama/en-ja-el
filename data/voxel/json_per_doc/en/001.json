    "sentences": {
      "00": {
        "orig_text": "Unemployment in the EU at record low since 2008.",
        "text": "Unemployment in the EU at record low since 2008.",
        "mention_ids": [
          "M001"
        ]
      },
      "01": {
        "orig_text": "According to the latest Eurostat figures the unemployment has hit its lowest rate since December 2008 for the second month in a row in June.",
        "text": "According to the latest Eurostat figures the unemployment has hit its lowest rate since December 2008 for the second month in a row in June.",
        "mention_ids": [
          "M002"
        ]
      },
      "02": {
        "orig_text": "According to the latest Eurostat findings, the unemployment in the EU was 7.7 percent in June 2017, the same rate as in May, and down from 8.6 percent in June 2016.",
        "text": "According to the latest Eurostat findings, the unemployment in the EU was 7.7 percent in June 2017, the same rate as in May, and down from 8.6 percent in June 2016.",
        "mention_ids": [
          "M003",
          "M004"
        ]
      },
      "03": {
        "orig_text": "The current rate is the lowest recorded in the EU since December 2008.",
        "text": "The current rate is the lowest recorded in the EU since December 2008.",
        "mention_ids": [
          "M005"
        ]
      },
      "04": {
        "orig_text": "The euro area seasonally-adjusted unemployment was 9.1 percent in June 2017, down from 9.2 percent in May 2017 and down from 10.1 percent in June 2016.",
        "text": "The euro area seasonally-adjusted unemployment was 9.1 percent in June 2017, down from 9.2 percent in May 2017 and down from 10.1 percent in June 2016.",
        "mention_ids": []
      },
      "05": {
        "orig_text": "This is the lowest rate recorded in the euro area since February 2009.",
        "text": "This is the lowest rate recorded in the euro area since February 2009.",
        "mention_ids": []
      },
      "06": {
        "orig_text": "Eurostat estimates that 18.725 million men and women in the EU28, of whom 14.718 million in the euro area, were unemployed in June 2017.",
        "text": "Eurostat estimates that 18.725 million men and women in the EU28, of whom 14.718 million in the euro area, were unemployed in June 2017.",
        "mention_ids": [
          "M006",
          "M007"
        ]
      },
      "07": {
        "orig_text": "Among the Member States, the lowest unemployment in June 2017 was recorded in the Czech Republic (2.9 percent), Germany (3.8 percent) and Malta (4.1percent).",
        "text": "Among the Member States, the lowest unemployment in June 2017 was recorded in the Czech Republic (2.9 percent), Germany (3.8 percent) and Malta (4.1percent).",
        "mention_ids": [
          "M008",
          "M009",
          "M010"
        ]
      },
      "08": {
        "orig_text": "The highest unemployment were observed in Greece (21.7 percent in April 2017) and Spain (17.1 percent).",
        "text": "The highest unemployment were observed in Greece (21.7 percent in April 2017) and Spain (17.1 percent).",
        "mention_ids": [
          "M011",
          "M012"
        ]
      },
      "09": {
        "orig_text": "Compared with a year ago, unemployment fell in all Member States for which data is comparable over time, except Estonia which showed an increase (from 6.5 percent in May 2016 to 6.9 percent in May 2017).",
        "text": "Compared with a year ago, unemployment fell in all Member States for which data is comparable over time, except Estonia which showed an increase (from 6.5 percent in May 2016 to 6.9 percent in May 2017).",
        "mention_ids": [
          "M013"
        ]
      },
      "10": {
        "orig_text": "The largest decreases were registered in Spain (from 19.9 percent to 17.1 percent) and Croatia (from 13.3 percent to 10.6 percent).",
        "text": "The largest decreases were registered in Spain (from 19.9 percent to 17.1 percent) and Croatia (from 13.3 percent to 10.6 percent).",
        "mention_ids": [
          "M014",
          "M015"
        ]
      },
      "11": {
        "orig_text": "As a comparison, in June 2017, the unemployment in the United States was 4.4 percent, up from 4.3 percent in May 2017 but down from 4.9 percent in June 2016.",
        "text": "As a comparison, in June 2017, the unemployment in the United States was 4.4 percent, up from 4.3 percent in May 2017 but down from 4.9 percent in June 2016.",
        "mention_ids": [
          "M016"
        ]
      }
    },
