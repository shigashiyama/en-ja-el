    "sentences": {
      "00": {
        "orig_text": "On 2 February David Cameron and European Council president Donald Tusk agreed on a project settlement that would allow British Prime minister to campaign to keep the UK in the EU in the referendum to be held later this year.",
        "text": "On 2 February David Cameron and European Council president Donald Tusk agreed on a project settlement that would allow British Prime minister to campaign to keep the UK in the EU in the referendum to be held later this year.",
        "mention_ids": [
          "M054",
          "M055",
          "M056",
          "M057"
        ]
      },
      "01": {
        "orig_text": "The plan is a response to Cameron's demands, offering Britain, which already enjoys several exceptions from EU treaty, a gradual brake on social benefits for EU workers, and  cuts for the children of EU workers living outside Britain.",
        "text": "The plan is a response to Cameron's demands, offering Britain, which already enjoys several exceptions from EU treaty, a gradual brake on social benefits for EU workers, and cuts for the children of EU workers living outside Britain.",
        "mention_ids": [
          "M058",
          "M059",
          "M060",
          "M061",
          "M062"
        ]
      },
      "02": {
        "orig_text": "The pact also states that treaty references to “ever-closer union” are “not an equivalent to the objective of political integration”.",
        "text": "The pact also states that treaty references to “ever-closer union” are “not an equivalent to the objective of political integration”.",
        "mention_ids": [
          "M063"
        ]
      },
      "03": {
        "orig_text": "The pact has to be agreed at an EU summit starting on 18 February.",
        "text": "The pact has to be agreed at an EU summit starting on 18 February.",
        "mention_ids": [
          "M064"
        ]
      }
