    "sentences": {
      "00": {
        "orig_text": "A map of asylum seekers by country.",
        "text": "A map of asylum seekers by country.",
        "mention_ids": []
      },
      "01": {
        "orig_text": "All together, more than 400,000 people have requested asylum in Europe over the first six months of the year.",
        "text": "All together, more than 400,000 people have requested asylum in Europe over the first six months of the year.",
        "mention_ids": [
          "M098"
        ]
      },
      "02": {
        "orig_text": "The small map shows countries as proportional to their population.",
        "text": "The small map shows countries as proportional to their population.",
        "mention_ids": []
      },
      "03": {
        "orig_text": "States which appear larger than normal have taken in a particularly high number of asylum seekers.",
        "text": "States which appear larger than normal have taken in a particularly high number of asylum seekers.",
        "mention_ids": []
      },
      "04": {
        "orig_text": "These are for the most part countries in the centre of Europe, with Germany topping the list, but also Switzerland, Austria, Hungary, Benelux and Sweden.",
        "text": "These are for the most part countries in the centre of Europe, with Germany topping the list, but also Switzerland, Austria, Hungary, Benelux and Sweden.",
        "mention_ids": [
          "M099",
          "M100",
          "M101",
          "M102",
          "M103",
          "M104",
          "M105"
        ]
      },
      "05": {
        "orig_text": "Apart from Malta and Italy, all the 'countries' of Europe have welcomed a relatively low number of asylum seekers – for certain countries so few that they practically disappear of the map.",
        "text": "Apart from Malta and Italy, all the 'countries' of Europe have welcomed a relatively low number of asylum seekers – for certain countries so few that they practically disappear of the map.",
        "mention_ids": [
          "M106",
          "M107",
          "M108"
        ]
      },
      "06": {
        "orig_text": "The map was published by geographer Benjamin Henning on his site, where a 2014 map is also available in this year.",
        "text": "The map was published by geographer Benjamin Henning on his site, where a 2014 map is also available in this year.",
        "mention_ids": [
          "M109"
        ]
      },
      "07": {
        "orig_text": "The data used are from Eurostat.",
