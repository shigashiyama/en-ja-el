      "00": {
        "orig_text": "Old Europe.",
        "text": "Old Europe.",
        "mention_ids": [
          "M087"
        ]
      },
      "01": {
        "orig_text": "On 7 October, Operation Triton, a mission monitoring migration routes in Italy, was joined by Operation Sophia. ",
        "text": "On 7 October, Operation Triton, a mission monitoring migration routes in Italy, was joined by Operation Sophia. ",
        "mention_ids": [
          "M088"
        ]
      },
      "02": {
        "orig_text": "It consists of intercepting smugglers in international waters, arresting and chasing them and boarding their boats, using the force if necessary. ",
        "text": "It consists of intercepting smugglers in international waters, arresting and chasing them and boarding their boats, using the force if necessary. ",
        "mention_ids": []
      },
      "03": {
        "orig_text": " \"Sophia\" can boasts nine warships and 1,300 men, taking its name from a little girl who was born while crossing the Mediterranean.",
        "text": " \"Sophia\" can boasts nine warships and 1,300 men, taking its name from a little girl who was born while crossing the Mediterranean.",
        "mention_ids": [
          "M089"
        ]
      },
      "04": {
        "orig_text": "Since the beginning of the year, 564,000 people arrived in Europe via the Mediterranean and some 3,000 death, according to the International Organisation for Migration.",
        "text": "Since the beginning of the year, 564,000 people arrived in Europe via the Mediterranean and some 3,000 death, according to the International Organisation for Migration.",
        "mention_ids": [
          "M090",
          "M091",
          "M092"
        ]
      },
      "05": {
        "orig_text": "This cartoon by Marilena Nardi was awarded the first prize for the best cartoon on Europe published in Italy, given in 2015 during Internazionale's festival in Ferrara, on 4 October.",
        "text": "This cartoon by Marilena Nardi was awarded the first prize for the best cartoon on Europe published in Italy, given in 2015 during Internazionale's festival in Ferrara, on 4 October.",
        "mention_ids": [
          "M093",
          "M094",
          "M095",
          "M096",
          "M097"
        ]
      }
