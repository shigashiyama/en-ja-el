    "sentences": {
      "00": {
        "orig_text": "Briggs Terrace , also known as Evergreen Lane , is a nationally recognized historic district located in Nevada , Iowa , United States .",
        "text": "Briggs Terrace , also known as Evergreen Lane , is a nationally recognized historic district located in Nevada , Iowa , United States .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It was listed on the National Register of Historic Places in 1998 .",
        "text": "It was listed on the National Register of Historic Places in 1998 .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "At the time of its nomination it consisted of eight resources , which included six contributing buildings , one contributing site , and two non - contributing buildings .",
        "text": "At the time of its nomination it consisted of eight resources , which included six contributing buildings , one contributing site , and two non - contributing buildings .",
        "mention_ids": [
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "text": "This estate was established and built by ■■■■■■■■■■■ , a local banker who founded Farmers Bank in Nevada .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M1_1"
        ]
      },
      "04": {
        "orig_text": "He arrived in town in 1857 from Des Moines , four years after Nevada and Story County were founded .",
        "text": "He arrived in town in 1857 from Des Moines , four years after Nevada and Story County were founded .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0",
          "M15_0",
          "M1_2"
        ]
      },
      "05": {
        "orig_text": "He worked in a variety of commercial ventures before becoming a banker .",
        "text": "He worked in a variety of commercial ventures before becoming a banker .",
        "mention_ids": []
      },
      "06": {
        "text": "■■■■■■ invested heavily in real estate , and he became one of the largest land owners in the county .",
        "mention_ids": [
          "M10_1"
        ]
      },
      "07": {
        "orig_text": "The two - story , brick , Italianate style house was completed in 1879 .",
        "text": "The two - story , brick , Italianate style house was completed in 1879 .",
        "mention_ids": [
          "M16_0",
          "M17_0",
          "M18_0"
        ]
      },
      "08": {
        "orig_text": "It features floor - length windows , a wraparound porch , and a bay window .",
        "text": "It features floor - length windows , a wraparound porch , and a bay window .",
        "mention_ids": []
      },
      "09": {
        "orig_text": "It is surrounded by an planted grounds , which contribute to the historic nature of the district .",
        "text": "It is surrounded by an planted grounds , which contribute to the historic nature of the district .",
        "mention_ids": []
      },
      "10": {
        "orig_text": "The other historic building located here are the carriage house ( c. 1879 ) , barn ( c. 1879 ) , and late 19th century out buildings .",
        "text": "The other historic building located here are the carriage house ( c. 1879 ) , barn ( c. 1879 ) , and late 19th century out buildings .",
        "mention_ids": [
          "M18_1",
          "M18_2",
          "M19_0"
        ]
      }
