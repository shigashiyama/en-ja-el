        ]
      },
      "01": {
        "orig_text": "At the 2010 census , the population was 4,133 .",
        "text": "At the 2010 census , the population was 4,133 .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Isle of Palms is a barrier island on the South Carolina coast .",
        "text": "Isle of Palms is a barrier island on the South Carolina coast .",
        "mention_ids": [
          "M0_1",
          "M2_1"
        ]
      },
      "03": {
        "orig_text": "The city is included within the Charleston - North Charleston - Summerville metropolitan area and the Charleston - North Charleston Urbanized Area .",
        "text": "The city is included within the Charleston - North Charleston - Summerville metropolitan area and the Charleston - North Charleston Urbanized Area .",
        "mention_ids": [
          "M6_0",
