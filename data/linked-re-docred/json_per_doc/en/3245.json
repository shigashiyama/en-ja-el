    "sentences": {
      "00": {
        "orig_text": "The Honourable Sir Sidney Cornwallis Peel , 1st Baronet DSO ( 3 June 1870 – 19 December 1938 ) , was a British soldier , financier and Conservative politician .",
        "text": "The Honourable Sir Sidney Cornwallis Peel , 1st Baronet DSO ( 3 June 1870 – 19 December 1938 ) , was a British soldier , financier and Conservative politician .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "Peel was the second son of Arthur Peel , 1st Viscount Peel , Speaker of the House of Commons and the youngest son of Prime Minister Sir Robert Peel , Bt .",
        "text": "Peel was the second son of Arthur Peel , 1st Viscount Peel , Speaker of the House of Commons and the youngest son of Prime Minister Sir Robert Peel , Bt .",
        "mention_ids": [
          "M0_1",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "02": {
        "text": "His mother was ■■■■■■■■ , daughter of William Stratford Dugdale .",
        "mention_ids": [
          "M10_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "Peel sat as Member of Parliament for Uxbridge bretween 1918 and 1922 .",
        "text": "Peel sat as Member of Parliament for Uxbridge bretween 1918 and 1922 .",
        "mention_ids": [
          "M0_2",
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "04": {
        "orig_text": "He was also a Colonel in the British Army .",
        "text": "He was also a Colonel in the British Army .",
        "mention_ids": [
          "M14_0"
        ]
      },
      "05": {
        "orig_text": "In 1936 he was created a Baronet , of Eyeworth in the County of Bedford .",
        "text": "In 1936 he was created a Baronet , of Eyeworth in the County of Bedford .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      },
      "06": {
        "text": "Peel married ■■■■■■■■■■■■■■■■■■■■■■■■■■■■ , daughter of Charles Spencer , 6th Earl Spencer , in 1914 .",
        "mention_ids": [
          "M0_3",
          "M18_0",
          "M19_0",
          "M20_0"
        ]
      },
      "07": {
        "orig_text": "He died in December 1938 , aged 68 , at which time the baronetcy became extinct .",
        "text": "He died in December 1938 , aged 68 , at which time the baronetcy became extinct .",
        "mention_ids": [
          "M21_0",
          "M22_0"
        ]
      },
      "08": {
        "text": "■■■■■■■■■ , who was 19 years younger than her husband , died in January 1981 , aged 91 .",
        "mention_ids": [
          "M23_0",
          "M24_0",
          "M25_0",
          "M26_0"
        ]
      }
    },
