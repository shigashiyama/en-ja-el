    "sentences": {
      "00": {
        "orig_text": "David Low Hackett ( November 12 , 1926 – April 23 , 2011 ) was an American official .",
        "text": "David Low Hackett ( November 12 , 1926 – April 23 , 2011 ) was an American official .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "Born in Dedham , Massachusetts , Hackett was appointed by President John F. Kennedy to head the President 's Committee on Juvenile Delinquency and Youth Crime .",
        "text": "Born in Dedham , Massachusetts , Hackett was appointed by President John F. Kennedy to head the President 's Committee on Juvenile Delinquency and Youth Crime .",
        "mention_ids": [
          "M0_4",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "Later , Hackett headed a study group for the establishment of a domestic peace corps group which later became AmeriCorps Vista .",
        "text": "Later , Hackett headed a study group for the establishment of a domestic peace corps group which later became AmeriCorps Vista .",
        "mention_ids": [
          "M0_3",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "A friend of Robert F. Kennedy , Hackett helped with Kennedy 's presidential campaign in 1968 .",
        "text": "A friend of Robert F. Kennedy , Hackett helped with Kennedy 's presidential campaign in 1968 .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M6_1",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "He served in the United States Army in Europe , during World War II and then went to McGill University .",
        "text": "He served in the United States Army in Europe , during World War II and then went to McGill University .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "05": {
        "orig_text": "Hackett lived in Bethesda , Maryland .",
        "text": "Hackett lived in Bethesda , Maryland .",
        "mention_ids": [
          "M0_1",
          "M15_0",
          "M16_0"
        ]
      },
      "06": {
        "text": "He was the inspiration behind the character of ■■■■■■■ in A Separate Peace by John Knowles .",
        "mention_ids": [
          "M17_0",
          "M18_0",
          "M19_0"
        ]
      },
      "07": {
        "text": "\" It 's clear that ■■■■■■■ ' house was 848 High Street , \" across from the Dedham Common .",
        "mention_ids": [
          "M17_1",
          "M20_0",
          "M21_0"
        ]
      }
