    "sentences": {
      "00": {
        "orig_text": "The Treaty of Edinburgh – Northampton was a peace treaty , signed in 1328 between the Kingdoms of England and Scotland .",
        "text": "The Treaty of Edinburgh – Northampton was a peace treaty , signed in 1328 between the Kingdoms of England and Scotland .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_2",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It brought an end to the First War of Scottish Independence , which had begun with the English invasion of Scotland in 1296 .",
        "text": "It brought an end to the First War of Scottish Independence , which had begun with the English invasion of Scotland in 1296 .",
        "mention_ids": [
          "M2_0",
          "M3_1",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "The treaty was signed in Edinburgh by Robert the Bruce , King of Scotland , on 17 March 1328 , and was ratified by the English Parliament at Northampton on 1 May.",
        "text": "The treaty was signed in Edinburgh by Robert the Bruce , King of Scotland , on 17 March 1328 , and was ratified by the English Parliament at Northampton on 1 May.",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M3_5",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The document was written in French , and is held by the National Archives of Scotland in Edinburgh .",
        "text": "The document was written in French , and is held by the National Archives of Scotland in Edinburgh .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M6_1"
        ]
      },
      "04": {
        "orig_text": "The terms of the treaty stipulated that , in exchange for £ 100,000 sterling , the English Crown would recognise :",
        "text": "The terms of the treaty stipulated that , in exchange for £ 100,000 sterling , the English Crown would recognise :",
        "mention_ids": [
          "M14_0",
          "M15_0"
        ]
      },
      "05": {
        "orig_text": "The Kingdom of Scotland as fully independent ;",
        "text": "The Kingdom of Scotland as fully independent ;",
        "mention_ids": [
          "M3_4"
        ]
      },
      "06": {
        "orig_text": "Robert the Bruce , and his heirs and successors , as the rightful rulers of Scotland .",
        "text": "Robert the Bruce , and his heirs and successors , as the rightful rulers of Scotland .",
        "mention_ids": [
          "M3_2",
          "M7_1"
        ]
      },
      "07": {
        "orig_text": "The border between Scotland and England as that recognised under the reign of Alexander III ( 1249 - 1286 ) .",
        "text": "The border between Scotland and England as that recognised under the reign of Alexander III ( 1249 - 1286 ) .",
        "mention_ids": [
          "M16_0",
          "M17_0",
          "M18_0",
          "M2_1",
          "M3_3"
        ]
      }
    },
