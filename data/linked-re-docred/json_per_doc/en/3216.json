      "00": {
        "orig_text": "' , officially the ' , ( ) , or simply known as City , is a   in the province of , .",
        "text": "Malolos, officially the City of Malolos (Filipino: Lungsod ng Malolos), is a 3rd class component city in the province of Bulacan, Philippines.",
        "mention_ids": []
      },
      "01": {
        "orig_text": "According to the , it has a population of people .",
        "text": "According to the 2020 census, it has a population of 261,189 people.",
        "mention_ids": []
      },
      "02": {
        "orig_text": "It is the capital city of the province of Bulacan as the seat of the provincial government .",
        "text": "It is the capital city of the province of Bulacan as the seat of the provincial government .",
        "mention_ids": [
          "M0_0"
        ]
      },
      "03": {
        "orig_text": "The city is north of Manila , the capital city of the Philippines .",
        "text": "The city is north of Manila , the capital city of the Philippines .",
        "mention_ids": [
          "M1_0",
          "M2_0"
        ]
      },
      "04": {
        "orig_text": "It is one of the major suburbs conurbated to Metro Manila , situated in the southwestern part of Bulacan , in the Central Luzon Region ( Region 3 ) in the island of Luzon and part of the Metro Luzon Urban Beltway Super Region .",
        "text": "It is one of the major suburbs conurbated to Metro Manila , situated in the southwestern part of Bulacan , in the Central Luzon Region ( Region 3 ) in the island of Luzon and part of the Metro Luzon Urban Beltway Super Region .",
        "mention_ids": [
          "M0_1",
          "M3_0",
          "M4_0",
          "M4_1",
          "M5_0",
          "M6_0"
        ]
      },
      "05": {
        "orig_text": "Malolos was the site of the constitutional convention of 1898 , known as the Malolos Convention , that led to the establishment of the First Philippine Republic , at the sanctuary of the Barasoain Church .",
        "text": "Malolos was the site of the constitutional convention of 1898 , known as the Malolos Convention , that led to the establishment of the First Philippine Republic , at the sanctuary of the Barasoain Church .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "The convent of the Malolos Cathedral served as the presidential palace at that time .",
        "text": "The convent of the Malolos Cathedral served as the presidential palace at that time .",
        "mention_ids": [
          "M12_0"
        ]
      },
      "07": {
        "orig_text": "Malolos gave birth to the first constitutional republic in Asia .",
        "text": "Malolos gave birth to the first constitutional republic in Asia .",
