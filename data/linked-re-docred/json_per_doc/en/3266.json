      "title": "Janaka",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Janaka was a king of Videha , approximately in the 8th or 7th century BCE , who later appears as a character in the Ramayana .",
        "text": "Janaka was a king of Videha , approximately in the 8th or 7th century BCE , who later appears as a character in the Ramayana .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "He is revered as being an ideal example of non - attachment to material possessions .",
        "text": "He is revered as being an ideal example of non - attachment to material possessions .",
        "mention_ids": []
      },
      "02": {
        "orig_text": "As a king , he had access to luxuries and pleasures far beyond the ordinary , but his internal state was closer to that of a sadhu .",
        "text": "As a king , he had access to luxuries and pleasures far beyond the ordinary , but his internal state was closer to that of a sadhu .",
        "mention_ids": []
      },
      "03": {
        "orig_text": "He was intensely interested in spiritual discourse and considered himself free from worldly illusions .",
        "text": "He was intensely interested in spiritual discourse and considered himself free from worldly illusions .",
        "mention_ids": []
      },
      "04": {
        "orig_text": "His interactions with sages and seekers such as Ashtavakra and Sulabha are recorded in ancient texts .",
        "text": "His interactions with sages and seekers such as Ashtavakra and Sulabha are recorded in ancient texts .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "05": {
        "orig_text": "His relationship with adopted daughter Sita led her to be called Janaki Mata .",
        "text": "His relationship with adopted daughter Sita led her to be called Janaki Mata .",
        "mention_ids": [
          "M6_0",
          "M6_2"
        ]
      },
      "06": {
        "orig_text": "The Nepalese city of Janakpur is named for him and daughter Sita .",
        "text": "The Nepalese city of Janakpur is named for him and daughter Sita .",
        "mention_ids": [
          "M6_1",
          "M7_0",
          "M8_0"
        ]
      },
      "07": {
        "orig_text": "The Videha ( or Mithila ) kingdom was located between east of Gandaki River , west of Mahananda River , north of Ganga river and south of Himalayas .",
        "text": "The Videha ( or Mithila ) kingdom was located between east of Gandaki River , west of Mahananda River , north of Ganga river and south of Himalayas .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M1_1",
          "M9_0"
        ]
      },
      "08": {
        "orig_text": "The region is now divided between the present day Indian state of Bihar and a small part of Terai Region in Nepal .",
        "text": "The region is now divided between the present day Indian state of Bihar and a small part of Terai Region in Nepal .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      }
    },
