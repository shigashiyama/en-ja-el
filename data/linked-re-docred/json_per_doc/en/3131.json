        ]
      },
      "03": {
        "orig_text": "It stretches above the dam site for to where Carranglan River originates in the Caraballo on the north , and for to where Pantabangan River originates in the Sierra Madre on the east .",
        "text": "It stretches above the dam site for to where Carranglan River originates in the Caraballo on the north , and for to where Pantabangan River originates in the Sierra Madre on the east .",
        "mention_ids": [
