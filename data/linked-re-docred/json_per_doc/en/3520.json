    "sentences": {
      "00": {
        "orig_text": "Dahod is a small city on the banks of the Dudhimati River in Dahod District in the State of Gujarat , India .",
        "text": "Dahod is a small city on the banks of the Dudhimati River in Dahod District in the State of Gujarat , India .",
        "mention_ids": [
          "M0_2",
          "M1_0",
          "M2_1",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "It is said that it have taken its name from Saint Dadhichi , who had an Ashram on the bank of Dadhumati river .",
        "text": "It is said that it have taken its name from Saint Dadhichi , who had an Ashram on the bank of Dadhumati river .",
        "mention_ids": [
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "The city serves as District Headquarters for Dahod District .",
        "text": "The city serves as District Headquarters for Dahod District .",
        "mention_ids": [
          "M2_0",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "It is from Ahmedabad and from Vadodara .",
        "text": "It is from Ahmedabad and from Vadodara .",
        "mention_ids": [
          "M10_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "It is also known as Dohad ( meaning \" two boundaries \" , as the borders of the states of Rajasthan and Madhya Pradesh are nearby ) .",
        "text": "It is also known as Dohad ( meaning \" two boundaries \" , as the borders of the states of Rajasthan and Madhya Pradesh are nearby ) .",
        "mention_ids": [
          "M0_4",
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "05": {
        "orig_text": "Mughal Emperor Aurangzeb was born in the fort of Dahod in 1618 , during the reign of Jahangir .",
        "text": "Mughal Emperor Aurangzeb was born in the fort of Dahod in 1618 , during the reign of Jahangir .",
        "mention_ids": [
          "M0_0",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "06": {
        "orig_text": "Aurangzeb was said to have ordered his ministers to favour this town , as it was his birthplace .",
        "text": "Aurangzeb was said to have ordered his ministers to favour this town , as it was his birthplace .",
        "mention_ids": [
          "M14_1"
        ]
      },
      "07": {
        "orig_text": "Tatya Tope , the freedom fighter , is known to have absconded in Dahod .",
        "text": "Tatya Tope , the freedom fighter , is known to have absconded in Dahod .",
        "mention_ids": [
          "M0_1",
          "M17_0"
        ]
      },
      "08": {
        "orig_text": "He is believed to have lived his last days in this region .",
        "text": "He is believed to have lived his last days in this region .",
        "mention_ids": []
      },
      "09": {
        "orig_text": "It was previously within the boundaries of Panchmahal District .",
        "text": "It was previously within the boundaries of Panchmahal District .",
        "mention_ids": [
          "M18_0"
        ]
      },
      "10": {
        "orig_text": "The area of Godi Road / Godhra Road has been considerably developed , making the overall residence and commercial area very expansive .",
        "text": "The area of Godi Road / Godhra Road has been considerably developed , making the overall residence and commercial area very expansive .",
        "mention_ids": [
          "M19_0",
          "M20_0"
        ]
      },
      "11": {
        "orig_text": "Urban Bank Hospital is situated here .",
        "text": "Urban Bank Hospital is situated here .",
        "mention_ids": [
          "M21_0"
        ]
      },
      "12": {
        "text": "The foundation stone for a dental college was recently laid by the trust of philanthropist ■■■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M22_0"
        ]
      },
      "13": {
        "text": "Dahod has been selected as one of the hundred Indian cities to be developed as a smart city under Prime Minister ■■■■■■■■■■■■■ 's flagship Smart Cities Mission .",
        "mention_ids": [
          "M0_3",
          "M23_0",
          "M24_0",
          "M4_1"
        ]
      }
    },
