      "title": "Zillebeke Churchyard Commonwealth War Graves Commission Cemetery",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Zillebeke Churchyard Commonwealth War Graves Commission Cemetery forms part of the village churchyard located around the Catholic parish church of Zillebeke in Belgium .",
        "text": "Zillebeke Churchyard Commonwealth War Graves Commission Cemetery forms part of the village churchyard located around the Catholic parish church of Zillebeke in Belgium .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "A section of the parish churchyard used by the inhabitants of Zillebeke is maintained as a war cemetery by the Commonwealth War Graves Commission as a burial ground for the dead of the First World War near Ypres ( now Ieper ) on the Western Front .",
        "text": "A section of the parish churchyard used by the inhabitants of Zillebeke is maintained as a war cemetery by the Commonwealth War Graves Commission as a burial ground for the dead of the First World War near Ypres ( now Ieper ) on the Western Front .",
        "mention_ids": [
          "M2_1",
          "M4_0",
          "M5_0",
          "M6_0",
          "M6_1",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "The grounds of the war cemetery were assigned to the United Kingdom in perpetuity by King Albert I of Belgium in recognition of the sacrifices made by the British Empire in the defence and liberation of Belgium during the war .",
        "text": "The grounds of the war cemetery were assigned to the United Kingdom in perpetuity by King Albert I of Belgium in recognition of the sacrifices made by the British Empire in the defence and liberation of Belgium during the war .",
        "mention_ids": [
          "M10_0",
          "M3_1",
          "M3_2",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "Within Zillebeke Churchyard CWGC Cemetery there is a section with war graves of soldiers from aristocratic backgrounds ; this plot is called The Aristocrat 's Cemetery .",
        "text": "Within Zillebeke Churchyard CWGC Cemetery there is a section with war graves of soldiers from aristocratic backgrounds ; this plot is called The Aristocrat 's Cemetery .",
        "mention_ids": [
          "M0_1",
          "M11_0"
        ]
      }
    },
