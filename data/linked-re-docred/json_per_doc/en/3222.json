        ]
      },
      "01": {
        "orig_text": "The Abu Dhabi Islands Archaeological Survey ADIAS carried out an initial archaeological survey of Dalma island in 1992 .",
        "text": "The Abu Dhabi Islands Archaeological Survey ADIAS carried out an initial archaeological survey of Dalma island in 1992 .",
        "mention_ids": [
          "M0_0",
          "M5_0",
          "M5_1",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "A total of more than 20 archaeological sites were identified on the island , ranging in time from the Neolithic ( Late Stone Age ) .",
        "text": "A total of more than 20 archaeological sites were identified on the island , ranging in time from the Neolithic ( Late Stone Age ) .",
        "mention_ids": [
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The population consists of about 10,000 inhabitants , most of whom are Qatari who have been granted United Arab Emirates ( UAE ) nationality .",
        "text": "The population consists of about 10,000 inhabitants , most of whom are Qatari who have been granted United Arab Emirates ( UAE ) nationality .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M12_1"
        ]
      },
      "04": {
        "orig_text": "The island has many private farms for the ruling family of Abu Dhabi .",
        "text": "The island has many private farms for the ruling family of Abu Dhabi .",
        "mention_ids": [
          "M3_0"
        ]
      },
      "05": {
        "text": "The late ruler ■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■■ had a palace here .",
        "mention_ids": [
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "There are many oil field workers working on the island , some of them who fly in from the major cities .",
        "text": "There are many oil field workers working on the island , some of them who fly in from the major cities .",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The island is served by the Dalma Airport and since recent times , by the mainland ferry .",
        "text": "The island is served by the Dalma Airport and since recent times , by the mainland ferry .",
        "mention_ids": [
          "M14_0"
        ]
      }
