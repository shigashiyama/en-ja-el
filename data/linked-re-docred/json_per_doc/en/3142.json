    "sentences": {
      "00": {
        "orig_text": "Suikoden Tactics , originally released in Japan as , is a tactical role - playing video game developed and published by Konami for the PlayStation 2 console as part of their Suikoden series .",
        "text": "Suikoden Tactics , originally released in Japan as , is a tactical role - playing video game developed and published by Konami for the PlayStation 2 console as part of their Suikoden series .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "Initially released in Japan and North America in late 2005 , the game was later made available in Europe and the PAL region in early 2006 .",
        "text": "Initially released in Japan and North America in late 2005 , the game was later made available in Europe and the PAL region in early 2006 .",
        "mention_ids": [
          "M1_1",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "It is the first strategy - based installment of the series , using tactical , grid - based combat instead of the turn - based battles employed by previous games in the series .",
        "text": "It is the first strategy - based installment of the series , using tactical , grid - based combat instead of the turn - based battles employed by previous games in the series .",
        "mention_ids": []
      },
      "03": {
        "text": "The game 's music was composed by series veteran ■■■■■■■■■■■■■■ and features the opening theme \" Another World \" performed by Japanese vocalist ■■■■■■■ .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M1_2"
        ]
      },
      "04": {
        "orig_text": "The game 's fantasy story is told in two parts , taking place both before and after the events of Suikoden IV released one year earlier , and features appearances by characters from the title .",
        "text": "The game 's fantasy story is told in two parts , taking place both before and after the events of Suikoden IV released one year earlier , and features appearances by characters from the title .",
        "mention_ids": [
          "M13_0",
          "M14_0",
          "M15_0"
        ]
      },
      "05": {
        "text": "Players assume the role of ■■■■■ , a young man investigating the power of mysterious weapons called \" Rune Cannons , \" which hold strange powers of transformation .",
        "mention_ids": [
          "M16_0",
          "M17_0"
        ]
      }
