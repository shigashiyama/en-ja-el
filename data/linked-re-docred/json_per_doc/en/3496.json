    "sentences": {
      "00": {
        "orig_text": "Waldshut station ( ) is a railway station in the city of Waldshut - Tiengen in the German state of Baden - Württemberg .",
        "text": "Waldshut station ( ) is a railway station in the city of Waldshut - Tiengen in the German state of Baden - Württemberg .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It was opened on 30 October 1856 .",
        "text": "It was opened on 30 October 1856 .",
        "mention_ids": [
          "M4_0"
        ]
      },
      "02": {
        "orig_text": "The station lies on the Upper Rhine Railway , which connects Basel and Singen along the northern and , mostly , German bank of the Rhine .",
        "text": "The station lies on the Upper Rhine Railway , which connects Basel and Singen along the northern and , mostly , German bank of the Rhine .",
        "mention_ids": [
          "M2_1",
          "M5_1",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "It is the junction point for the Turgi – Koblenz – Waldshut line , which crosses the Rhine from Switzerland on the Waldshut to Koblenz railway bridge just to the south of the station .",
        "text": "It is the junction point for the Turgi – Koblenz – Waldshut line , which crosses the Rhine from Switzerland on the Waldshut to Koblenz railway bridge just to the south of the station .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M8_1",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The Upper Rhine Railway is not electrified and most services are operated by diesel locomotives or railcars .",
        "text": "The Upper Rhine Railway is not electrified and most services are operated by diesel locomotives or railcars .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "05": {
        "orig_text": "The line from Switzerland is electrified using the Swiss standard of 15kV and 16.7Hz from an overhead line .",
        "text": "The line from Switzerland is electrified using the Swiss standard of 15kV and 16.7Hz from an overhead line .",
        "mention_ids": [
          "M10_1",
          "M13_0",
          "M14_0",
          "M15_0"
        ]
      },
      "06": {
        "orig_text": "Only one terminal platform in the station is electrified , and this is used by all trains from Switzerland .",
        "text": "Only one terminal platform in the station is electrified , and this is used by all trains from Switzerland .",
        "mention_ids": [
          "M10_2",
          "M16_0"
        ]
      }
