    "sentences": {
      "00": {
        "orig_text": "Taiwanese expatriates and descendants in Vietnam consist largely of expatriate businessmen and their families .",
        "text": "Taiwanese expatriates and descendants in Vietnam consist largely of expatriate businessmen and their families .",
        "mention_ids": [
