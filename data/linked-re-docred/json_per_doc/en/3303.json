    "sentences": {
      "00": {
        "orig_text": "The Scribe is a 1966 comedy film short starring Buster Keaton in his last film role before his death from lung cancer on February 1 , 1966 .",
        "text": "The Scribe is a 1966 comedy film short starring Buster Keaton in his last film role before his death from lung cancer on February 1 , 1966 .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It was produced by the Construction Safety Association of Ontario , Canada to promote Construction site safety , and was filmed where the Ontario Government Buildings were under construction in Toronto .",
        "text": "It was produced by the Construction Safety Association of Ontario , Canada to promote Construction site safety , and was filmed where the Ontario Government Buildings were under construction in Toronto .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "text": "The director was ■■■■■■■■■■■ .",
        "mention_ids": [
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "Keaton portrays the janitor at a newspaper who accidentally intercepts a call from the editor , ordering him to do a story about safety practices at a massive construction site .",
        "text": "Keaton portrays the janitor at a newspaper who accidentally intercepts a call from the editor , ordering him to do a story about safety practices at a massive construction site .",
        "mention_ids": [
          "M2_2"
        ]
      },
      "04": {
        "orig_text": "He sneaks onto the construction site and finds a list of 16 safety rules posted on a wall .",
        "text": "He sneaks onto the construction site and finds a list of 16 safety rules posted on a wall .",
        "mention_ids": [
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "He takes the list and attempts to confront workers when he sees them acting in an unsafe manner , often causing more accidents than he prevents .",
        "text": "He takes the list and attempts to confront workers when he sees them acting in an unsafe manner , often causing more accidents than he prevents .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "Keaton is silent throughout the film as he recreates several routines from his youth .",
        "text": "Keaton is silent throughout the film as he recreates several routines from his youth .",
        "mention_ids": [
          "M2_1"
        ]
      },
      "07": {
        "orig_text": "Most notable was his recreation of a gag from his 1918 film The Bell Boy in which he mops the floor using only the tip of the mop , little by little while sitting on the floor .",
        "text": "Most notable was his recreation of a gag from his 1918 film The Bell Boy in which he mops the floor using only the tip of the mop , little by little while sitting on the floor .",
        "mention_ids": [
          "M10_0",
          "M11_0"
        ]
      }
