    "sentences": {
      "00": {
        "orig_text": "House of Angels ( ) is a Swedish drama film which was released to cinemas in Sweden on 21 February 1992 , about a little village in Västergötland , Sweden , where an aging recluse lives in a mansion on a large wooded property .",
        "text": "House of Angels ( ) is a Swedish drama film which was released to cinemas in Sweden on 21 February 1992 , about a little village in Västergötland , Sweden , where an aging recluse lives in a mansion on a large wooded property .",
        "mention_ids": [
          "M0_0",
          "M1_1",
          "M1_2",
          "M1_3",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "text": "One day he is accidentally killed and an unknown relative by the name of ■■■■■■■■■■■■ inherits the mansion and land .",
        "mention_ids": [
          "M4_0"
        ]
      },
      "02": {
        "text": "When she and her friend ■■■ arrive , they turn life in the staid village upside down .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "The film was screened out of competition at the 1992 Cannes Film Festival .",
        "text": "The film was screened out of competition at the 1992 Cannes Film Festival .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "04": {
        "orig_text": "At the 28th Guldbagge Awards the film won the awards for Best Film and Best Director .",
        "text": "At the 28th Guldbagge Awards the film won the awards for Best Film and Best Director .",
        "mention_ids": [
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "05": {
        "text": "It was also nominated for Best Actress ( ■■■■■■■■■■■■■■■■ ) , Best Screenplay and Best Cinematography ( ■■■■■■■■■■■■ ) .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "06": {
        "orig_text": "The film was selected as the Swedish entry for the Best Foreign Language Film at the 65th Academy Awards , but was not accepted as a nominee .",
        "text": "The film was selected as the Swedish entry for the Best Foreign Language Film at the 65th Academy Awards , but was not accepted as a nominee .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M1_0"
        ]
      },
      "07": {
        "orig_text": "A sequel , Änglagård – andra sommaren , was produced in 1994 .",
        "text": "A sequel , Änglagård – andra sommaren , was produced in 1994 .",
        "mention_ids": [
          "M17_0",
          "M18_0"
        ]
      },
      "08": {
        "orig_text": "A second sequel , Änglagård – tredje gången gillt , was released on DVD and Blu - ray on 25 May 2011 .",
        "text": "A second sequel , Änglagård – tredje gången gillt , was released on DVD and Blu - ray on 25 May 2011 .",
        "mention_ids": [
          "M19_0",
          "M20_0",
          "M21_0"
        ]
      }
