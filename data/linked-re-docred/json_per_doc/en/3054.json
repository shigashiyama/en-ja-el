    "sentences": {
      "00": {
        "orig_text": "Vladimir Mitrofanovich Orlov ( ) ( July 15 , 1895 - July 28 , 1938 ) was a Russian military leader and Commander - in - Chief of the Soviet Naval Forces from July 1931 to July 1937 .",
        "text": "Vladimir Mitrofanovich Orlov ( ) ( July 15 , 1895 - July 28 , 1938 ) was a Russian military leader and Commander - in - Chief of the Soviet Naval Forces from July 1931 to July 1937 .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "01": {
        "orig_text": "Orlov was born in Kherson and initially studied in the Legal faculty of St Petersburg University ( although he did not complete his studies ) .",
        "text": "Orlov was born in Kherson and initially studied in the Legal faculty of St Petersburg University ( although he did not complete his studies ) .",
        "mention_ids": [
          "M0_2",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "He joined the Baltic Fleet in 1916 and served as a navigating officer on the cruiser Bogatyr .",
        "text": "He joined the Baltic Fleet in 1916 and served as a navigating officer on the cruiser Bogatyr .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0"
        ]
      },
      "03": {
        "orig_text": "In 1919 - 20 he was political officer of the Baltic Fleet and fought against the forces of the white General Nikolai Yudenich in the defence of Petrograd .",
        "text": "In 1919 - 20 he was political officer of the Baltic Fleet and fought against the forces of the white General Nikolai Yudenich in the defence of Petrograd .",
        "mention_ids": [
          "M10_1",
          "M13_0",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "04": {
        "orig_text": "In the 1920s he was commisar for water transport and in 1923 he became political commissar for all naval academies .",
        "text": "In the 1920s he was commisar for water transport and in 1923 he became political commissar for all naval academies .",
        "mention_ids": [
          "M17_0",
          "M18_0"
        ]
      },
      "05": {
        "orig_text": "Between 1926 and 1930 he commanded the Black Sea Fleet .",
        "text": "Between 1926 and 1930 he commanded the Black Sea Fleet .",
        "mention_ids": [
          "M19_0",
          "M20_0",
          "M21_0"
        ]
      },
      "06": {
        "orig_text": "In 1931 he was appointed commander of the Soviet Navy and in 1937 he was appointed deputy minister of defence .",
        "text": "In 1931 he was appointed commander of the Soviet Navy and in 1937 he was appointed deputy minister of defence .",
        "mention_ids": [
          "M22_0",
          "M23_0",
          "M24_0"
        ]
      },
      "07": {
        "orig_text": "Orlov was arrested on 10 July 1937 and was sentenced to death on 28 July 1938 and executed .",
        "text": "Orlov was arrested on 10 July 1937 and was sentenced to death on 28 July 1938 and executed .",
        "mention_ids": [
          "M0_1",
          "M24_1",
          "M25_0"
        ]
      },
      "08": {
        "orig_text": "He was posthumously rehabilitated in 1956 .",
        "text": "He was posthumously rehabilitated in 1956 .",
        "mention_ids": [
          "M26_0"
        ]
      }
    },
