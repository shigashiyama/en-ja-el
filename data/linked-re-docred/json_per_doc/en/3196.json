      "01": {
        "text": "Their last tour with guitarist ■■■■■■■■■■■■■ prior to his departure , and the first with ■■■■■■■■■■■■■■■■ as their touring drummer , the tour was staged in support of their 1976 album Wind & Wuthering and their 1977 extended play Spot the Pigeon , visiting theatres and arenas from January to July 1977 .",
        "mention_ids": [
          "M10_1",
          "M11_0",
          "M12_0",
          "M13_0",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "The band used improved sound and stage lighting systems than before , including a set of Boeing aircraft landing lights .",
        "text": "The band used improved sound and stage lighting systems than before , including a set of Boeing aircraft landing lights .",
        "mention_ids": [
          "M14_0"
        ]
      },
      "03": {
        "orig_text": "The tour featured Genesis ' first South American dates , playing eight shows in three brazilian cities , drawing large crowds and an ethusiastic response from fans and the press .",
        "text": "The tour featured Genesis ' first South American dates , playing eight shows in three brazilian cities , drawing large crowds and an ethusiastic response from fans and the press .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M3_1",
          "M5_0"
        ]
      },
      "04": {
        "orig_text": "Recordings from the tour 's dates in Paris were used for the band 's second live album Seconds Out , released in 1977 .",
        "text": "Recordings from the tour 's dates in Paris were used for the band 's second live album Seconds Out , released in 1977 .",
        "mention_ids": [
          "M10_0",
          "M17_0",
          "M18_0"
        ]
      }
