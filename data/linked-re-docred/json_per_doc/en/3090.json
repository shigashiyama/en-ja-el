    "sentences": {
      "00": {
        "orig_text": "Adolfo Nicolás Pachón ( born 29 April 1936 ) , is a Spanish priest of the Roman Catholic Church .",
        "text": "Adolfo Nicolás Pachón ( born 29 April 1936 ) , is a Spanish priest of the Roman Catholic Church .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "He was the thirtieth Superior General of the Society of Jesus , the largest religious order in the Roman Catholic Church .",
        "text": "He was the thirtieth Superior General of the Society of Jesus , the largest religious order in the Roman Catholic Church .",
        "mention_ids": [
          "M3_1",
          "M4_1"
        ]
      },
      "02": {
        "text": "Nicolás , after consulting with Pope ■■■■■■■ , determined to resign after his 80th birthday , and initiated the process of calling a Jesuit General Congregation to elect his successor .",
        "mention_ids": [
          "M0_1",
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "Until the resignation of his predecessor , Peter Hans Kolvenbach , it was not the norm for a Jesuit Superior General to resign ; they , like the great majority of the Popes up until Benedict XVI , generally served until death .",
        "text": "Until the resignation of his predecessor , Peter Hans Kolvenbach , it was not the norm for a Jesuit Superior General to resign ; they , like the great majority of the Popes up until Benedict XVI , generally served until death .",
        "mention_ids": [
          "M7_0",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "However , the Jesuit constitutions include provision for a resignation .",
        "text": "However , the Jesuit constitutions include provision for a resignation .",
        "mention_ids": [
          "M9_0"
        ]
      },
      "05": {
        "text": "In October 2016 the thirty - sixth General Congregation of the Society of Jesus appointed his successor , ■■■■■■■■■■■ from Venezuela .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M4_0"
        ]
      }
    },
