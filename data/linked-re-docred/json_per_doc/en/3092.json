    "sentences": {
      "00": {
        "orig_text": "Assassin 's Creed Unity is an action - adventure video game developed by Ubisoft Montreal and published by Ubisoft .",
        "text": "Assassin 's Creed Unity is an action - adventure video game developed by Ubisoft Montreal and published by Ubisoft .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It was released in November 2014 for Microsoft Windows , PlayStation 4 and Xbox One .",
        "text": "It was released in November 2014 for Microsoft Windows , PlayStation 4 and Xbox One .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "It is the eighth major installment in the Assassin 's Creed series , and the successor to 2013 's .",
        "text": "It is the eighth major installment in the Assassin 's Creed series , and the successor to 2013 's .",
        "mention_ids": [
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "It also has ties to Assassin 's Creed Rogue which was released for the previous generation consoles , the PlayStation 3 and Xbox 360 on the same date .",
        "text": "It also has ties to Assassin 's Creed Rogue which was released for the previous generation consoles , the PlayStation 3 and Xbox 360 on the same date .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The plot is set in a fictional history of real world events and follows the centuries - old struggle between the Assassins , who fight for peace with free will , and the Templars , who desire peace through control .",
        "text": "The plot is set in a fictional history of real world events and follows the centuries - old struggle between the Assassins , who fight for peace with free will , and the Templars , who desire peace through control .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      },
      "05": {
        "text": "The story is set in Paris during the French Revolution ; the single - player story follows ■■■■■■■■■■■■■■■■■■ in his efforts to expose the true powers behind the Revolution .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      },
      "06": {
        "orig_text": "The game retains the series ' third - person open world exploration as well as introducing a revamped combat , parkour and stealth system .",
        "text": "The game retains the series ' third - person open world exploration as well as introducing a revamped combat , parkour and stealth system .",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The game also introduces cooperative multiplayer to the Assassin 's Creed series , letting up to four players engage in narrative - driven missions and explore the open world map .",
        "text": "The game also introduces cooperative multiplayer to the Assassin 's Creed series , letting up to four players engage in narrative - driven missions and explore the open world map .",
        "mention_ids": [
          "M18_0",
          "M7_1"
        ]
      },
      "08": {
        "orig_text": "Assassin 's Creed Unity received mixed reviews upon its release .",
        "text": "Assassin 's Creed Unity received mixed reviews upon its release .",
        "mention_ids": [
          "M0_1"
        ]
      },
      "09": {
        "orig_text": "Praise was directed towards its visuals , improved gameplay , customization options , multiplayer - oriented format , mission design , setting , characterization , and narrative .",
        "text": "Praise was directed towards its visuals , improved gameplay , customization options , multiplayer - oriented format , mission design , setting , characterization , and narrative .",
        "mention_ids": []
      },
      "10": {
        "orig_text": "However , the game was criticized for a lack of gameplay - innovation , unrefined controls , and numerous graphical issues and bugs upon release .",
        "text": "However , the game was criticized for a lack of gameplay - innovation , unrefined controls , and numerous graphical issues and bugs upon release .",
        "mention_ids": []
      },
      "11": {
        "orig_text": "Because of the game 's poor launch owing to the technical issues , Ubisoft issued an apology , and offered compensation .",
        "text": "Because of the game 's poor launch owing to the technical issues , Ubisoft issued an apology , and offered compensation .",
        "mention_ids": [
          "M2_1"
        ]
      },
      "12": {
        "orig_text": "Assassin 's Creed Unity was followed by Assassin 's Creed Syndicate , which takes place in Victorian England , and was released in October 2015 .",
        "text": "Assassin 's Creed Unity was followed by Assassin 's Creed Syndicate , which takes place in Victorian England , and was released in October 2015 .",
        "mention_ids": [
          "M0_2",
          "M19_0",
          "M20_0",
          "M21_0"
        ]
      }
