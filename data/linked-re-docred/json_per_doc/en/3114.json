    "sentences": {
      "00": {
        "orig_text": "Uhuru Gardens Memorial Park is a commemorative park in Nairobi , Kenya that celebrates independence from the British Empire in 12 December 1963 .",
        "text": "Uhuru Gardens Memorial Park is a commemorative park in Nairobi , Kenya that celebrates independence from the British Empire in 12 December 1963 .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "The word \" uhuru \" is Swahili for \" freedom \" .",
        "text": "The word \" uhuru \" is Swahili for \" freedom \" .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "It contains three points of significance :",
        "text": "It contains three points of significance :",
        "mention_ids": [
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "The Mũgumo tree ( Ficus sycomorus ) , purportedly planted in the exact location the Union Jack , or more likely the former Flag of the Colony of Kenya , was removed and the Flag of Kenya was placed .",
        "text": "The Mũgumo tree ( Ficus sycomorus ) , purportedly planted in the exact location the Union Jack , or more likely the former Flag of the Colony of Kenya , was removed and the Flag of Kenya was placed .",
        "mention_ids": [
          "M10_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The tree is also of significance to the Kikuyu people .",
        "text": "The tree is also of significance to the Kikuyu people .",
        "mention_ids": [
          "M11_0"
        ]
      },
      "05": {
        "orig_text": "The independence commemorative monument , built in 1973 , is a twenty - four meter high column , supporting a pair of clasped hands and a dove of peace .",
        "text": "The independence commemorative monument , built in 1973 , is a twenty - four meter high column , supporting a pair of clasped hands and a dove of peace .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "This point celebrates the declaration of independence at midnight , 12 December 1963 .",
        "text": "This point celebrates the declaration of independence at midnight , 12 December 1963 .",
        "mention_ids": [
          "M14_0",
          "M4_1"
        ]
      },
      "07": {
        "orig_text": "It also celebrates the inauguration of Kenya 's first president , Jomo Kenyatta , in the same night .",
        "text": "It also celebrates the inauguration of Kenya 's first president , Jomo Kenyatta , in the same night .",
        "mention_ids": [
          "M15_0",
          "M2_1"
        ]
      },
      "08": {
        "orig_text": "On one side of this monument is a statue of soldiers raising the Kenyan flag .",
        "text": "On one side of this monument is a statue of soldiers raising the Kenyan flag .",
        "mention_ids": [
          "M2_2"
        ]
      },
      "09": {
        "orig_text": "A fountain celebrating \" Twenty - Five years of Uhuru – peace , love and unity monument \" .",
        "text": "A fountain celebrating \" Twenty - Five years of Uhuru – peace , love and unity monument \" .",
        "mention_ids": [
          "M0_1",
          "M16_0"
        ]
      }
    },
