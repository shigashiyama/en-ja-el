    "sentences": {
      "00": {
        "orig_text": "John Walter Ripley ( June 29 , 1939 – October 28 , 2008 ) was a United States Marine Corps officer who received the Navy Cross for his actions in combat during the Vietnam War .",
        "text": "John Walter Ripley ( June 29 , 1939 – October 28 , 2008 ) was a United States Marine Corps officer who received the Navy Cross for his actions in combat during the Vietnam War .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "On Easter morning 1972 , Captain Ripley repeatedly exposed himself to intense enemy fire over a three - hour period as he prepared to blow up an essential bridge in Dong Ha .",
        "text": "On Easter morning 1972 , Captain Ripley repeatedly exposed himself to intense enemy fire over a three - hour period as he prepared to blow up an essential bridge in Dong Ha .",
        "mention_ids": [
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "His actions significantly hampered the North Vietnamese Army 's advance into South Vietnam .",
        "text": "His actions significantly hampered the North Vietnamese Army 's advance into South Vietnam .",
        "mention_ids": [
          "M10_0",
          "M11_0"
        ]
      },
      "03": {
        "orig_text": "The story of \" Ripley at the Bridge \" is legendary in the Marine Corps and is captured in a gripping diorama at the United States Naval Academy .",
        "text": "The story of \" Ripley at the Bridge \" is legendary in the Marine Corps and is captured in a gripping diorama at the United States Naval Academy .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "04": {
        "orig_text": "On October 28 , 2009 , the first biography about Col . Ripley was published .",
        "text": "On October 28 , 2009 , the first biography about Col . Ripley was published .",
        "mention_ids": [
          "M15_0",
          "M7_1"
        ]
      },
      "05": {
        "text": "It was written by ■■■■■■■■■■■■■■■■ and is titled An American Knight , The Life of Col . John W. Ripley .",
        "mention_ids": [
          "M16_0",
          "M17_0"
        ]
      }
