    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■■■■■ ( born 13 April 1941 , or possibly 1942 or 1943 ) is an Australian radio broadcaster .",
        "mention_ids": [
          "M0_2",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "He is a former coach of the Australia national rugby union team and rugby league coach and administrator .",
        "text": "He is a former coach of the Australia national rugby union team and rugby league coach and administrator .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "He has worked as a school teacher , a speech writer in the office of the Prime Minister Malcolm Fraser , and in musical theatre .",
        "text": "He has worked as a school teacher , a speech writer in the office of the Prime Minister Malcolm Fraser , and in musical theatre .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "He has a Bachelor of Arts from the University of Queensland , and completed a one - year teaching diploma at Worcester College , Oxford .",
        "text": "He has a Bachelor of Arts from the University of Queensland , and completed a one - year teaching diploma at Worcester College , Oxford .",
        "mention_ids": [
          "M10_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "He has received civil and industry awards .",
        "text": "He has received civil and industry awards .",
        "mention_ids": []
      },
      "05": {
        "text": "■■■■■ hosts a popular Sydney breakfast radio program , on radio station 2 GB .",
        "mention_ids": [
          "M0_0",
          "M11_0",
          "M12_0"
        ]
      },
      "06": {
        "text": "■■■■■ advocates mainly conservative views , and the popularity of his radio program has made him a highly paid and influential media personality in Australia .",
        "mention_ids": [
          "M0_1",
          "M5_1"
        ]
      },
      "07": {
        "orig_text": "Despite his success , he remains a controversial figure .",
        "text": "Despite his success , he remains a controversial figure .",
        "mention_ids": []
      },
      "08": {
        "orig_text": "His on - air conduct has received adverse findings from Australia 's media regulators , and he has frequently been sued for defamation .",
        "text": "His on - air conduct has received adverse findings from Australia 's media regulators , and he has frequently been sued for defamation .",
        "mention_ids": [
          "M5_2"
        ]
      }
