    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■ is a Singaporean footballer who plays for Woodlands Wellington FC .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "Initially starting out as a midfielder , he was groomed to realize his potential as a defender when he was playing for Gombak United .",
        "text": "Initially starting out as a midfielder , he was groomed to realize his potential as a defender when he was playing for Gombak United .",
        "mention_ids": [
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "The 2013 season will be his return to professional football in the S.League after a two - year absence due to his national service conscription .",
        "text": "The 2013 season will be his return to professional football in the S.League after a two - year absence due to his national service conscription .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "text": "During that time , ■■■■■■ turned out for Police Sports Association in the National Football League .",
        "mention_ids": [
          "M0_1",
          "M7_0",
          "M8_0"
        ]
      },
      "04": {
        "text": "■■■■■■ is also a former Singapore Under-21 and Singapore Under-23 international , having most recently received his call up to the 2010 Asian Games football team .",
        "mention_ids": [
          "M0_3",
          "M10_0",
          "M11_0",
          "M12_0",
          "M9_0"
        ]
      },
      "05": {
        "text": "■■■■■■ made his debut for Woodlands Wellington on 21 February 2013 in a 2 - 2 draw against Warriors F.C. , coming on as a second - half substitute for ■■■■■■■■■■■■■■■ , who was also making his debut for the Rams .",
        "mention_ids": [
          "M0_2",
          "M13_0",
          "M14_0",
          "M15_0",
          "M16_0",
          "M2_1"
        ]
      }
