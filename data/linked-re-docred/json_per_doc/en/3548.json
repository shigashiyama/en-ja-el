    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■■■■■■■■■■ ( ) ( born 25 January 1980 in Tambov ) is a Russian sprint athlete .",
        "mention_ids": [
          "M0_1",
          "M1_0",
          "M2_0",
          "M3_3"
        ]
      },
      "01": {
        "orig_text": "He won the silver medal in the 400 metres at the 2006 European Athletics Championships in Gothenburg , running a personal best of 45.09 s.",
        "text": "He won the silver medal in the 400 metres at the 2006 European Athletics Championships in Gothenburg , running a personal best of 45.09 s.",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "02": {
        "text": "At the 2007 European Indoor Athletics Championships he won a silver medal in the 4 x 400 metres relay , with teammates ■■■■■■■■■■■■ , ■■■■■■■■■■■■■ and ■■■■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M4_1",
          "M9_0"
        ]
      },
      "03": {
        "text": "The Russian team originally finished third , after Russia 's anchor runner ■■■■■■■■■■■ was pushed by Germany 's ■■■■■■■■■■■■■■■■ when the latter advanced past ■■■■■■■■■■■ on the last lap .",
        "mention_ids": [
          "M14_0",
          "M14_1",
          "M15_1",
          "M16_0",
          "M3_0",
          "M3_2"
        ]
      },
      "04": {
        "orig_text": "However , the incident resulted in the disqualification of the victorious German team and the subsequent promotion of Russia to second place .",
        "text": "However , the incident resulted in the disqualification of the victorious German team and the subsequent promotion of Russia to second place .",
        "mention_ids": [
          "M15_0",
          "M3_1"
        ]
      },
      "05": {
        "text": "■■■■■■ was part of the team that finished third in Men 's 4x400 m relay at the 2008 Summer Olympics , but the team was disqualified after team mate ■■■■■■■■■■■■■■■ tested positive for doping .",
        "mention_ids": [
          "M0_0",
          "M17_0",
          "M18_0",
          "M19_0"
        ]
      }
    },
