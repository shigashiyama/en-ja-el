    "sentences": {
      "00": {
        "orig_text": "Solingen ( ) is a city in North Rhine - Westphalia , Germany .",
        "text": "Solingen ( ) is a city in North Rhine - Westphalia , Germany .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It is located on the northern edge of the region called Bergisches Land , south of the Ruhr area , and , with a 2009 population of 161,366 , is after Wuppertal the second largest city in the Bergisches Land .",
        "text": "It is located on the northern edge of the region called Bergisches Land , south of the Ruhr area , and , with a 2009 population of 161,366 , is after Wuppertal the second largest city in the Bergisches Land .",
        "mention_ids": [
          "M3_0",
          "M3_1",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "It is a member of the regional authority of the Rhineland .",
        "text": "It is a member of the regional authority of the Rhineland .",
        "mention_ids": [
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "Solingen is called the \" City of Blades \" , since it has long been renowned for the manufacturing of fine swords , knives , scissors and razors made by famous firms such as Dreiturm , DOVO , Wüsthof , Zwilling J.",
        "text": "Solingen is called the \" City of Blades \" , since it has long been renowned for the manufacturing of fine swords , knives , scissors and razors made by famous firms such as Dreiturm , DOVO , Wüsthof , Zwilling J.",
        "mention_ids": [
          "M0_1",
          "M0_4",
          "M10_0",
          "M11_0",
          "M12_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "A. Henckels , Böker , Clauberg , Eickhorn , Carl Schmidt Sohn , and numerous other manufacturers .",
        "text": "A. Henckels , Böker , Clauberg , Eickhorn , Carl Schmidt Sohn , and numerous other manufacturers .",
