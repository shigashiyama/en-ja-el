    "sentences": {
      "00": {
        "text": "\" Perdono \" ( also released under the title \" Xdono \" ) is a 2001 song recorded by Italian singer ■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "Released on May 26 , 2001 , it was the singer 's debut single and lead one from his album Rosso Relativo , which was released four months later .",
        "text": "Released on May 26 , 2001 , it was the singer 's debut single and lead one from his album Rosso Relativo , which was released four months later .",
        "mention_ids": [
          "M1_1",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "text": "Based on a sample of ■■■■■■■■ 's song ' Did You Ever Think ' , it achieved huge success in many European countries and even topped the charts in the Netherlands , Spain , Italy , Austria and Belgium ( Wallonia ) .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M2_1",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The song , considered as \" sweet and melodious \" in its Franco - Italian version , was much aired on radio in France .",
        "text": "The song , considered as \" sweet and melodious \" in its Franco - Italian version , was much aired on radio in France .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "04": {
        "text": "The music video was directed by ■■■■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M17_0"
        ]
      }
    },
