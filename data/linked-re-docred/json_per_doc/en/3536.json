    "sentences": {
      "00": {
        "orig_text": "The Exe estuary is an estuary on the south coast of Devon , England .",
        "text": "The Exe estuary is an estuary on the south coast of Devon , England .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "The estuary starts just to the south ( ) of the city of Exeter , and extends south for approximately eight miles to meet the English Channel ( ) .",
        "text": "The estuary starts just to the south ( ) of the city of Exeter , and extends south for approximately eight miles to meet the English Channel ( ) .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "The estuary is a ria and so is larger than would be the case given the size of the River Exe , the main river feeding into the estuary .",
        "text": "The estuary is a ria and so is larger than would be the case given the size of the River Exe , the main river feeding into the estuary .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "On the east shore ( from north to south ) are the town of Topsham , the villages of Exton and Lympstone and at the estuary mouth , the seaside resort of Exmouth .",
        "text": "On the east shore ( from north to south ) are the town of Topsham , the villages of Exton and Lympstone and at the estuary mouth , the seaside resort of Exmouth .",
        "mention_ids": [
          "M10_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Opposite Exmouth on the west shore is the village of Dawlish Warren with its sand spit extending across the mouth of the estuary .",
        "text": "Opposite Exmouth on the west shore is the village of Dawlish Warren with its sand spit extending across the mouth of the estuary .",
        "mention_ids": [
          "M10_1",
          "M11_0"
        ]
      },
      "05": {
        "orig_text": "Above this there are fewer settlements on the west shore , with just the villages of Starcross and Cockwood , both adjoining the lower portion of the estuary .",
        "text": "Above this there are fewer settlements on the west shore , with just the villages of Starcross and Cockwood , both adjoining the lower portion of the estuary .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "The River Clyst also feeds into the estuary , just below Topsham .",
        "text": "The River Clyst also feeds into the estuary , just below Topsham .",
        "mention_ids": [
          "M14_0",
          "M7_1"
        ]
      },
      "07": {
        "orig_text": "The River Kenn feeds into the estuary near Kenton .",
        "text": "The River Kenn feeds into the estuary near Kenton .",
        "mention_ids": [
          "M15_0",
          "M16_0"
        ]
      },
      "08": {
        "orig_text": "The soil is alluvial , derived from Devonian , Carboniferous and Permian rocks .",
        "text": "The soil is alluvial , derived from Devonian , Carboniferous and Permian rocks .",
        "mention_ids": [
          "M17_0",
          "M18_0",
          "M19_0"
        ]
      }
