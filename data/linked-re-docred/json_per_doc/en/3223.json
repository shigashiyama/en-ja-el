    "sentences": {
      "00": {
        "orig_text": "Philipp Brammer ( 28 August 1969 – 28 July 2014 ) was a German actor and voice actor from Munich .",
        "text": "Philipp Brammer ( 28 August 1969 – 28 July 2014 ) was a German actor and voice actor from Munich .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "text": "He began his voice acting career with the German dub of Nils no Fushigi na Tabi , and has since gone on to other things , such as being the dubover artist of ■■■■■■■■■■■■■■■ and ■■■■■■■■■■ .",
        "mention_ids": [
          "M3_1",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "text": "He was best known as the teacher ■■■■■■■■■■ in the ARD series Lindenstraße .",
        "mention_ids": [
          "M10_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "On Monday 28 July 2014 , he did not return from a mountain tour to the Edelweißlahnerkopf .",
        "text": "On Monday 28 July 2014 , he did not return from a mountain tour to the Edelweißlahnerkopf .",
        "mention_ids": [
          "M11_0",
          "M12_0"
        ]
      },
      "04": {
        "orig_text": "After his wife made a missing report to the local police , a search and rescue mission were started by the mountain rescue service and the police .",
        "text": "After his wife made a missing report to the local police , a search and rescue mission were started by the mountain rescue service and the police .",
        "mention_ids": []
      },
      "05": {
        "orig_text": "His body was found on Friday 1 August 2014 .",
        "text": "His body was found on Friday 1 August 2014 .",
        "mention_ids": [
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "He died as a result of a fall by accident .",
        "text": "He died as a result of a fall by accident .",
        "mention_ids": []
      },
      "07": {
        "orig_text": "He left behind his wife and their two daughters .",
        "text": "He left behind his wife and their two daughters .",
        "mention_ids": [
          "M14_0"
        ]
      }
