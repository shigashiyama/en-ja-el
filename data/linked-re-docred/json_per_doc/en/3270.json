      "02": {
        "orig_text": "The business school was initially founded as the corporate university of Orange Group , first telecom operator in France and formerly a public company .",
        "text": "The business school was initially founded as the corporate university of Orange Group , first telecom operator in France and formerly a public company .",
        "mention_ids": [
          "M14_0",
          "M7_1"
        ]
      },
      "03": {
        "orig_text": "It is now fully independent from Orange though many partnerships remain in research , training and other different programmes .",
        "text": "It is now fully independent from Orange though many partnerships remain in research , training and other different programmes .",
        "mention_ids": [
          "M14_1"
        ]
      },
      "04": {
        "orig_text": "The school offers programmes taught in French or English , from bachelor level to PhD.",
        "text": "The school offers programmes taught in French or English , from bachelor level to PhD.",
        "mention_ids": [
          "M15_0",
          "M4_1"
        ]
      },
      "05": {
        "orig_text": "Its main programme is the Masters in Management programme ( aka Programme Grande Ecole ) .",
        "text": "Its main programme is the Masters in Management programme ( aka Programme Grande Ecole ) .",
        "mention_ids": [
          "M13_1",
          "M13_2"
        ]
      },
      "06": {
        "orig_text": "The graduates from this programme are highly reputed among recruiters in France and abroad for their ability to work with engineers .",
        "text": "The graduates from this programme are highly reputed among recruiters in France and abroad for their ability to work with engineers .",
        "mention_ids": [
          "M7_2"
        ]
      },
      "07": {
        "orig_text": "It was ranked 59th ( 2017 ) by the Financial Times .",
        "text": "It was ranked 59th ( 2017 ) by the Financial Times .",
