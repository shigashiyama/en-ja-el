    "sentences": {
      "00": {
        "orig_text": "Isa is a town and Local Government Area in the state of Sokoto in Nigeria .",
        "text": "Isa is a town and Local Government Area in the state of Sokoto in Nigeria .",
        "mention_ids": [
          "M0_1",
          "M1_1",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It shares borders with Shinkafi in Zamfara State , goronyo and Sabon Birni from west and north respectively in Sokoto and the Republic of Niger in the east .",
        "text": "It shares borders with Shinkafi in Zamfara State , goronyo and Sabon Birni from west and north respectively in Sokoto and the Republic of Niger in the east .",
        "mention_ids": [
          "M1_0",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "It has an area of 2,158  km² and a population of 146,103 at the 2006 census .",
        "text": "It has an area of 2,158 km² and a population of 146,103 at the 2006 census .",
        "mention_ids": [
          "M10_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The postal code of the area is 842 .",
        "text": "The postal code of the area is 842 .",
        "mention_ids": [
          "M11_0"
        ]
      },
      "04": {
        "text": "The Isa LGA is made up of the capital town of Isa and other towns and villages such as Turba , Bafarawa ( birthplace of former Sokoto State governor and 2007 presidential candidate ■■■■■■■■■■■■■■■■■ ) .",
        "mention_ids": [
          "M0_0",
