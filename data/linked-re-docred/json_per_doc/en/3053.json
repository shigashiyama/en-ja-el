    "sentences": {
      "00": {
        "text": "The Loud Tour was the fourth overall and third world concert tour by Barbadian recording artist ■■■■■■■ .",
        "mention_ids": [
          "M0_2",
          "M1_0",
          "M2_3"
        ]
      },
      "01": {
        "text": "Performing in over twenty countries in the Americas and Europe , the tour was launched in support of ■■■■■■■ 's fifth studio album Loud ( 2010 ) .",
        "mention_ids": [
          "M0_0",
          "M2_2",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "text": "Critics acclaimed the show for its liveliness and higher caliber of quality when compared to ■■■■■■■ 's previous tours .",
        "mention_ids": [
          "M2_1"
        ]
      },
      "03": {
        "orig_text": "The Loud Tour was a large commercial success , experiencing demand for an extension of shows in the United Kingdom due to popularity .",
        "text": "The Loud Tour was a large commercial success , experiencing demand for an extension of shows in the United Kingdom due to popularity .",
        "mention_ids": [
          "M0_3",
          "M7_0"
        ]
      },
      "04": {
        "text": "In London , ■■■■■■■ played a record breaking 10 dates at The O2 Arena .",
        "mention_ids": [
          "M10_0",
          "M2_0",
          "M8_0",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "The tour ultimately grossed an estimated value of US$ 90 million from 98 reported shows and a total audience of 1,200,800 .",
        "text": "The tour ultimately grossed an estimated value of US$ 90 million from 98 reported shows and a total audience of 1,200,800 .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "The Loud Tour became the seventh - highest grossing tour of 2011 .",
        "text": "The Loud Tour became the seventh - highest grossing tour of 2011 .",
        "mention_ids": [
          "M0_1",
          "M14_0"
        ]
      }
    },
