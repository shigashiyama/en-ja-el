        ]
      },
      "02": {
        "orig_text": "This king is , by far , the most famous in the whole Chero line .",
        "text": "This king is , by far , the most famous in the whole Chero line .",
        "mention_ids": [
          "M3_1"
        ]
      },
      "03": {
        "orig_text": "He extended his sway over South Gaya , Bihar and large parts of Hazaribagh and Surguja , Chhattisgarh .",
        "text": "He extended his sway over South Gaya , Bihar and large parts of Hazaribagh and Surguja , Chhattisgarh .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M9_1"
        ]
      },
      "04": {
        "orig_text": "He invaded and defeated the Maharaja of Chhotanagpur in his capital city of Doisa , now called Navaratragarh , 33 miles from Ranchi .",
        "text": "He invaded and defeated the Maharaja of Chhotanagpur in his capital city of Doisa , now called Navaratragarh , 33 miles from Ranchi .",
        "mention_ids": [
