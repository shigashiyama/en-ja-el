    "sentences": {
      "00": {
        "text": "Howard the Duck is a 1986 American science fiction comedy film directed by ■■■■■■■■■■■■■ and starring ■■■■■■■■■■■■ , ■■■■■■■■■■■■■ , and ■■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "01": {
        "text": "Produced by Gloria Katz and written by ■■■■■ and Katz , with ■■■■■■■■■■■■ as executive producer , the screenplay was originally intended to be an animated film based on the Marvel comic book of the same name , but the film adaptation became live - action because of a contractual obligation .",
        "mention_ids": [
          "M10_0",
          "M11_1",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "Although several TV adaptations of Marvel characters had aired during the preceding 21 years , this was the first theatrically released feature film , coming after the serial Captain America .",
        "text": "Although several TV adaptations of Marvel characters had aired during the preceding 21 years , this was the first theatrically released feature film , coming after the serial Captain America .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "03": {
        "text": "■■■■■ proposed adapting the comic book following the production of American Graffiti ( 1973 ) .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "04": {
        "orig_text": "Following multiple production difficulties and mixed response to test screenings , Howard the Duck was released in theaters on August 1 , 1986 .",
        "text": "Following multiple production difficulties and mixed response to test screenings , Howard the Duck was released in theaters on August 1 , 1986 .",
        "mention_ids": [
          "M0_1",
          "M1_1"
        ]
      },
      "05": {
        "orig_text": "Upon its release , the film was a critical and commercial failure , and in the years since , was considered one of the worst films of all time .",
        "text": "Upon its release , the film was a critical and commercial failure , and in the years since , was considered one of the worst films of all time .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "It was nominated for seven Razzie Awards , and made about $ 15 million domestically compared to its $ 30 million budget .",
        "text": "It was nominated for seven Razzie Awards , and made about $ 15 million domestically compared to its $ 30 million budget .",
        "mention_ids": [
          "M17_0",
          "M18_0",
          "M19_0",
          "M20_0"
        ]
      },
      "07": {
        "orig_text": "Despite the criticism , it has gained a cult following among fans of the comic - book series .",
        "text": "Despite the criticism , it has gained a cult following among fans of the comic - book series .",
        "mention_ids": []
      }
