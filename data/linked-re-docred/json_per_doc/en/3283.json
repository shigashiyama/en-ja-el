      "title": "Trail Smelter dispute",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "The Trail Smelter dispute was a trans - boundary pollution case involving the federal governments of both Canada and the United States , which eventually contributed to establishing the Harm principle in the environmental law of transboundary pollution .",
        "text": "The Trail Smelter dispute was a trans - boundary pollution case involving the federal governments of both Canada and the United States , which eventually contributed to establishing the Harm principle in the environmental law of transboundary pollution .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "The smelter in Trail , British Columbia was historically operated by the Consolidated Mining and Smelting Company ( COMINCO ) until COMINCO merged with Teck in 2001 , and has processed lead and zinc since 1896 .",
        "text": "The smelter in Trail , British Columbia was historically operated by the Consolidated Mining and Smelting Company ( COMINCO ) until COMINCO merged with Teck in 2001 , and has processed lead and zinc since 1896 .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0",
          "M6_2",
          "M6_3",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "Smoke from the smelter caused damage to forests and crops in the surrounding area and also across the Canada – US border in Washington .",
        "text": "Smoke from the smelter caused damage to forests and crops in the surrounding area and also across the Canada – US border in Washington .",
        "mention_ids": [
          "M10_0",
          "M1_1",
          "M2_1"
        ]
      },
      "03": {
        "orig_text": "The smoke from the smelter distressed residents , resulting in complaints to COMINCO and demands for compensation .",
        "text": "The smoke from the smelter distressed residents , resulting in complaints to COMINCO and demands for compensation .",
        "mention_ids": [
          "M6_1"
        ]
      },
      "04": {
        "orig_text": "The dispute between the smelter operators and affected landowners could not be resolved , resulting in the case being sent to an arbitration tribunal .",
        "text": "The dispute between the smelter operators and affected landowners could not be resolved , resulting in the case being sent to an arbitration tribunal .",
        "mention_ids": []
      },
      "05": {
        "orig_text": "Negotiation and resulting litigation and arbitration was settled in 1941 .",
        "text": "Negotiation and resulting litigation and arbitration was settled in 1941 .",
        "mention_ids": [
          "M11_0"
        ]
      }
    },
