    "sentences": {
      "00": {
        "orig_text": "Pedro León Díaz Gallo ( 29 June 1782 – 7 February 1852 ) was an Argentine statesman and priest .",
        "text": "Pedro León Díaz Gallo ( 29 June 1782 – 7 February 1852 ) was an Argentine statesman and priest .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "He was a representative to the Congress of Tucumán which on 9 July 1816 declared the Independence of Argentina .",
        "text": "He was a representative to the Congress of Tucumán which on 9 July 1816 declared the Independence of Argentina .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "Gallo was born in Santiago del Estero and studied at the Monserrat School in Córdoba until he was ordained , graduating as a teacher of art ( or philosophy according to other sources ) at the University of San Carlos .",
        "text": "Gallo was born in Santiago del Estero and studied at the Monserrat School in Córdoba until he was ordained , graduating as a teacher of art ( or philosophy according to other sources ) at the University of San Carlos .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M7_1",
          "M8_1",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "Gallo was elected to represent Santiago del Estero in the Tucumán Congress and served for the declaration in 1816 .",
        "text": "Gallo was elected to represent Santiago del Estero in the Tucumán Congress and served for the declaration in 1816 .",
        "mention_ids": [
          "M4_1",
          "M5_1",
          "M7_0",
          "M8_2"
        ]
      },
      "04": {
        "orig_text": "He was vice - president of the Congress in August 1816 and twice president after it was moved to Buenos Aires .",
        "text": "He was vice - president of the Congress in August 1816 and twice president after it was moved to Buenos Aires .",
        "mention_ids": [
          "M12_0",
          "M4_3",
          "M5_2"
        ]
      },
      "05": {
        "orig_text": "When the Congress was dissolved in 1820 , he and his colleagues were imprisoned as traitors .",
        "text": "When the Congress was dissolved in 1820 , he and his colleagues were imprisoned as traitors .",
        "mention_ids": [
          "M13_0",
          "M4_2"
        ]
      },
      "06": {
        "orig_text": "Gallo returned to Santiago del Estero and was a signatory of the peace treaty of Vinará in 1821 , signing on his province 's behalf with Pedro Miguel Aráoz of Tucumán and José Andrés Pacheco de Melo of Córdoba .",
        "text": "Gallo returned to Santiago del Estero and was a signatory of the peace treaty of Vinará in 1821 , signing on his province 's behalf with Pedro Miguel Aráoz of Tucumán and José Andrés Pacheco de Melo of Córdoba .",
        "mention_ids": [
          "M10_1",
          "M14_0",
          "M15_0",
          "M16_0",
          "M17_0",
          "M18_0",
          "M7_2",
          "M8_0"
        ]
      },
      "07": {
        "orig_text": "He was a minister in the government of Juan Felipe Ibarra , before retiring in Tucumán where he died .",
        "text": "He was a minister in the government of Juan Felipe Ibarra , before retiring in Tucumán where he died .",
        "mention_ids": [
          "M17_1",
          "M19_0"
        ]
      }
    },
