      "title": "Operation Unified Resolve",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Operation Unified Resolve is an air and ground operation to flush out and trap al - Qaeda fighters hiding in the eastern Afghanistan provinces .",
        "text": "Operation Unified Resolve is an air and ground operation to flush out and trap al - Qaeda fighters hiding in the eastern Afghanistan provinces .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "Launched on 23 June 2003 , Operation Unified Resolve is a joint operation between Pakistan , United States , and Afghanistan .",
        "text": "Launched on 23 June 2003 , Operation Unified Resolve is a joint operation between Pakistan , United States , and Afghanistan .",
        "mention_ids": [
          "M0_1",
          "M2_1",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Over 500 troops , mostly from the U.S. 82nd Airborne Division , began hunting the Taliban and al - Qaeda fighters in the provinces of Nangarhar and Kunar on Afghanistan 's eastern border .",
        "text": "Over 500 troops , mostly from the U.S. 82nd Airborne Division , began hunting the Taliban and al - Qaeda fighters in the provinces of Nangarhar and Kunar on Afghanistan 's eastern border .",
        "mention_ids": [
          "M10_0",
          "M1_1",
          "M2_2",
          "M5_1",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The operation is especially focused on the city of Jalalabad , a known al - Qaeda stronghold strategically located on the main route between the Afghan capital Kabul and Pakistan city of Peshawar .",
        "text": "The operation is especially focused on the city of Jalalabad , a known al - Qaeda stronghold strategically located on the main route between the Afghan capital Kabul and Pakistan city of Peshawar .",
        "mention_ids": [
          "M11_0",
          "M12_1",
          "M13_0",
          "M14_0",
          "M1_2",
          "M4_1"
        ]
      },
      "04": {
        "text": "Anti - coalition forces , led by former Afghan prime minister ■■■■■■■■■■■■■■■■■■■ , have attacked coalition forces with their usual retinue of rockets , mines , and bobby traps .",
        "mention_ids": [
          "M12_0",
          "M15_0"
        ]
      },
      "05": {
        "text": "■■■■■■■■■ has been organizing alliances between the remaining anti - coalition forces in the area and increasing their coordination .",
        "mention_ids": [
          "M15_1"
        ]
      },
      "06": {
        "orig_text": "In addition to hunting the Taliban and al - Qaeda fighters , Operation Unified Resolve also distributes humanitarian assistance to the Afghan people in the nearby valleys .",
        "text": "In addition to hunting the Taliban and al - Qaeda fighters , Operation Unified Resolve also distributes humanitarian assistance to the Afghan people in the nearby valleys .",
        "mention_ids": [
          "M0_2",
          "M12_2",
          "M1_3",
          "M8_1"
        ]
      }
    },
