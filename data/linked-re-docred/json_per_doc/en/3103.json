      "title": "Museum of Army Flying",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "The Museum of Army Flying is a British military aviation museum about the history of flying in the British Army .",
        "text": "The Museum of Army Flying is a British military aviation museum about the history of flying in the British Army .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It is located beside the Army Air Corps Centre in Middle Wallop , close to Andover in Hampshire , England .",
        "text": "It is located beside the Army Air Corps Centre in Middle Wallop , close to Andover in Hampshire , England .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "The Museum covers the history of Army aviation from the Balloon sections of the Royal Engineers , through the establishment of the Royal Flying Corps in 1912 and Air Observation Post ( AOP)Squadrons .",
        "text": "The Museum covers the history of Army aviation from the Balloon sections of the Royal Engineers , through the establishment of the Royal Flying Corps in 1912 and Air Observation Post ( AOP)Squadrons .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M11_0",
          "M12_0",
          "M12_1",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "It brings the story up to date with the establishment of the Army Air Corps in 1957 , from the merger of the Glider Pilot Regiment and the AOP Squadrons .",
        "text": "It brings the story up to date with the establishment of the Army Air Corps in 1957 , from the merger of the Glider Pilot Regiment and the AOP Squadrons .",
        "mention_ids": [
          "M12_2",
          "M13_0",
          "M14_0",
          "M15_0"
        ]
      },
      "04": {
        "orig_text": "The Museum also contains multiple flight simulators that anyone can use for a small fee .",
        "text": "The Museum also contains multiple flight simulators that anyone can use for a small fee .",
        "mention_ids": [
          "M0_1"
        ]
      },
      "05": {
        "orig_text": "Outside the museum is a play park featuring aviation themed play pieces that kids can climb on , including a control tower based on the Middle Wallop control tower .",
        "text": "Outside the museum is a play park featuring aviation themed play pieces that kids can climb on , including a control tower based on the Middle Wallop control tower .",
        "mention_ids": [
          "M4_1"
        ]
      }
    },
