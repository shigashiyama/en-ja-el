        ]
      },
      "01": {
        "orig_text": "Located some north - west of Bucharest , the city straddles the Cibin River , a tributary of the river Olt .",
        "text": "Located some north - west of Bucharest , the city straddles the Cibin River , a tributary of the river Olt .",
        "mention_ids": [
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "Now the capital of Sibiu County , between 1692 and 1791 and 1849 – 65 Sibiu was the capital of the Principality of Transylvania .",
        "text": "Now the capital of Sibiu County , between 1692 and 1791 and 1849 – 65 Sibiu was the capital of the Principality of Transylvania .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "Sibiu is one of the most important cultural centres of Romania and was designated the European Capital of Culture for the year 2007 , along with the city of Luxembourg .",
        "text": "Sibiu is one of the most important cultural centres of Romania and was designated the European Capital of Culture for the year 2007 , along with the city of Luxembourg .",
        "mention_ids": [
          "M0_0",
          "M14_0",
          "M15_0",
          "M16_0",
          "M3_1"
        ]
      },
      "04": {
        "orig_text": "Formerly the centre of the Transylvanian Saxons , the old city of Sibiu was ranked as \" Europe 's 8th - most idyllic place to live \" by Forbes in 2008 .",
        "text": "Formerly the centre of the Transylvanian Saxons , the old city of Sibiu was ranked as \" Europe 's 8th - most idyllic place to live \" by Forbes in 2008 .",
        "mention_ids": [
          "M0_2",
          "M17_0",
          "M18_0",
          "M19_0",
          "M20_0"
        ]
      },
      "05": {
        "orig_text": "The city administers the Păltiniș ski resort .",
        "text": "The city administers the Păltiniș ski resort .",
        "mention_ids": [
          "M21_0"
        ]
      }
