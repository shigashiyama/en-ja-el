    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■ (; born 29 June 1947 , née ■■■■■■■■ ) is a human rights and women 's rights activist in Serbia and Montenegro .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "She was the 2006 laureate of Amnesty International 's Ginetta Sagan Fund Award .",
        "text": "She was the 2006 laureate of Amnesty International 's Ginetta Sagan Fund Award .",
        "mention_ids": [
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "text": "■■■■■■■■ created Montenegro 's SOS LINE , the first NGO that in that country which served women by providing resources for women in abusive relationships .",
        "mention_ids": [
          "M0_1",
          "M4_1",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "text": "After noticing that the women calling the SOS LINE needed additional assistance , such a safe place to stay , as well as medical , psychological , and legal assistance , ■■■■■■■■ established the Women 's Safe House , the first shelter in Montenegro for victims of domestic violence .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M4_2",
          "M8_1"
        ]
      },
      "04": {
        "orig_text": "With the Safe House , she successfully lobbied the Parliament of Montenegro for the adoption of a witness protection law .",
        "text": "With the Safe House , she successfully lobbied the Parliament of Montenegro for the adoption of a witness protection law .",
        "mention_ids": [
          "M10_1",
          "M11_0",
          "M4_3"
        ]
      }
    },
