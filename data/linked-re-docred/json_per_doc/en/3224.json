    "sentences": {
      "00": {
        "text": "Sir ■■■■■■■■■■■■■■■■■■■■■ , CBE ( born 2 October 1948 ) is a former England international footballer , manager , pundit and football administrator ; he now works as director of football development in England .",
        "mention_ids": [
          "M0_2",
          "M1_1",
          "M2_0",
          "M3_0",
          "M3_1"
        ]
      },
      "01": {
        "orig_text": "He played most of his career for West Ham United making 647 appearances for the club .",
        "text": "He played most of his career for West Ham United making 647 appearances for the club .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "He won the 1975 FA Cup and the 1980 FA Cup in which he scored the only goal .",
        "text": "He won the 1975 FA Cup and the 1980 FA Cup in which he scored the only goal .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "He was also the club 's player of the season on four occasions and caretaker manager on two occasions in 2003 .",
        "text": "He was also the club 's player of the season on four occasions and caretaker manager on two occasions in 2003 .",
        "mention_ids": [
          "M10_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "text": "■■■■■■■■ played 47 times for England , scoring five times .",
        "mention_ids": [
          "M0_0",
          "M11_0",
          "M12_0",
          "M3_2"
        ]
      },
      "05": {
        "orig_text": "He was appointed a Member of the Order of the British Empire ( MBE ) in 1981 , elevated to Commander of same order ( CBE ) in 1999 , and knighted in 2004 .",
        "text": "He was appointed a Member of the Order of the British Empire ( MBE ) in 1981 , elevated to Commander of same order ( CBE ) in 1999 , and knighted in 2004 .",
        "mention_ids": [
          "M13_0",
          "M13_1",
          "M14_0",
          "M15_0",
          "M16_0",
          "M1_0",
          "M1_2"
        ]
      },
      "06": {
        "text": "In 2009 a stand at Upton Park was named after ■■■■■■■■ .",
        "mention_ids": [
          "M0_1",
          "M17_0",
          "M18_0"
        ]
      },
      "07": {
        "orig_text": "Since retiring from playing , he has taken up a number of positions in broadcasting as an on - air analyst and in football and sports administration .",
        "text": "Since retiring from playing , he has taken up a number of positions in broadcasting as an on - air analyst and in football and sports administration .",
        "mention_ids": []
      }
    },
