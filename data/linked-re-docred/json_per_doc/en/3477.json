      "02": {
        "orig_text": "He served as a Senator for the Cultural and Educational Panel from 1989 to 1992 .",
        "text": "He served as a Senator for the Cultural and Educational Panel from 1989 to 1992 .",
        "mention_ids": [
          "M19_0",
          "M20_0",
          "M6_1"
        ]
      },
      "03": {
        "text": "He unsuccessfully contested the leadership of Fianna Fáil after the resignation of ■■■■■■■■■■■ , but lost to ■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M21_0",
          "M22_0",
          "M3_1"
        ]
      },
      "04": {
        "text": "■■■■■■ appointed ■■■■■■ Deputy Leader of Fianna Fáil after Brian Lenihan Jnr 's death .",
        "mention_ids": [
          "M0_2",
          "M22_1",
          "M23_0",
          "M3_3"
        ]
      },
      "05": {
        "text": "However , ■■■■■■ ceased to be Deputy Leader of Fianna Fáil on 29 February 2012 , because of his opposition to his party 's stance on the European Fiscal Compact .",
        "mention_ids": [
          "M0_1",
          "M24_0",
          "M25_0",
          "M3_4"
        ]
      }
