    "sentences": {
      "00": {
        "orig_text": "Elias Brown ( May 9 , 1793 – July 7 , 1857 ) was a U.S. Representative from Maryland .",
        "text": "Elias Brown ( May 9 , 1793 – July 7 , 1857 ) was a U.S. Representative from Maryland .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "Born near Baltimore , Maryland , Brown attended the common schools .",
        "text": "Born near Baltimore , Maryland , Brown attended the common schools .",
        "mention_ids": [
          "M0_2",
          "M4_1",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "He served as presidential elector on the ticket of James Monroe and Daniel D. Tompkins in 1820 and on the ticket of John Quincy Adams and Richard Rush in 1828 .",
        "text": "He served as presidential elector on the ticket of James Monroe and Daniel D. Tompkins in 1820 and on the ticket of John Quincy Adams and Richard Rush in 1828 .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "Brown was elected as a Jacksonian to the Twenty - first Congress , where he served from March 4 , 1829 to March 3 , 1831 .",
        "text": "Brown was elected as a Jacksonian to the Twenty - first Congress , where he served from March 4 , 1829 to March 3 , 1831 .",
        "mention_ids": [
          "M0_1",
          "M12_0",
          "M13_0",
          "M14_0",
          "M15_0"
        ]
      },
      "04": {
        "orig_text": "He also served as member of the Maryland House of Delegates in 1834 and 1835 , and as member of the Maryland Senate from 1836 to 1838 .",
        "text": "He also served as member of the Maryland House of Delegates in 1834 and 1835 , and as member of the Maryland Senate from 1836 to 1838 .",
        "mention_ids": [
          "M16_0",
          "M17_0",
          "M18_0",
          "M19_0",
          "M20_0",
          "M21_0"
        ]
      },
      "05": {
        "orig_text": "In 1836 , he served as presidential elector on the ticket of William Henry Harrison and John Tyler , and served as delegate to the State constitutional convention the same year .",
        "text": "In 1836 , he served as presidential elector on the ticket of William Henry Harrison and John Tyler , and served as delegate to the State constitutional convention the same year .",
        "mention_ids": [
          "M20_1",
          "M22_0",
          "M23_0",
          "M24_0"
        ]
      },
      "06": {
        "orig_text": "He died near Baltimore , Maryland , and is interred in a private cemetery near Eldersburg , Maryland .",
        "text": "He died near Baltimore , Maryland , and is interred in a private cemetery near Eldersburg , Maryland .",
        "mention_ids": [
          "M25_0",
          "M4_2",
          "M4_3",
          "M5_1"
        ]
      }
