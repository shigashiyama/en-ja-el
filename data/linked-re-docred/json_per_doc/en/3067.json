    "sentences": {
      "00": {
        "orig_text": "Paradise Cracked ( ) is a cyberpunk single - player turn - based strategy video game .",
        "text": "Paradise Cracked ( ) is a cyberpunk single - player turn - based strategy video game .",
        "mention_ids": [
          "M0_0"
        ]
      },
      "01": {
        "orig_text": "It was created by MiST Land South ( renamed as GFI Russia in 2006 ) for Microsoft Windows and released in 2002 .",
        "text": "It was created by MiST Land South ( renamed as GFI Russia in 2006 ) for Microsoft Windows and released in 2002 .",
        "mention_ids": [
          "M1_0",
          "M1_1",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "02": {
        "orig_text": "It has several translation problems that make the game difficult to understand in English .",
        "text": "It has several translation problems that make the game difficult to understand in English .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "The player starts as a character named \" Hacker \" who is just as the name implies .",
        "text": "The player starts as a character named \" Hacker \" who is just as the name implies .",
        "mention_ids": [
          "M6_1"
        ]
      },
      "04": {
        "orig_text": "The player starts with a pistol and the ability to hack certain objects ( ATM 's , Trade Robots , etc . )",
        "text": "The player starts with a pistol and the ability to hack certain objects ( ATM 's , Trade Robots , etc . )",
        "mention_ids": [
          "M7_0",
          "M8_0"
        ]
      },
      "05": {
        "orig_text": "as well as a journal which gives maps of the area and tells them missions both current and completed .",
        "text": "as well as a journal which gives maps of the area and tells them missions both current and completed .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "As certain missions are completed , the character 's level will increase and skills can have points added to them increasing areas like strength , aim , hacking skill , health points and so forth .",
        "text": "As certain missions are completed , the character 's level will increase and skills can have points added to them increasing areas like strength , aim , hacking skill , health points and so forth .",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The game offers the option to play solo or to join with other characters or groups ( such as the mob ) .",
        "text": "The game offers the option to play solo or to join with other characters or groups ( such as the mob ) .",
        "mention_ids": []
      },
      "08": {
        "orig_text": "Hacker is constantly hunted by the law making teaming up with someone in the best interest of the player 's survival .",
        "text": "Hacker is constantly hunted by the law making teaming up with someone in the best interest of the player 's survival .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "09": {
        "orig_text": "Teaming up will also add new missions to Hacker 's journal .",
        "text": "Teaming up will also add new missions to Hacker 's journal .",
        "mention_ids": [
          "M6_2"
        ]
      },
      "10": {
        "orig_text": "Additional weapons , items , and body armor can be acquired by purchase or by killing another player .",
        "text": "Additional weapons , items , and body armor can be acquired by purchase or by killing another player .",
        "mention_ids": []
      },
      "11": {
        "orig_text": "The character 's strength determines what weight of weapons , items etc .",
        "text": "The character 's strength determines what weight of weapons , items etc .",
        "mention_ids": []
      },
      "12": {
        "orig_text": "he can carry while certain clothing will increase or decrease the number of items they can carry .",
        "text": "he can carry while certain clothing will increase or decrease the number of items they can carry .",
        "mention_ids": []
      },
      "13": {
        "orig_text": "The heavier the weight , the less distance the character can move in a given turn .",
        "text": "The heavier the weight , the less distance the character can move in a given turn .",
        "mention_ids": []
      },
      "14": {
        "orig_text": "As strength and level increase , so will a character 's distance he can travel .",
        "text": "As strength and level increase , so will a character 's distance he can travel .",
        "mention_ids": []
      },
      "15": {
        "orig_text": "The game never really gained much ground in terms of commercial success .",
        "text": "The game never really gained much ground in terms of commercial success .",
        "mention_ids": []
      }
