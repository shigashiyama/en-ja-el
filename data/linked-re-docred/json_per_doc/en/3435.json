    "sentences": {
      "00": {
        "orig_text": "The Esprit Orchestra is an orchestra based in Toronto , Ontario , Canada that is dedicated to the performance of new orchestral works .",
        "text": "The Esprit Orchestra is an orchestra based in Toronto , Ontario , Canada that is dedicated to the performance of new orchestral works .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "text": "It was established in 1983 by Music Director and Conductor ■■■■■■■■■ and is Canada 's only full - sized orchestra devoted exclusively to new music .",
        "mention_ids": [
          "M3_1",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Currently , there are 45 full - time members .",
        "text": "Currently , there are 45 full - time members .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "A season typically features five concerts featuring 20th and 21st Century music as well as newly commissioned works .",
        "text": "A season typically features five concerts featuring 20th and 21st Century music as well as newly commissioned works .",
        "mention_ids": [
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "text": "Notable composers who have written for Esprit include John Burke , ■■■■■■■■■■■■■ , ■■■■■■■■ , ■■■■■■■■■■■■■ , Murray Schafer , ■■■■■■■■■■■■■■ , and John Beckwith .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "05": {
        "text": "The orchestra has also participated in film recordings for directors such as ■■■■■■■■■■■■■■■ , ■■■■■■■■■■■■ , ■■■■■■■■■■■■■■ , ■■■■■■■■■■■■■ and ■■■■■■■■■■■ .",
        "mention_ids": [
          "M17_0",
          "M18_0",
          "M19_0",
          "M20_0",
          "M21_0"
        ]
      },
      "06": {
        "orig_text": "In October 2009 , Esprit began to perform in the Koerner Hall and has since called it home .",
        "text": "In October 2009 , Esprit began to perform in the Koerner Hall and has since called it home .",
        "mention_ids": [
          "M0_1",
          "M22_0",
          "M23_0"
        ]
      },
      "07": {
        "orig_text": "Its concerts have also been recorded and broadcast by CBC Radio Two .",
        "text": "Its concerts have also been recorded and broadcast by CBC Radio Two .",
        "mention_ids": [
          "M24_0"
        ]
      },
      "08": {
        "orig_text": "Three of the orchestra 's commercial recordings have been nominated for the Juno Award .",
        "text": "Three of the orchestra 's commercial recordings have been nominated for the Juno Award .",
        "mention_ids": [
          "M25_0",
          "M26_0"
        ]
      }
