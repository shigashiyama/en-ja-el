{
  "3259": {
    "doc_info": {
      "title": "United States Marine Band",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "The United States Marine Band is the premier band of the United States Marine Corps .",
        "text": "The United States Marine Band is the premier band of the United States Marine Corps .",
        "mention_ids": [
          "M0_8",
          "M1_0"
        ]
      },
      "01": {
        "orig_text": "Established by act of Congress on July 11 , 1798 , it is the oldest of the United States military bands and the oldest professional musical organization in the United States .",
        "text": "Established by act of Congress on July 11 , 1798 , it is the oldest of the United States military bands and the oldest professional musical organization in the United States .",
        "mention_ids": [
          "M2_0",
          "M3_0",
          "M4_0",
          "M4_1"
        ]
      },
      "02": {
        "orig_text": "Today , the Marine Band also includes the Marine Chamber Orchestra and Marine Chamber Ensembles .",
        "text": "Today , the Marine Band also includes the Marine Chamber Orchestra and Marine Chamber Ensembles .",
        "mention_ids": [
          "M0_0",
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "The Marine Band is entirely separate from its sister military band , the United States Marine Drum and Bugle Corps ( \" The Commandant 's Own \" ) and the 10 active duty Marine Corps field bands .",
        "text": "The Marine Band is entirely separate from its sister military band , the United States Marine Drum and Bugle Corps ( \" The Commandant 's Own \" ) and the 10 active duty Marine Corps field bands .",
        "mention_ids": [
          "M0_1",
          "M1_1",
          "M7_0",
          "M7_1",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "The Marine Band has been uniquely known as \" The President 's Own \" since 1801 due to the historic connection to the President of the United States .",
        "text": "The Marine Band has been uniquely known as \" The President 's Own \" since 1801 due to the historic connection to the President of the United States .",
        "mention_ids": [
          "M0_2",
          "M0_9",
          "M4_2",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "The relationship between the Marine Band and the White House began on New Year 's Day 1801 , when President John Adams invited the band to perform at the Executive Mansion .",
        "text": "The relationship between the Marine Band and the White House began on New Year 's Day 1801 , when President John Adams invited the band to perform at the Executive Mansion .",
        "mention_ids": [
          "M0_3",
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "Later that year , Thomas Jefferson initiated the tradition of Marine Band performances by requesting that it perform at his inauguration .",
        "text": "Later that year , Thomas Jefferson initiated the tradition of Marine Band performances by requesting that it perform at his inauguration .",
        "mention_ids": [
          "M0_4",
          "M14_0"
        ]
      },
      "07": {
        "orig_text": "The Marine Band has played at every United States presidential inauguration since .",
        "text": "The Marine Band has played at every United States presidential inauguration since .",
        "mention_ids": [
          "M0_5",
          "M15_0"
        ]
      },
      "08": {
        "orig_text": "President Thomas Jefferson gave the title \" The President 's Own \" in 1801 .",
        "text": "President Thomas Jefferson gave the title \" The President 's Own \" in 1801 .",
        "mention_ids": [
          "M0_10",
          "M14_1",
          "M9_1"
        ]
      },
      "09": {
        "orig_text": "Today , the Marine Band performs in approximately 500 events every year including state funerals , state arrival ceremonies , state dinners , parades , concerts , and other social events .",
        "text": "Today , the Marine Band performs in approximately 500 events every year including state funerals , state arrival ceremonies , state dinners , parades , concerts , and other social events .",
        "mention_ids": [
          "M0_6",
          "M16_0"
        ]
      },
      "10": {
        "orig_text": "The Marine Band also travels across the country each October and November during its fall concert tour , a tradition that began in 1891 under its most famous director , composer John Philip Sousa .",
        "text": "The Marine Band also travels across the country each October and November during its fall concert tour , a tradition that began in 1891 under its most famous director , composer John Philip Sousa .",
        "mention_ids": [
          "M0_7",
          "M17_0",
          "M18_0",
          "M19_0",
          "M20_0"
        ]
      }
    },
