    "sentences": {
      "00": {
        "text": "Altsys Corporation was a Texas - based software company founded by ■■■■■■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It was an early Apple Macintosh developer and publisher .",
        "text": "It was an early Apple Macintosh developer and publisher .",
        "mention_ids": [
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "Three major products from Altsys for PC / Macintosh and NeXT were :",
        "text": "Three major products from Altsys for PC / Macintosh and NeXT were :",
        "mention_ids": [
          "M0_1",
          "M4_0",
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "Fontographer for Windows and Mac OS - font editor",
        "text": "Fontographer for Windows and Mac OS - font editor",
        "mention_ids": [
          "M6_0",
          "M7_0",
          "M8_0"
