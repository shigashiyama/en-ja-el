    "sentences": {
      "00": {
        "orig_text": "Skai TV is a Greek free - to - air television network based in Piraeus .",
        "text": "Skai TV is a Greek free - to - air television network based in Piraeus .",
        "mention_ids": [
          "M0_1",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It is part of the Skai Group , one of the largest media groups in the country .",
        "text": "It is part of the Skai Group , one of the largest media groups in the country .",
        "mention_ids": [
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "It was relaunched in its present form on 1st of April 2006 in the Athens metropolitan area , and gradually spread its coverage nationwide .",
        "text": "It was relaunched in its present form on 1st of April 2006 in the Athens metropolitan area , and gradually spread its coverage nationwide .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "Besides digital terrestrial transmission , it is available on the subscription - based encrypted services of Nova and Cosmote TV .",
        "text": "Besides digital terrestrial transmission , it is available on the subscription - based encrypted services of Nova and Cosmote TV .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "Skai TV is also a member of Digea , a consortium of private television networks introducing digital terrestrial transmission in Greece .",
        "text": "Skai TV is also a member of Digea , a consortium of private television networks introducing digital terrestrial transmission in Greece .",
        "mention_ids": [
          "M0_0",
          "M8_0",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "At launch , Skai TV opted for dubbing all foreign language content into Greek , instead of using subtitles .",
        "text": "At launch , Skai TV opted for dubbing all foreign language content into Greek , instead of using subtitles .",
        "mention_ids": [
          "M0_2",
          "M10_0"
        ]
      },
      "06": {
        "orig_text": "This is very uncommon in Greece for anything except documentaries ( using voiceover dubbing ) and children 's programmes ( using lip - synced dubbing ) , so after intense criticism the station switched to using subtitles for almost all foreign shows .",
        "text": "This is very uncommon in Greece for anything except documentaries ( using voiceover dubbing ) and children 's programmes ( using lip - synced dubbing ) , so after intense criticism the station switched to using subtitles for almost all foreign shows .",
        "mention_ids": [
          "M9_1"
        ]
      }
