      "title": "Starin's Glen Island",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Starin 's Glen Island was a summer resort in the community of New Rochelle in Westchester County , New York , developed by shipping magnate and U.S.",
        "text": "Starin 's Glen Island was a summer resort in the community of New Rochelle in Westchester County , New York , developed by shipping magnate and U.S.",
        "mention_ids": [
          "M0_0",
          "M2_2",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "Congressman John H. Starin in the late 1800s .",
        "text": "Congressman John H. Starin in the late 1800s .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "Starin 's resort , referred to as \" America 's pleasure grounds \" and \" Disneyworld on the Sound \" , was the first theme park in the country .",
        "text": "Starin 's resort , referred to as \" America 's pleasure grounds \" and \" Disneyworld on the Sound \" , was the first theme park in the country .",
        "mention_ids": [
          "M0_1",
          "M5_1",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "The park 's original design exhibited the five cultures of the western world on individual islands linked together with piers and causeways .",
        "text": "The park 's original design exhibited the five cultures of the western world on individual islands linked together with piers and causeways .",
        "mention_ids": [
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The extreme popularity of the park resulted in a building boom in New Rochelle in the first decade of the twentieth century .",
        "text": "The extreme popularity of the park resulted in a building boom in New Rochelle in the first decade of the twentieth century .",
        "mention_ids": [
          "M10_0",
          "M2_1"
        ]
      },
      "05": {
        "orig_text": "The resort was located on Glen Island , in New Rochelle 's Lower Harbor , just off of Long Island Sound .",
        "text": "The resort was located on Glen Island , in New Rochelle 's Lower Harbor , just off of Long Island Sound .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M1_0",
          "M2_3"
        ]
      },
      "06": {
        "orig_text": "The island is situated between Davids ' , Neptune , and Travers Islands in New Rochelle , and Hunter Island in nearby Pelham Bay Park , in the Bronx borough of New York City .",
        "text": "The island is situated between Davids ' , Neptune , and Travers Islands in New Rochelle , and Hunter Island in nearby Pelham Bay Park , in the Bronx borough of New York City .",
        "mention_ids": [
          "M13_0",
          "M14_0",
          "M15_0",
          "M16_0",
          "M16_1",
          "M17_0",
          "M2_0",
          "M4_1"
        ]
      },
      "07": {
        "orig_text": "Although now one island , the site originally consisted of one large main island in close proximity to at least four smaller nearby islands , a number of rocky outcroppings , low - lying flats , and salt marshes .",
        "text": "Although now one island , the site originally consisted of one large main island in close proximity to at least four smaller nearby islands , a number of rocky outcroppings , low - lying flats , and salt marshes .",
        "mention_ids": [
          "M18_0",
          "M18_1",
          "M19_0"
        ]
      },
      "08": {
        "orig_text": "The site is currently occupied by the county 's Glen Island Park , which includes remnants of some of the historic structures .",
        "text": "The site is currently occupied by the county 's Glen Island Park , which includes remnants of some of the historic structures .",
        "mention_ids": [
          "M20_0"
        ]
      }
    },
