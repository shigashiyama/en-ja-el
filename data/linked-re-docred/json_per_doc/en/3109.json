      "title": "Operator Toll Dialing",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Operator Toll Dialing was a project and method used by the Bell System in the USA and Canada to automate the switching and billing of long distance calls in the public switched telephone network ( PSTN ) .",
        "text": "Operator Toll Dialing was a project and method used by the Bell System in the USA and Canada to automate the switching and billing of long distance calls in the public switched telephone network ( PSTN ) .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_1",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "The program was initiated by 1945 , at which time ca .",
        "text": "The program was initiated by 1945 , at which time ca .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "5 % of the 2.7 million toll board calls per day were handled by this method .",
        "text": "5 % of the 2.7 million toll board calls per day were handled by this method .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "Operator Toll Dialing eliminated the need for intermediate and inward operators to complete toll calls to distant central offices .",
        "text": "Operator Toll Dialing eliminated the need for intermediate and inward operators to complete toll calls to distant central offices .",
        "mention_ids": [
          "M0_1"
        ]
      },
      "04": {
        "orig_text": "Initially this system involved step - by - step routings to set up each circuit , but later was improved by the development of area codes with machine translation which helped to standardize dialing across the network .",
        "text": "Initially this system involved step - by - step routings to set up each circuit , but later was improved by the development of area codes with machine translation which helped to standardize dialing across the network .",
        "mention_ids": []
      },
      "05": {
        "orig_text": "The system was implemented in the USA beginning in 1946 .",
        "text": "The system was implemented in the USA beginning in 1946 .",
        "mention_ids": [
          "M2_0",
          "M8_0"
        ]
      },
      "06": {
        "orig_text": "In 1947 , the North American Numbering Plan for area codes was set up .",
        "text": "In 1947 , the North American Numbering Plan for area codes was set up .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M9_0"
        ]
      },
      "07": {
        "orig_text": "Operator Toll DIaling was gradually superseded by Direct Distance Dialing ( DDD ) , in which the customer dialed the area code as well as the number .",
        "text": "Operator Toll DIaling was gradually superseded by Direct Distance Dialing ( DDD ) , in which the customer dialed the area code as well as the number .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M13_1"
        ]
      },
      "08": {
        "orig_text": "Beginning in 1951 on a limited basis , it was not fully implemented until the 1970s .",
        "text": "Beginning in 1951 on a limited basis , it was not fully implemented until the 1970s .",
        "mention_ids": [
          "M14_0",
          "M15_0"
        ]
      },
      "09": {
        "orig_text": "Telephone administrations in other countries implemented similar programs , such as in the United Kingdom , which led to Subscriber Trunk Dialling ( STD ) .",
        "text": "Telephone administrations in other countries implemented similar programs , such as in the United Kingdom , which led to Subscriber Trunk Dialling ( STD ) .",
        "mention_ids": [
          "M16_0",
          "M17_0",
          "M17_1"
        ]
      }
    },
