    "sentences": {
      "00": {
        "orig_text": "Bigpoint GmbH is a German video game developer .",
        "text": "Bigpoint GmbH is a German video game developer .",
        "mention_ids": [
          "M0_3",
          "M1_0"
        ]
      },
      "01": {
        "orig_text": "The company develops stand - alone browser - based games as well as social network games .",
        "text": "The company develops stand - alone browser - based games as well as social network games .",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Bigpoint also runs a gaming portal website with a number of browser - based massively multiplayer online role playing games it has produced and has over 200 million registered users ( announced in June 2011 ) .",
        "text": "Bigpoint also runs a gaming portal website with a number of browser - based massively multiplayer online role playing games it has produced and has over 200 million registered users ( announced in June 2011 ) .",
        "mention_ids": [
          "M0_2",
          "M2_0",
          "M3_0"
        ]
      },
      "03": {
        "orig_text": "Bigpoint.com states that 337,104,419 players are registered in the top right corner , as of 10/10/13 .",
        "text": "Bigpoint.com states that 337,104,419 players are registered in the top right corner , as of 10/10/13 .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "04": {
        "orig_text": "NBCU has featured some of Bigpoint 's games on the websites of some of its cable channels .",
        "text": "NBCU has featured some of Bigpoint 's games on the websites of some of its cable channels .",
        "mention_ids": [
          "M0_1",
          "M6_0"
        ]
      },
      "05": {
        "orig_text": "In addition to its headquarters in Hamburg , Germany , Bigpoint maintains offices in Berlin , San Francisco , Malta and São Paulo .",
        "text": "In addition to its headquarters in Hamburg , Germany , Bigpoint maintains offices in Berlin , San Francisco , Malta and São Paulo .",
        "mention_ids": [
          "M0_0",
          "M10_0",
          "M11_0",
          "M12_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "Some of the company 's games include Battlestar Galactica Online , Farmerama , Drakensang Online , DarkOrbit and Seafight .",
        "text": "Some of the company 's games include Battlestar Galactica Online , Farmerama , Drakensang Online , DarkOrbit and Seafight .",
        "mention_ids": [
          "M13_0",
          "M14_0",
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      },
      "07": {
        "orig_text": "The studio was acquired by Youzu Interactive in 2016 and continues to operate as an independent subsidiary .",
        "text": "The studio was acquired by Youzu Interactive in 2016 and continues to operate as an independent subsidiary .",
