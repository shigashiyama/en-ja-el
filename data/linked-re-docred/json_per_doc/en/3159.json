    "sentences": {
      "00": {
        "orig_text": "Lemany ( ) is a village in the administrative district of Gmina Szczytno , within Szczytno County , Warmian - Masurian Voivodeship , in northern Poland .",
        "text": "Lemany ( ) is a village in the administrative district of Gmina Szczytno , within Szczytno County , Warmian - Masurian Voivodeship , in northern Poland .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "It lies approximately north - east of Szczytno and south - east of the regional capital Olsztyn .",
        "text": "It lies approximately north - east of Szczytno and south - east of the regional capital Olsztyn .",
        "mention_ids": [
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "Before 1945 the area was part of Germany ( East Prussia ) .",
        "text": "Before 1945 the area was part of Germany ( East Prussia ) .",
        "mention_ids": [
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "In the East Prussian plebiscite of 1920 , which was largely boycotted by ethnic Poles , the inhabitants voted to remain in Germany by a count of 198 - 0 .",
        "text": "In the East Prussian plebiscite of 1920 , which was largely boycotted by ethnic Poles , the inhabitants voted to remain in Germany by a count of 198 - 0 .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0",
          "M8_1"
        ]
      },
      "04": {
        "orig_text": "After World War II the region was placed under Polish administration by the Potsdam Agreement under territorial changes demanded by the Soviet Union .",
        "text": "After World War II the region was placed under Polish administration by the Potsdam Agreement under territorial changes demanded by the Soviet Union .",
        "mention_ids": [
          "M15_0",
          "M16_1",
          "M17_0",
          "M18_0"
        ]
      },
      "05": {
        "orig_text": "Most Germans fled or were expelled and replaced with Poles expelled from the Polish areas annexed by the Soviet Union or Ukrainians forced to settle in the area through Operation Vistula in 1947 .",
        "text": "Most Germans fled or were expelled and replaced with Poles expelled from the Polish areas annexed by the Soviet Union or Ukrainians forced to settle in the area through Operation Vistula in 1947 .",
