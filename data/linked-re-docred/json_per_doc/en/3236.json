    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■■ ( born 1951 ) is a Ugandan composer of contemporary classical music .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_1"
        ]
      },
      "01": {
        "orig_text": "His music combines elements of traditional Ugandan music and Western music .",
        "text": "His music combines elements of traditional Ugandan music and Western music .",
        "mention_ids": [
          "M2_0",
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "He is best known for his first string quartet , which was included by the Kronos Quartet on their 1992 CD Pieces of Africa , which contains music by seven African composers .",
        "text": "He is best known for his first string quartet , which was included by the Kronos Quartet on their 1992 CD Pieces of Africa , which contains music by seven African composers .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "His music has also been performed by the Imani Winds .",
        "text": "His music has also been performed by the Imani Winds .",
        "mention_ids": [
          "M9_0"
        ]
      },
      "04": {
        "text": "■■■■■■■■ was born in Kibisi .",
        "mention_ids": [
          "M0_1",
          "M10_0"
        ]
      },
      "05": {
        "orig_text": "His early training was in Baganda traditional music .",
        "text": "His early training was in Baganda traditional music .",
        "mention_ids": [
          "M11_0"
        ]
      },
      "06": {
        "text": "His early instructors included the Reverend ■■■■■■■■■■■■■ and ■■■■■■■■■■■■ at Queen 's University Belfast in Belfast , Northern Ireland .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "07": {
        "orig_text": "He received his doctorate in composition at Northwestern University , where he studied with Alan Stout .",
        "text": "He received his doctorate in composition at Northwestern University , where he studied with Alan Stout .",
        "mention_ids": [
          "M17_0",
          "M18_0"
        ]
      },
      "08": {
        "orig_text": "He has taught at Makerere University in Kampala , Uganda , as well as at Northwestern University .",
        "text": "He has taught at Makerere University in Kampala , Uganda , as well as at Northwestern University .",
        "mention_ids": [
          "M17_1",
          "M19_0",
          "M20_0",
          "M21_0"
        ]
      },
      "09": {
        "orig_text": "His music is published by International Opus .",
        "text": "His music is published by International Opus .",
        "mention_ids": [
