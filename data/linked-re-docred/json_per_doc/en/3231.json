    "sentences": {
      "00": {
        "orig_text": "Marselisborg Gymnasium is a school of secondary education in Aarhus , Denmark .",
        "text": "Marselisborg Gymnasium is a school of secondary education in Aarhus , Denmark .",
        "mention_ids": [
          "M0_1",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "The school is a financially independent self - owning educational institution under the Danish state .",
        "text": "The school is a financially independent self - owning educational institution under the Danish state .",
        "mention_ids": [
          "M3_1"
        ]
      },
      "02": {
        "orig_text": "The school offers the 3-year Matriculation examination ( STX ) programme within five main branches ; natural sciences , social sciences , language , music and , since 2006 , sports through a partnership with Team Danmark .",
        "text": "The school offers the 3-year Matriculation examination ( STX ) programme within five main branches ; natural sciences , social sciences , language , music and , since 2006 , sports through a partnership with Team Danmark .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "text": "Marselisborg Gymnasium was founded in 1898 by ■■■■■■■■■■ under the name Marselisborg Boarding and Learned School .",
        "mention_ids": [
          "M0_0",
          "M10_0",
          "M11_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The school became a popular alternative to Aarhus Katedralskole and was first expanded in 1904 .",
        "text": "The school became a popular alternative to Aarhus Katedralskole and was first expanded in 1904 .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      },
      "05": {
        "orig_text": "From 1916 the school became owned by Aarhus Municipality and in 1973 by Aarhus County .",
        "text": "From 1916 the school became owned by Aarhus Municipality and in 1973 by Aarhus County .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      },
      "06": {
        "orig_text": "In the Danish Municipal Reform of 2007 the Danish counties were abolished and Marselisborg Gymnasium became independent and self - owning like most other Danish educational institutions .",
        "text": "In the Danish Municipal Reform of 2007 the Danish counties were abolished and Marselisborg Gymnasium became independent and self - owning like most other Danish educational institutions .",
        "mention_ids": [
          "M0_2",
          "M18_0",
          "M3_0",
          "M3_2"
        ]
      }
