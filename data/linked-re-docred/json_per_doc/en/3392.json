      "title": "Tourism in Montreal",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Tourism is an important industry in Montreal , Quebec , Canada .",
        "text": "Tourism is an important industry in Montreal , Quebec , Canada .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "The city welcomed 10.2 million overnight visitors in 2016 and 11,792,970 day trip visitors in 2010 .",
        "text": "The city welcomed 10.2 million overnight visitors in 2016 and 11,792,970 day trip visitors in 2010 .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "Montreal attracted 1,770,939 international overnight visitors in 2010 , most of them from the United States , France , the United Kingdom , Germany , Mexico and Japan .",
        "text": "Montreal attracted 1,770,939 international overnight visitors in 2010 , most of them from the United States , France , the United Kingdom , Germany , Mexico and Japan .",
        "mention_ids": [
          "M0_3",
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M6_1",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "82 740 direct jobs and 48 199 indirect jobs , in Montreal were generated by the tourism industry in 2014 .",
        "text": "82 740 direct jobs and 48 199 indirect jobs , in Montreal were generated by the tourism industry in 2014 .",
        "mention_ids": [
          "M0_1",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "04": {
        "orig_text": "Crescent Street in Downtown Montreal is popular among tourists .",
        "text": "Crescent Street in Downtown Montreal is popular among tourists .",
        "mention_ids": [
          "M17_0",
          "M18_0"
        ]
      },
      "05": {
        "orig_text": "Throughout the summer , it features various street fairs and festivals .",
        "text": "Throughout the summer , it features various street fairs and festivals .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "Among locals , Crescent Street is known better for its many clubs and bars .",
        "text": "Among locals , Crescent Street is known better for its many clubs and bars .",
        "mention_ids": [
          "M17_1"
        ]
      },
      "07": {
        "orig_text": "Saint - Laurent Boulevard and the surrounding Plateau Mont - Royal neighbourhood , are also well known for their nightlife , with many bars , nightclubs and restaurants .",
        "text": "Saint - Laurent Boulevard and the surrounding Plateau Mont - Royal neighbourhood , are also well known for their nightlife , with many bars , nightclubs and restaurants .",
        "mention_ids": [
          "M19_0",
          "M20_0"
        ]
      },
      "08": {
        "orig_text": "The main sectors that attract tourism in Montreal are Festivals drawing in 7.5 million tourists , museums accounting for 7 million visits and Old Montreal bringing in 2.5 million tourists in the year 2013 .",
        "text": "The main sectors that attract tourism in Montreal are Festivals drawing in 7.5 million tourists , museums accounting for 7 million visits and Old Montreal bringing in 2.5 million tourists in the year 2013 .",
