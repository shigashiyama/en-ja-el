    "sentences": {
      "00": {
        "orig_text": "London Calling is the third studio album by English punk rock band The Clash .",
        "text": "London Calling is the third studio album by English punk rock band The Clash .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It was released as a double album in the United Kingdom on 14 December 1979 by CBS Records , and in the United States in January 1980 by Epic Records .",
        "text": "It was released as a double album in the United Kingdom on 14 December 1979 by CBS Records , and in the United States in January 1980 by Epic Records .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "02": {
        "orig_text": "London Calling is an album that incorporates a range of styles , including punk , reggae , rockabilly , ska , New Orleans R&B ; , pop , lounge jazz , and hard rock .",
        "text": "London Calling is an album that incorporates a range of styles , including punk , reggae , rockabilly , ska , New Orleans R&B ; , pop , lounge jazz , and hard rock .",
        "mention_ids": [
          "M0_1",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The album 's subject matter included social displacement , unemployment , racial conflict , drug use , and the responsibilities of adulthood .",
        "text": "The album 's subject matter included social displacement , unemployment , racial conflict , drug use , and the responsibilities of adulthood .",
        "mention_ids": []
      },
      "04": {
        "orig_text": "While working on \" The Card Cheat \" , the band recorded each part twice to create a \" sound as big as possible \" .",
        "text": "While working on \" The Card Cheat \" , the band recorded each part twice to create a \" sound as big as possible \" .",
        "mention_ids": [
          "M10_0"
        ]
      },
      "05": {
        "orig_text": "The final track , \" Train in Vain \" , was originally excluded from the back cover 's track listing .",
        "text": "The final track , \" Train in Vain \" , was originally excluded from the back cover 's track listing .",
        "mention_ids": [
          "M11_0"
        ]
      },
      "06": {
        "orig_text": "It was intended to be given away through a promotion with NME , but was added to the album at the last minute after the deal fell through .",
        "text": "It was intended to be given away through a promotion with NME , but was added to the album at the last minute after the deal fell through .",
        "mention_ids": [
          "M12_0"
        ]
      },
      "07": {
        "orig_text": "The album received widespread acclaim and was ranked at number eight on Rolling Stones list of The 500 Greatest Albums of All Time in 2003 .",
        "text": "The album received widespread acclaim and was ranked at number eight on Rolling Stones list of The 500 Greatest Albums of All Time in 2003 .",
        "mention_ids": [
          "M13_0",
          "M14_0"
        ]
      },
      "08": {
        "orig_text": "London Calling was a top ten album in the UK , and its lead single \" London Calling \" was a top 20 single .",
        "text": "London Calling was a top ten album in the UK , and its lead single \" London Calling \" was a top 20 single .",
        "mention_ids": [
          "M0_2",
          "M0_3",
          "M15_0",
          "M3_1"
        ]
      },
      "09": {
        "orig_text": "It has sold over five million copies worldwide , and was certified platinum in the United States .",
        "text": "It has sold over five million copies worldwide , and was certified platinum in the United States .",
        "mention_ids": [
          "M16_0",
          "M6_1"
        ]
      }
