    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■■■■■■■■■ ( born 17 October 1968 ) is a former Brazilian international footballer .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "He has played for several domestic club teams in Brazil such as Vasco Esporte Clube , Flamengo , Fluminense FC , Clube Atlético Mineiro etc .",
        "text": "He has played for several domestic club teams in Brazil such as Vasco Esporte Clube , Flamengo , Fluminense FC , Clube Atlético Mineiro etc .",
        "mention_ids": [
