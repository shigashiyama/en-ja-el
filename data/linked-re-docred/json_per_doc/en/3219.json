        ]
      },
      "02": {
        "text": "■■■■■■■■■■■■■■■■■■■■■■■■■ was part of the Brazilian U17 football team at the inaugural edition of the FIFA U-16 World Championship ( now known as FIFA U17 World Cup ) in 1985 .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M11_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "He played a vital role in Brazil 's third place finish at the first edition of the Youth FIFA World Cup with scoring 5 goals in the entire tournament .",
        "text": "He played a vital role in Brazil 's third place finish at the first edition of the Youth FIFA World Cup with scoring 5 goals in the entire tournament .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M3_1"
        ]
      },
      "04": {
        "text": "He was awarded the Golden Ball at the 1985 World Cup as he was the second leading goal scorer in the tournament just behind ■■■■■■■■■■■■■■■ of Germany .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      }
