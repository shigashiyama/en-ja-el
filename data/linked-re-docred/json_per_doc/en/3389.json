    "sentences": {
      "00": {
        "text": "Edward Ngoyai Lowassa ( born August 26 , 1953 ) is a Tanzanian politician who was Prime Minister of Tanzania from 2005 to 2008 , serving under President ■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M2_2",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "Lowassa has gone into record as the first Prime Minister to have been forced to resign by a fraud scandal in the history of Tanzania .",
        "text": "Lowassa has gone into record as the first Prime Minister to have been forced to resign by a fraud scandal in the history of Tanzania .",
        "mention_ids": [
          "M0_1",
          "M2_1"
        ]
      },
      "02": {
        "text": "Following his resignation President ■■■■■■■ was obliged to dissolve his cabinet as required by the Constitution and with minimum delay , constituted a new one under a new Prime Minister , ■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M5_1",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "After the ruling CCM failed to select him as its candidate for the October 2015 presidential election , he left the party and stood instead as an opposition candidate .",
        "text": "After the ruling CCM failed to select him as its candidate for the October 2015 presidential election , he left the party and stood instead as an opposition candidate .",
        "mention_ids": [
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "He was defeated in the election by CCM candidate John Magufuli .",
        "text": "He was defeated in the election by CCM candidate John Magufuli .",
        "mention_ids": [
          "M10_0",
          "M8_1"
        ]
      }
    },
