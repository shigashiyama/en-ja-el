    "sentences": {
      "00": {
        "orig_text": "Appenzell is a village , a statistic town , but not a municipality , and the capital of the canton of Appenzell Innerrhoden in Switzerland .",
        "text": "Appenzell is a village , a statistic town , but not a municipality , and the capital of the canton of Appenzell Innerrhoden in Switzerland .",
        "mention_ids": [
          "M0_2",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "Appenzell has no municipal government of its own ; rather , the different parts of Appenzell belong to and are governed by the districts ( and municipalities ) Appenzell , Schwende and R端te .",
        "text": "Appenzell has no municipal government of its own ; rather , the different parts of Appenzell belong to and are governed by the districts ( and municipalities ) Appenzell , Schwende and R端te .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M0_3",
          "M3_0",
          "M4_0"
        ]
      },
      "02": {
        "orig_text": "Because of that , for firefighting , energy and water , the village Appenzell has a special - purpose municipality , the Feuerschaugemeinde .",
        "text": "Because of that , for firefighting , energy and water , the village Appenzell has a special - purpose municipality , the Feuerschaugemeinde .",
        "mention_ids": [
          "M0_4",
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "In 1071 the village was referred to as Abbacella .",
        "text": "In 1071 the village was referred to as Abbacella .",
        "mention_ids": [
          "M0_5",
          "M6_0"
        ]
      },
      "04": {
        "orig_text": "By 1223 this changed to Abbatiscella , meaning the Abbot 's cell .",
        "text": "By 1223 this changed to Abbatiscella , meaning the Abbot 's cell .",
        "mention_ids": [
          "M0_6",
          "M7_0"
        ]
      },
      "05": {
        "orig_text": "This refers to the abbot of the Abbey of Saint Gall .",
        "text": "This refers to the abbot of the Abbey of Saint Gall .",
        "mention_ids": [
          "M8_0"
        ]
      },
      "06": {
        "orig_text": "The official language of Appenzell is ( the Swiss variety of Standard ) German , but the main spoken language is the local variant of the Alemannic Swiss German dialect .",
        "text": "The official language of Appenzell is ( the Swiss variety of Standard ) German , but the main spoken language is the local variant of the Alemannic Swiss German dialect .",
        "mention_ids": [
          "M0_7",
          "M10_0",
          "M11_0",
          "M12_0",
          "M9_0"
        ]
      },
      "07": {
        "orig_text": "The buildings in the village core , the parish church , the 1563 town hall , the Salesis house , the ruins of Castle Clanx and the state archives with the administration building are listed as heritage sites of national significance .",
        "text": "The buildings in the village core , the parish church , the 1563 town hall , the Salesis house , the ruins of Castle Clanx and the state archives with the administration building are listed as heritage sites of national significance .",
        "mention_ids": [
          "M13_0",
          "M14_0",
          "M15_0"
        ]
      }
