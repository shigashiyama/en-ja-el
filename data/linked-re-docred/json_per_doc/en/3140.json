    "sentences": {
      "00": {
        "orig_text": "Cine - Allianz Tonfilm was a German film production company established in 1932 by Arnold Pressburger and Gregor Rabinovitch .",
        "text": "Cine - Allianz Tonfilm was a German film production company established in 1932 by Arnold Pressburger and Gregor Rabinovitch .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "The company specalised in co - productions targeted at international markets , and enjoyed immediate success during the final year of the Weimar Republic .",
        "text": "The company specalised in co - productions targeted at international markets , and enjoyed immediate success during the final year of the Weimar Republic .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "During the Nazi era the company 's Jewish owners came under increasing pressure from the government and their property was expropriated .",
        "text": "During the Nazi era the company 's Jewish owners came under increasing pressure from the government and their property was expropriated .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "They were forced into exile , while Cine - Allianz continued to produce films under the Nazi regime until its merger with UFA in 1942 .",
        "text": "They were forced into exile , while Cine - Allianz continued to produce films under the Nazi regime until its merger with UFA in 1942 .",
        "mention_ids": [
          "M0_2",
          "M6_1",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Rabinovitch went into exile in France where he set up a fresh production company also named Cine - Allianz which produced films such as I Was an Adventuress ( 1938 ) .",
        "text": "Rabinovitch went into exile in France where he set up a fresh production company also named Cine - Allianz which produced films such as I Was an Adventuress ( 1938 ) .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M11_0",
          "M12_0",
          "M4_1"
        ]
      },
      "05": {
        "orig_text": "The 1951 film The Lost One was partly financed by money received as post - war compensation for the loss of Cine - Allianz .",
        "text": "The 1951 film The Lost One was partly financed by money received as post - war compensation for the loss of Cine - Allianz .",
        "mention_ids": [
          "M0_3",
          "M13_0",
          "M14_0"
        ]
      }
