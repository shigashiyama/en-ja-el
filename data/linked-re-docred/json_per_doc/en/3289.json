    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■ ( born October 13 , 1970 ) is a South Korean actor .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "text": "■■ began his career on stage , performing in plays and musicals for many years before moving on to bit parts in films .",
        "mention_ids": [
          "M0_2"
        ]
      },
      "02": {
        "text": "He once earned his living by working on farms and in iron foundries , then acquired fame through his scene - stealing roles in ■■■■■■■■■ 's films , notably as the movie director in Rough Cut ( 2008 ) , and the Vietnamese gang boss in Secret Reunion ( 2010 ) .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "He has since become one of the busiest supporting actors in Korean cinema .",
        "text": "He has since become one of the busiest supporting actors in Korean cinema .",
        "mention_ids": [
          "M9_0"
        ]
      },
      "04": {
        "text": "■■ 's real - life wife and daughter played his character 's family in the period film The Showdown ( 2011 ) .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M11_0"
        ]
      }
    },
