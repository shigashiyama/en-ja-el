      "title": "Blue River (Colorado)",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "The Blue River is a tributary of the Colorado River , approximately long , in the U.S. state of Colorado .",
        "text": "The Blue River is a tributary of the Colorado River , approximately long , in the U.S. state of Colorado .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It rises in southern Summit County , on the western side of the continental divide in the Ten Mile Range , near Quandary Peak .",
        "text": "It rises in southern Summit County , on the western side of the continental divide in the Ten Mile Range , near Quandary Peak .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "It flows north past Blue River and Breckenridge , then through the Dillon Reservoir near Dillon .",
        "text": "It flows north past Blue River and Breckenridge , then through the Dillon Reservoir near Dillon .",
        "mention_ids": [
          "M0_1",
          "M7_0",
          "M8_1",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The West portal for the \" Roberts Tunnel \" is at the base of Dillon Reservoir .",
        "text": "The West portal for the \" Roberts Tunnel \" is at the base of Dillon Reservoir .",
        "mention_ids": [
          "M10_0",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "The Roberts Tunnel is a trans - basin diversion , built by Denver Water in 1962 , that diverts water under the Continental Divide from the Colorado River basin into the South Plate River Basin .",
        "text": "The Roberts Tunnel is a trans - basin diversion , built by Denver Water in 1962 , that diverts water under the Continental Divide from the Colorado River basin into the South Plate River Basin .",
        "mention_ids": [
          "M10_1",
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0",
          "M1_1"
        ]
      },
      "05": {
        "orig_text": "The East portal is approximately one mile upstream of Grants , Colorado .",
        "text": "The East portal is approximately one mile upstream of Grants , Colorado .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M3_1"
        ]
      },
      "06": {
        "orig_text": "North of Dillon the river flows NNW along the eastern slope of the Gore Range and joins the Colorado River at Kremmling .",
        "text": "North of Dillon the river flows NNW along the eastern slope of the Gore Range and joins the Colorado River at Kremmling .",
        "mention_ids": [
          "M17_0",
          "M18_0",
          "M19_0",
          "M1_2",
          "M9_1"
        ]
      },
      "07": {
        "orig_text": "The Green Mountain Dam , upstream from Kremmling , forms the Green Mountain Reservoir , providing hydroelectric power and diversionary water for irrigation , as part of the Colorado - Big Thompson Project .",
        "text": "The Green Mountain Dam , upstream from Kremmling , forms the Green Mountain Reservoir , providing hydroelectric power and diversionary water for irrigation , as part of the Colorado - Big Thompson Project .",
        "mention_ids": [
          "M19_1",
          "M20_0",
          "M21_0",
          "M22_0"
        ]
      },
      "08": {
        "orig_text": "The dam is a project of the United States Bureau of Reclamation .",
        "text": "The dam is a project of the United States Bureau of Reclamation .",
        "mention_ids": [
          "M23_0"
        ]
      }
    },
