    "sentences": {
      "00": {
        "orig_text": "The Denali National Park Improvement Act ( ) is a bill that was introduced into the United States Senate during the 113th United States Congress .",
        "text": "The Denali National Park Improvement Act ( ) is a bill that was introduced into the United States Senate during the 113th United States Congress .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "If enacted , the bill would do four main things .",
        "text": "If enacted , the bill would do four main things .",
        "mention_ids": [
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "First , it would allow the United States Department of the Interior to \" issue permits for microhydroelectric projects in the Kantishna Hills area of the Denali National Park and Preserve in Alaska . \"",
        "text": "First , it would allow the United States Department of the Interior to \" issue permits for microhydroelectric projects in the Kantishna Hills area of the Denali National Park and Preserve in Alaska . \"",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "Second , the bill would authorize the Department of the Interior and a company called Doyon Tourism , Inc. to exchange some land in the area .",
        "text": "Second , the bill would authorize the Department of the Interior and a company called Doyon Tourism , Inc. to exchange some land in the area .",
        "mention_ids": [
          "M4_1",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "Third , the bill would authorize the National Park Service ( NPS ) to \" issue permits to construct a natural gas pipeline in the Denali National Park . \"",
        "text": "Third , the bill would authorize the National Park Service ( NPS ) to \" issue permits to construct a natural gas pipeline in the Denali National Park . \"",
        "mention_ids": [
          "M10_0",
          "M9_0",
          "M9_1"
        ]
      },
      "05": {
        "orig_text": "Finally , the bill would rename the existing Talkeetna Ranger Station the Walter Harper Talkeetna Ranger Station .",
        "text": "Finally , the bill would rename the existing Talkeetna Ranger Station the Walter Harper Talkeetna Ranger Station .",
        "mention_ids": [
          "M11_0",
          "M12_0"
        ]
      }
    },
