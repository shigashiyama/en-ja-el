      "title": "Lavaca Bay",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Lavaca Bay ( ) is a northwestern extension of the Matagorda Bay system found mostly in Calhoun County , Texas , United States .",
        "text": "Lavaca Bay ( ) is a northwestern extension of the Matagorda Bay system found mostly in Calhoun County , Texas , United States .",
        "mention_ids": [
          "M0_1",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "The ports of Port Lavaca and Point Comfort have been established on the bay , and are the main areas of human habitation .",
        "text": "The ports of Port Lavaca and Point Comfort have been established on the bay , and are the main areas of human habitation .",
        "mention_ids": [
          "M5_0",
          "M6_1"
        ]
      },
      "02": {
        "orig_text": "Linnville was located on the bay until its abandonment after the Great Raid of 1840 , and the major port of Indianola was found near the confluence with the main Matagorda Bay , until the town 's final destruction following the massive hurricane of 1886 .",
        "text": "Linnville was located on the bay until its abandonment after the Great Raid of 1840 , and the major port of Indianola was found near the confluence with the main Matagorda Bay , until the town 's final destruction following the massive hurricane of 1886 .",
        "mention_ids": [
          "M10_0",
          "M1_1",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "Smaller communities include Olivia , Alamo Beach and Magnolia Beach .",
        "text": "Smaller communities include Olivia , Alamo Beach and Magnolia Beach .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "04": {
        "orig_text": "Lavaca Bay is approximately northeast of Corpus Christi , about southwest of Houston , and southeast of San Antonio .",
        "text": "Lavaca Bay is approximately northeast of Corpus Christi , about southwest of Houston , and southeast of San Antonio .",
        "mention_ids": [
          "M0_0",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "05": {
        "orig_text": "The bay is noted for its superfund site , caused by mercury pollution from the heavy industry in Point Comfort ( specifically Alcoa ) , across the bay from the largest settlement of Port Lavaca .",
        "text": "The bay is noted for its superfund site , caused by mercury pollution from the heavy industry in Point Comfort ( specifically Alcoa ) , across the bay from the largest settlement of Port Lavaca .",
        "mention_ids": [
          "M17_0",
          "M5_1",
          "M6_0"
        ]
      },
      "06": {
        "orig_text": "Although fishing has declined in recent years due to fears of contamination , the bay supports a large finfish population , and the efforts of environmental organizations and the federal government have pressured Alcoa to reduce the polluted areas .",
        "text": "Although fishing has declined in recent years due to fears of contamination , the bay supports a large finfish population , and the efforts of environmental organizations and the federal government have pressured Alcoa to reduce the polluted areas .",
        "mention_ids": [
          "M17_1"
        ]
      }
    },
