    "sentences": {
      "00": {
        "orig_text": "M. C. Veerabahu Pillai ( 19 May 1903 – 15 April 1976 ) was an Indian lawyer , businessman , and politician from Tamil Nadu , who served in the first Lok Sabha of independent India ; he was also an independence activist .",
        "text": "M. C. Veerabahu Pillai ( 19 May 1903 – 15 April 1976 ) was an Indian lawyer , businessman , and politician from Tamil Nadu , who served in the first Lok Sabha of independent India ; he was also an independence activist .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "01": {
        "orig_text": "Prior to Indian independence , Veerabahu sacrificed his law career to participate in Mahatma Gandhi 's struggle .",
        "text": "Prior to Indian independence , Veerabahu sacrificed his law career to participate in Mahatma Gandhi 's struggle .",
        "mention_ids": [
          "M0_1",
          "M3_1",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "He was closely associated with stalwarts like Kamaraj and Rajaji .",
        "text": "He was closely associated with stalwarts like Kamaraj and Rajaji .",
        "mention_ids": [
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "He actively worked for removal of untouchability , prohibition and championed the cause of Scheduled Castes .",
        "text": "He actively worked for removal of untouchability , prohibition and championed the cause of Scheduled Castes .",
        "mention_ids": [
          "M10_0"
        ]
      },
      "04": {
        "orig_text": "He was a member of the Constituent Assembly and Provisional Parliament during 1946 - 1952 .",
        "text": "He was a member of the Constituent Assembly and Provisional Parliament during 1946 - 1952 .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "05": {
        "orig_text": "Though he worked for Freedom fighter ’s pension , he never took any pension throughout his life .",
        "text": "Though he worked for Freedom fighter ’s pension , he never took any pension throughout his life .",
        "mention_ids": [
          "M15_0"
        ]
      },
      "06": {
        "orig_text": "He managed his family expenses only from his ancestral property and income .",
        "text": "He managed his family expenses only from his ancestral property and income .",
        "mention_ids": []
      },
      "07": {
        "orig_text": "He always worked for the social cause .",
        "text": "He always worked for the social cause .",
        "mention_ids": []
      }
    },
