    "sentences": {
      "00": {
        "orig_text": "Derby ( ' dər - bē ) is a city in New Haven County , Connecticut , United States , approximately 8 miles west - northwest of New Haven , Connecticut .",
        "text": "Derby ( ' dər - bē ) is a city in New Haven County , Connecticut , United States , approximately 8 miles west - northwest of New Haven , Connecticut .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M1_0",
          "M1_1",
          "M2_0",
          "M2_1",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "Derby is located in southwest Connecticut at the confluence of the Housatonic and Naugatuck Rivers .",
        "text": "Derby is located in southwest Connecticut at the confluence of the Housatonic and Naugatuck Rivers .",
        "mention_ids": [
          "M0_2",
          "M2_2",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "It borders the cities of Ansonia to the north and Shelton to the southwest , and the towns of Orange to the south , Seymour to the northwest , and Woodbridge to the east .",
        "text": "It borders the cities of Ansonia to the north and Shelton to the southwest , and the towns of Orange to the south , Seymour to the northwest , and Woodbridge to the east .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The population was 12,903 at the 2010 census .",
        "text": "The population was 12,903 at the 2010 census .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      },
      "04": {
        "orig_text": "It is the smallest consolidated city in Connecticut by area and population .",
        "text": "It is the smallest consolidated city in Connecticut by area and population .",
        "mention_ids": [
          "M2_3"
        ]
      },
      "05": {
        "orig_text": "Derby was settled in 1642 as an Indian trading post under the name Paugasset .",
        "text": "Derby was settled in 1642 as an Indian trading post under the name Paugasset .",
        "mention_ids": [
          "M0_5",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "06": {
        "orig_text": "It was named after Derby , England , in 1675 .",
        "text": "It was named after Derby , England , in 1675 .",
        "mention_ids": [
          "M0_3",
          "M17_0",
          "M18_0"
        ]
      },
      "07": {
        "orig_text": "It included what is now Ansonia , Seymour , and parts of Oxford and Beacon Falls .",
        "text": "It included what is now Ansonia , Seymour , and parts of Oxford and Beacon Falls .",
        "mention_ids": [
          "M10_1",
          "M19_0",
          "M20_0",
          "M7_1"
        ]
      },
      "08": {
        "orig_text": "Derby is home to the first trolley system in New England and the first electric locomotive in U.S. history to be built and successfully used commercially for hauling freight .",
        "text": "Derby is home to the first trolley system in New England and the first electric locomotive in U.S. history to be built and successfully used commercially for hauling freight .",
        "mention_ids": [
          "M0_4",
          "M21_0",
          "M3_1"
        ]
      },
      "09": {
        "orig_text": "The locomotive , built in 1888 is still kept in running condition by the Shore Line Trolley Museum .",
        "text": "The locomotive , built in 1888 is still kept in running condition by the Shore Line Trolley Museum .",
        "mention_ids": [
          "M22_0",
          "M23_0"
        ]
      }
    },
