    "sentences": {
      "00": {
        "orig_text": "The lime leaf miner ( Phyllonorycter issikii ) is a moth of the Gracillariidae family .",
        "text": "The lime leaf miner ( Phyllonorycter issikii ) is a moth of the Gracillariidae family .",
        "mention_ids": [
          "M0_0",
          "M1_0"
        ]
      },
      "01": {
        "orig_text": "It is native to Japan , Korea and eastern Russia , but was probably introduced to eastern Europe around 1970 .",
        "text": "It is native to Japan , Korea and eastern Russia , but was probably introduced to eastern Europe around 1970 .",
        "mention_ids": [
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "text": "It is now widespread in the Czech Republic and ■■■■■■■■ and is expanding its range in Hungary and Germany .",
        "mention_ids": [
          "M10_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "It has also been recorded from the Baltic states and also the Benelux in 2009 .",
        "text": "It has also been recorded from the Baltic states and also the Benelux in 2009 .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "04": {
        "orig_text": "The wingspan is 7 - 7.5  mm .",
        "text": "The wingspan is 7 - 7.5 mm .",
        "mention_ids": [
          "M14_0"
        ]
      },
      "05": {
        "orig_text": "Adults are on wing from the end of June to mid - July and from the end of July to the end of August in two generations .",
        "text": "Adults are on wing from the end of June to mid - July and from the end of July to the end of August in two generations .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "The larvae feed on Tilia americana , Tilia cordata and Tilia platyphyllos .",
        "text": "The larvae feed on Tilia americana , Tilia cordata and Tilia platyphyllos .",
        "mention_ids": [
          "M15_0",
          "M15_1",
          "M15_2"
        ]
      },
      "07": {
        "orig_text": "They mine the leaves of their host plant .",
        "text": "They mine the leaves of their host plant .",
        "mention_ids": []
      },
      "08": {
        "orig_text": "They create a lower - surface , somewhat contracted tentiform mine .",
        "text": "They create a lower - surface , somewhat contracted tentiform mine .",
        "mention_ids": []
      },
      "09": {
        "orig_text": "There are often several mines in a single leaf .",
        "text": "There are often several mines in a single leaf .",
        "mention_ids": []
      },
      "10": {
        "orig_text": "The frass is concentrated in a corner of the mine , recognisable from above as a black patch .",
        "text": "The frass is concentrated in a corner of the mine , recognisable from above as a black patch .",
        "mention_ids": []
      }
    },
