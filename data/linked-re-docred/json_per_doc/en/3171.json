        ]
      },
      "01": {
        "text": "Unlike with most European monarchies , his large regnal number does not indicate that he was preceded by seventy - one previous rulers also named ■■■■■■■■ .",
        "mention_ids": [
          "M11_1",
          "M12_0",
          "M13_0"
        ]
      },
      "02": {
        "text": "By tradition , all male members of the House of Reuss are named ■■■■■■■■ , and are numbered by order of birth within the family in general .",
        "mention_ids": [
          "M13_1",
          "M14_0"
        ]
      },
      "03": {
        "orig_text": "In the Reuss Younger Line , the numbering system was reset at the beginning of each century ; thus Heinrich LXXII 's high number is directly related to his having been born at the very end of the eighteenth century .",
        "text": "In the Reuss Younger Line , the numbering system was reset at the beginning of each century ; thus Heinrich LXXII 's high number is directly related to his having been born at the very end of the eighteenth century .",
        "mention_ids": [
          "M0_2",
          "M15_0",
          "M16_0"
        ]
      },
      "04": {
        "orig_text": "Heinrich LXXII abdicated in 1848 , due to civil unrest in connection with the revolutions that spread through Germany and elsewhere in Europe that year .",
        "text": "Heinrich LXXII abdicated in 1848 , due to civil unrest in connection with the revolutions that spread through Germany and elsewhere in Europe that year .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M11_0",
          "M17_0"
        ]
      },
      "05": {
        "orig_text": "The last of his branch of the family , his principality passed to the Prince Reuss zu Schleiz , unifying the lands of the Reuss Younger Line .",
        "text": "The last of his branch of the family , his principality passed to the Prince Reuss zu Schleiz , unifying the lands of the Reuss Younger Line .",
        "mention_ids": [
          "M15_1",
          "M1_1"
        ]
      }
