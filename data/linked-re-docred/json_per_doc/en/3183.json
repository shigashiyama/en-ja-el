    "sentences": {
      "00": {
        "orig_text": "Dustin 's Bar Mitzvah are an English four piece band from Acton , London .",
        "text": "Dustin 's Bar Mitzvah are an English four piece band from Acton , London .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "They are signed to Hungry Kid Records in the United Kingdom and Vinyl Junkie records in Japan .",
        "text": "They are signed to Hungry Kid Records in the United Kingdom and Vinyl Junkie records in Japan .",
        "mention_ids": [
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "02": {
        "orig_text": "Dustin 's Bar Mitzvah have achieved greater success in Japan , issuing a collection of demos entitled Dial M For Mitzvah and entering the Japanese singles chart .",
        "text": "Dustin 's Bar Mitzvah have achieved greater success in Japan , issuing a collection of demos entitled Dial M For Mitzvah and entering the Japanese singles chart .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M8_1",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "They played a sold out tour of Japan in January 2006 .",
        "text": "They played a sold out tour of Japan in January 2006 .",
        "mention_ids": [
          "M11_0",
          "M8_2"
        ]
      },
      "04": {
        "orig_text": "They have released three singles in the UK : Jimmy White / Lucy , To The Ramones and Kick Him Out .",
        "text": "They have released three singles in the UK : Jimmy White / Lucy , To The Ramones and Kick Him Out .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0",
          "M6_1"
        ]
      },
      "05": {
        "orig_text": "They released their debut album , Get Your Mood On , on 4 December 2006 .",
        "text": "They released their debut album , Get Your Mood On , on 4 December 2006 .",
        "mention_ids": [
          "M11_1",
          "M15_0"
        ]
      },
      "06": {
        "text": "However , the band announced their decision to split up in April 2007 after drummer ■■■■■■■■■■■■■ decided to quit .",
        "mention_ids": [
          "M16_0",
          "M17_0"
        ]
      },
      "07": {
        "orig_text": "On 15 August 2011 they announced they were reforming and have scheduled a gig at Londons Lexington on 1 October .",
        "text": "On 15 August 2011 they announced they were reforming and have scheduled a gig at Londons Lexington on 1 October .",
        "mention_ids": [
          "M18_0",
          "M19_0",
          "M20_0"
        ]
      },
      "08": {
        "orig_text": "A new website was launched at www.dustins.org.uk",
        "text": "A new website was launched at www.dustins.org.uk",
        "mention_ids": []
      }
