    "sentences": {
      "00": {
        "orig_text": "Major League Baseball ( MLB ) is the highest level of play in North American professional baseball , and is the organization that operates the National League and the American League .",
        "text": "Major League Baseball ( MLB ) is the highest level of play in North American professional baseball , and is the organization that operates the National League and the American League .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "In 2000 , the two leagues were officially disbanded as separate legal entities , and all of their rights and functions were consolidated in the commissioner 's office .",
        "text": "In 2000 , the two leagues were officially disbanded as separate legal entities , and all of their rights and functions were consolidated in the commissioner 's office .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Since that time , MLB has operated as a single league , and constitutes one of the major professional sports leagues of the United States .",
        "text": "Since that time , MLB has operated as a single league , and constitutes one of the major professional sports leagues of the United States .",
        "mention_ids": [
          "M0_2",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "It is composed of 30 teams .",
        "text": "It is composed of 30 teams .",
        "mention_ids": [
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "The First - Year Player Draft , also known as the Rule 4 Draft , is MLB primary mechanism for assigning amateur baseball players from high schools , colleges , and other amateur baseball clubs to its teams .",
        "text": "The First - Year Player Draft , also known as the Rule 4 Draft , is MLB primary mechanism for assigning amateur baseball players from high schools , colleges , and other amateur baseball clubs to its teams .",
        "mention_ids": [
          "M0_3",
          "M8_0",
          "M8_1"
        ]
      },
      "05": {
        "orig_text": "The first draft took place in 1965 ; it was introduced to prevent richer teams from negotiating wealthier contracts with top - level prospects and therefore , monopolizing the player market .",
        "text": "The first draft took place in 1965 ; it was introduced to prevent richer teams from negotiating wealthier contracts with top - level prospects and therefore , monopolizing the player market .",
        "mention_ids": [
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "Originally , three drafts were held each year .",
        "text": "Originally , three drafts were held each year .",
        "mention_ids": [
          "M10_0"
        ]
      },
      "07": {
        "orig_text": "The first draft took place in June and involved high - school graduates and college seniors who had just finished their seasons .",
        "text": "The first draft took place in June and involved high - school graduates and college seniors who had just finished their seasons .",
        "mention_ids": [
          "M11_0"
        ]
      },
      "08": {
        "orig_text": "The second draft took place in January for high school and college players who had graduated in December .",
        "text": "The second draft took place in January for high school and college players who had graduated in December .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      },
      "09": {
        "orig_text": "The third draft took place in August and was for players who participated in American amateur summer leagues .",
        "text": "The third draft took place in August and was for players who participated in American amateur summer leagues .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "10": {
        "orig_text": "The August draft was eliminated after two years , and the January draft lasted until 1986 .",
        "text": "The August draft was eliminated after two years , and the January draft lasted until 1986 .",
        "mention_ids": [
          "M17_0",
          "M18_0"
        ]
      },
      "11": {
        "orig_text": "The draft order is determined based on the previous season 's standings , with the team possessing the worst record receiving the first pick .",
        "text": "The draft order is determined based on the previous season 's standings , with the team possessing the worst record receiving the first pick .",
        "mention_ids": []
      },
      "12": {
        "orig_text": "In addition , teams that lost free agents in the previous off - season may be awarded \" compensatory \" picks .",
        "text": "In addition , teams that lost free agents in the previous off - season may be awarded \" compensatory \" picks .",
        "mention_ids": []
      }
