    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■ ( born October 15 , 1942 ) is an American jurist .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "He served in both houses of the Ohio General Assembly as a member of the Ohio Republican party and was most recently an Associate Justice of the Supreme Court of Ohio .",
        "text": "He served in both houses of the Ohio General Assembly as a member of the Ohio Republican party and was most recently an Associate Justice of the Supreme Court of Ohio .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "text": "■■■■■■■ was born in Bucyrus in 1942 .",
        "mention_ids": [
          "M0_2",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "He grew up on his family 's dairy farm near Bucyrus .",
        "text": "He grew up on his family 's dairy farm near Bucyrus .",
        "mention_ids": [
          "M6_1"
        ]
      },
      "04": {
        "orig_text": "As a teenager , he raised purebred Yorkshire hogs to finance his college education .",
        "text": "As a teenager , he raised purebred Yorkshire hogs to finance his college education .",
        "mention_ids": [
          "M8_0"
        ]
      },
      "05": {
        "orig_text": "He earned a bachelor of arts degree in economics , political science , and history in 1963 from Ohio State University .",
        "text": "He earned a bachelor of arts degree in economics , political science , and history in 1963 from Ohio State University .",
        "mention_ids": [
          "M10_0",
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "In 1966 , he also earned a law degree from the College of Law .",
        "text": "In 1966 , he also earned a law degree from the College of Law .",
        "mention_ids": [
          "M11_0",
          "M12_0"
        ]
      },
      "07": {
        "text": "■■■■■■■ owns a cattle farm in Crawford County , near his childhood home .",
        "mention_ids": [
          "M0_1",
          "M13_0"
        ]
      },
      "08": {
        "text": "■■■■■■■ and his wife ■■■■■ have three children and four grandchildren .",
        "mention_ids": [
          "M0_3",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      }
    },
