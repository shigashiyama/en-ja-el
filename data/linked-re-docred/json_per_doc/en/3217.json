      "title": "Paul R. Ehrlich",
      "url": null
    },
    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■■ ( born May 29 , 1932 ) is an American biologist , best known for his warnings about the consequences of population growth and limited resources .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "text": "He is the ■■■■ Professor of Population Studies of the Department of Biology of Stanford University and president of Stanford 's Center for Conservation Biology .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M4_1",
          "M5_0"
        ]
      },
      "02": {
        "text": "■■■■■■■ became well known for his controversial 1968 book The Population Bomb , which asserted that the world 's human population would soon increase to the point where mass starvation ensued .",
        "mention_ids": [
          "M0_2",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "Among the solutions he suggested in that book was population control , to be used in his opinion if voluntary methods were to fail .",
        "text": "Among the solutions he suggested in that book was population control , to be used in his opinion if voluntary methods were to fail .",
        "mention_ids": []
      },
      "04": {
        "text": "■■■■■■■ has been criticized for his opinions ; for example , ■■■■■■■■■■■■■ termed ■■■■■■■ an \" irrepressible doomster \" .",
        "mention_ids": [
          "M0_3",
          "M0_5",
          "M8_0"
        ]
      },
      "05": {
        "text": "However , ■■■■■■■■■ observed that ■■■■■■■ 's warnings had encouraged governments to change their policies to avert disaster .",
        "mention_ids": [
          "M0_4",
          "M9_0"
        ]
      },
      "06": {
        "text": "■■■■■■■ has acknowledged that some of what he predicted has not occurred , but maintains that his predictions about disease and climate change were essentially correct , and that human overpopulation is a major problem .",
        "mention_ids": [
          "M0_1"
        ]
      }
    },
