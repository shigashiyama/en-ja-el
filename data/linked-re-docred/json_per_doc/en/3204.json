    "sentences": {
      "00": {
        "orig_text": "Giuseppe Barattolo ( 1882 – 1949 ) was an Italian lawyer , politician and film producer .",
        "text": "Giuseppe Barattolo ( 1882 – 1949 ) was an Italian lawyer , politician and film producer .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_1"
        ]
      },
      "01": {
        "orig_text": "In 1913 , during the silent era , Barattolo founded Caesar Film .",
        "text": "In 1913 , during the silent era , Barattolo founded Caesar Film .",
        "mention_ids": [
          "M0_1",
          "M4_0",
          "M5_2"
        ]
      },
      "02": {
        "orig_text": "Caesar made a series of popular films , some of which starred Francesca Bertini .",
        "text": "Caesar made a series of popular films , some of which starred Francesca Bertini .",
        "mention_ids": [
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "In 1919 he joined and became an influential figure at the conglomerate Unione Cinematografica Italiana which dominated Italian film production during the early 1920s , but this came to an end following the box office failure of Quo Vadis ( 1924 ) .",
        "text": "In 1919 he joined and became an influential figure at the conglomerate Unione Cinematografica Italiana which dominated Italian film production during the early 1920s , but this came to an end following the box office failure of Quo Vadis ( 1924 ) .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M3_4",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "In the early 1930s he tried to re - establish Caesar Film as a serious force in Italian production , but this also failed .",
        "text": "In the early 1930s he tried to re - establish Caesar Film as a serious force in Italian production , but this also failed .",
        "mention_ids": [
          "M12_0",
          "M3_2",
          "M5_1"
        ]
      },
      "05": {
        "orig_text": "During the 1930s , Barattolo lobbied the Fascist government of Italy for aid to rebuild the Italian film industry .",
        "text": "During the 1930s , Barattolo lobbied the Fascist government of Italy for aid to rebuild the Italian film industry .",
        "mention_ids": [
          "M0_3",
          "M12_1",
          "M13_0",
          "M14_0",
          "M3_0"
        ]
      },
      "06": {
        "orig_text": "He was a strong supporter of the government 's plans during the mid-1930s to invest large amounts of money constructing Cinecittà studios in Rome , and during the era he worked at the state - backed Scalera Film .",
        "text": "He was a strong supporter of the government 's plans during the mid-1930s to invest large amounts of money constructing Cinecittà studios in Rome , and during the era he worked at the state - backed Scalera Film .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M17_0",
          "M18_0"
        ]
      },
      "07": {
        "orig_text": "Barattolo was instrumental in attempts to rebuild the Italian film industry in Venice during the Italian Social Republic .",
        "text": "Barattolo was instrumental in attempts to rebuild the Italian film industry in Venice during the Italian Social Republic .",
        "mention_ids": [
          "M0_2",
          "M19_0",
          "M20_0",
          "M3_3"
        ]
      },
      "08": {
        "orig_text": "After the fall of Benito Mussolini Barattolo worked as an independent producer .",
        "text": "After the fall of Benito Mussolini Barattolo worked as an independent producer .",
        "mention_ids": [
          "M0_4",
          "M21_0"
        ]
      }
