    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■■■ is a professor at the Universidad Nacional de Ingeniería and a Peruvian politician who ran unsuccessfully for president in the 2006 election on the ■■■■■■■■■■■ ticket .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "text": "He was running against his brother , ■■■■■■■■■■■■■■ , and 18 other candidates .",
        "mention_ids": [
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "text": "■■■■■■ received 0.2 % of the vote , coming in 14th place .",
        "mention_ids": [
          "M7_1",
          "M8_0"
        ]
      },
      "03": {
        "text": "Like his brother ■■■■■■■ , ■■■■■■■■■■■■■ considers himself to be a nationalist .",
        "mention_ids": [
          "M5_2",
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "However , he claims to be less radical .",
        "text": "However , he claims to be less radical .",
        "mention_ids": []
      },
      "05": {
        "orig_text": "He also considers the current 1993 constitution , produced in the \" Democratic Constitutional Congress \" after Alberto Fujimori 's self - coup and during the Peruvian Constitutional Crisis of 1992 , to be illegal .",
        "text": "He also considers the current 1993 constitution , produced in the \" Democratic Constitutional Congress \" after Alberto Fujimori 's self - coup and during the Peruvian Constitutional Crisis of 1992 , to be illegal .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M9_0"
        ]
      },
      "06": {
        "text": "One of his other brothers , ■■■■■■■■■■■■■■ , is currently in prison for leading a failed military rebellion .",
        "mention_ids": [
          "M14_0"
        ]
      },
      "07": {
        "text": "The other brother , ■■■■■■■■■■■■■■ , served as the 65th President of Peru .",
        "mention_ids": [
          "M15_0",
          "M5_1"
        ]
      }
    },
