    "sentences": {
      "00": {
        "orig_text": "The Atlantic Charter was a pivotal policy statement issued during World War II on 14 August 1941 which defined the Allied goals for the post - war world .",
        "text": "The Atlantic Charter was a pivotal policy statement issued during World War II on 14 August 1941 which defined the Allied goals for the post - war world .",
        "mention_ids": [
          "M0_2",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "The leaders of the United Kingdom and the United States drafted the work and all the Allies of World War II later confirmed it .",
        "text": "The leaders of the United Kingdom and the United States drafted the work and all the Allies of World War II later confirmed it .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "The Charter stated the ideal goals of the war : no territorial aggrandizement ; no territorial changes made against the wishes of the people ( self - determination ) ; restoration of self - government to those deprived of it ; reduction of trade restrictions ; global cooperation to secure better economic and social conditions for all ; freedom from fear and want ; freedom of the seas ; and abandonment of the use of force , as well as disarmament of aggressor nations .",
        "text": "The Charter stated the ideal goals of the war : no territorial aggrandizement ; no territorial changes made against the wishes of the people ( self - determination ) ; restoration of self - government to those deprived of it ; reduction of trade restrictions ; global cooperation to secure better economic and social conditions for all ; freedom from fear and want ; freedom of the seas ; and abandonment of the use of force , as well as disarmament of aggressor nations .",
        "mention_ids": [
          "M0_4"
        ]
      },
      "03": {
        "orig_text": "Adherents of the Atlantic Charter signed the Declaration by United Nations on 1 January 1942 , which became the basis for the modern United Nations .",
        "text": "Adherents of the Atlantic Charter signed the Declaration by United Nations on 1 January 1942 , which became the basis for the modern United Nations .",
        "mention_ids": [
          "M0_1",
          "M7_0",
          "M8_0",
          "M8_1",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The Atlantic Charter set goals for the postwar world and inspired many of the international agreements that followed the war .",
        "text": "The Atlantic Charter set goals for the postwar world and inspired many of the international agreements that followed the war .",
        "mention_ids": [
          "M0_0"
        ]
      },
      "05": {
        "orig_text": "The General Agreement on Tariffs and Trade ( GATT ) , the postwar independence of European colonies , and much more are derived from the Atlantic Charter .",
        "text": "The General Agreement on Tariffs and Trade ( GATT ) , the postwar independence of European colonies , and much more are derived from the Atlantic Charter .",
        "mention_ids": [
          "M0_3",
          "M10_0",
          "M10_1",
          "M11_0"
        ]
      }
    },
