    "sentences": {
      "00": {
        "orig_text": "Saint John Eudes ( ) ( 14 November 1601 – 19 August 1680 ) was a French Roman Catholic priest and the founder of both the Eudists and the Order of Our Lady of Charity .",
        "text": "Saint John Eudes ( ) ( 14 November 1601 – 19 August 1680 ) was a French Roman Catholic priest and the founder of both the Eudists and the Order of Our Lady of Charity .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "Eudes was an ardent proponent of the Sacred Hearts and dedicated himself to its promotion and celebration ; the Masses he compiled for both Sacred Hearts were later said for the first time both in his lifetime .",
        "text": "Eudes was an ardent proponent of the Sacred Hearts and dedicated himself to its promotion and celebration ; the Masses he compiled for both Sacred Hearts were later said for the first time both in his lifetime .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M10_1",
          "M11_0"
        ]
      },
      "03": {
        "orig_text": "He preached missions across France including Paris and Versailles while becoming known as a popular evangelist as well as a sought - out confessor and preacher .",
        "text": "He preached missions across France including Paris and Versailles while becoming known as a popular evangelist as well as a sought - out confessor and preacher .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "04": {
        "text": "Father Eudes was also a prolific writer and wrote on the Sacred Hearts while also condemning the Jansenists in favour of full support for the ■■■■ .",
        "mention_ids": [
          "M0_2",
          "M10_2",
          "M15_0",
          "M16_0"
        ]
      },
      "05": {
        "orig_text": "Eudes was canonized as a saint in mid-1925 and there is a current push to have him named as a Doctor of the Church .",
        "text": "Eudes was canonized as a saint in mid-1925 and there is a current push to have him named as a Doctor of the Church .",
        "mention_ids": [
          "M0_3",
          "M17_0"
        ]
      }
