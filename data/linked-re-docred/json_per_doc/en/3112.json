    "sentences": {
      "00": {
        "orig_text": "Johannes Cuspinianus ( December 1473 – 19 April 1529 ) , born Johan Spießhaymer ( or Speißheimer ) , was an Austrian humanist , scientist , diplomat , and historian .",
        "text": "Johannes Cuspinianus ( December 1473 – 19 April 1529 ) , born Johan Spießhaymer ( or Speißheimer ) , was an Austrian humanist , scientist , diplomat , and historian .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M0_2",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "Born in Spießheim near Schweinfurt in Franconia , of which Cuspinianus is a Latinization , he studied in Leipzig and Würzburg .",
        "text": "Born in Spießheim near Schweinfurt in Franconia , of which Cuspinianus is a Latinization , he studied in Leipzig and Würzburg .",
        "mention_ids": [
          "M0_4",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "He went to Vienna in 1492 and became a professor of medicine at the University of Vienna .",
        "text": "He went to Vienna in 1492 and became a professor of medicine at the University of Vienna .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0"
        ]
      },
      "03": {
        "orig_text": "He became Rector of the university in 1500 and also served as Royal Superintendent until his death .",
        "text": "He became Rector of the university in 1500 and also served as Royal Superintendent until his death .",
        "mention_ids": [
          "M13_0"
        ]
      },
      "04": {
        "orig_text": "A leading scholar , he was the author of De Caesaribus et Imperatoribus and was also given a poet ’s laurel wreath by Maximilian I , Holy Roman Emperor .",
        "text": "A leading scholar , he was the author of De Caesaribus et Imperatoribus and was also given a poet ’s laurel wreath by Maximilian I , Holy Roman Emperor .",
        "mention_ids": [
          "M14_0",
          "M15_0"
        ]
      },
      "05": {
        "orig_text": "He was part of an intellectual circle that included Joachim Vadianus and Stiborius .",
        "text": "He was part of an intellectual circle that included Joachim Vadianus and Stiborius .",
        "mention_ids": [
          "M16_0",
          "M17_0"
        ]
      },
      "06": {
        "orig_text": "He rendered important service as the discoverer and editor of classical and medieval historical texts .",
        "text": "He rendered important service as the discoverer and editor of classical and medieval historical texts .",
        "mention_ids": []
      },
      "07": {
        "orig_text": "His unfinished Austria ( 1527 - 8 ) was an important historical - geographical regional survey of Lower Austria .",
        "text": "His unfinished Austria ( 1527 - 8 ) was an important historical - geographical regional survey of Lower Austria .",
        "mention_ids": [
          "M18_0",
          "M19_0",
          "M20_0",
          "M21_0"
        ]
      }
