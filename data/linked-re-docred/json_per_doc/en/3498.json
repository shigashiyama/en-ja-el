    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■ ( born 5 March 1983 ) is a German footballer .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_1"
        ]
      },
      "01": {
        "orig_text": "He first appeared in the German Bundesliga on 21 March 2004 while SC Freiburg 's first and second keepers had been injured .",
        "text": "He first appeared in the German Bundesliga on 21 March 2004 while SC Freiburg 's first and second keepers had been injured .",
        "mention_ids": [
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "A year later media and trainer looked upon him as the future number one of Freiburg , but his disastrous effort during a 0–7 defeat against Bayern Munich and the following massive public criticism terminated these ambitions .",
        "text": "A year later media and trainer looked upon him as the future number one of Freiburg , but his disastrous effort during a 0–7 defeat against Bayern Munich and the following massive public criticism terminated these ambitions .",
        "mention_ids": [
          "M5_1",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "While plying his trade at Hakoah Amidar / Ramat Gan on 23 October 2006 he became the first German footballer ever to play a professional match in Israel .",
        "text": "While plying his trade at Hakoah Amidar / Ramat Gan on 23 October 2006 he became the first German footballer ever to play a professional match in Israel .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M2_2",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Due to problems with his groin he quit Ramat Gan and , after a brief spell at FC Wil , is currently without a team .",
        "text": "Due to problems with his groin he quit Ramat Gan and , after a brief spell at FC Wil , is currently without a team .",
