    "sentences": {
      "00": {
        "orig_text": "USS Lyndon B. Johnson ( DDG-1002 ) will be the third and final built for the United States Navy .",
        "text": "USS Lyndon B. Johnson ( DDG-1002 ) will be the third and final built for the United States Navy .",
        "mention_ids": [
          "M0_0",
          "M0_3",
          "M1_0"
        ]
      },
      "01": {
        "orig_text": "The contract to build her was awarded to Bath Iron Works located in Bath , Maine , on 15 September 2011 .",
        "text": "The contract to build her was awarded to Bath Iron Works located in Bath , Maine , on 15 September 2011 .",
        "mention_ids": [
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "The award , along with funds for the construction of , was worth US$ 1.826 billion .",
        "text": "The award , along with funds for the construction of , was worth US$ 1.826 billion .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "03": {
        "text": "On 16 April 2012 , Secretary of the Navy ■■■■■■■■■ announced the ship would be named Lyndon B. Johnson in honor of Lyndon B. Johnson , who served as the 36th President of the United States from 1963 to 1969 .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M11_0",
          "M12_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Johnson served in the Navy during World War II , when he was awarded the Silver Star , and ultimately reached the U.S.",
        "text": "Johnson served in the Navy during World War II, when he was awarded the Silver Star, and ultimately reached the U.S. Naval Reserve rank of commander.",
        "mention_ids": [
          "M13_0",
          "M14_0",
          "M15_0",
          "M1_1",
          "M9_1"
        ]
      },
      "05": {
        "orig_text": "Naval Reserve rank of commander .",
        "text": "Naval Reserve rank of commander .",
        "mention_ids": [
          "M16_0"
        ]
      },
      "06": {
        "orig_text": "DDG-1002 is the 34th ship named by the Navy after a U.S. president .",
        "text": "DDG-1002 is the 34th ship named by the Navy after a U.S. president .",
        "mention_ids": [
          "M0_2",
          "M1_2"
        ]
      }
