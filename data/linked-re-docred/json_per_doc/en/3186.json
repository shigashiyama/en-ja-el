      "01": {
        "orig_text": "Belyi was born on February 2 , 1951 , in Magnitogorsk , Russia , then part of the Soviet Union .",
        "text": "Belyi was born on February 2 , 1951 , in Magnitogorsk , Russia , then part of the Soviet Union .",
        "mention_ids": [
          "M0_2",
          "M3_1",
          "M5_2",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "His family moved from there to Ukraine , and he began his studies at the Kiev Physics and Mathematics School but moved from there to Moscow State University .",
        "text": "His family moved from there to Ukraine , and he began his studies at the Kiev Physics and Mathematics School but moved from there to Moscow State University .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M4_2"
        ]
      },
      "03": {
        "orig_text": "After completing his studies in 1973 he returned to Ukraine , working in Kiev and then Lviv .",
        "text": "After completing his studies in 1973 he returned to Ukraine , working in Kiev and then Lviv .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0",
          "M4_1"
        ]
      },
      "04": {
        "orig_text": "He became a graduate student at the Steklov Institute of Mathematics in Moscow in 1975 , and studied there under the supervision of Igor Shafarevich , earning a candidate degree in 1979 .",
        "text": "He became a graduate student at the Steklov Institute of Mathematics in Moscow in 1975 , and studied there under the supervision of Igor Shafarevich , earning a candidate degree in 1979 .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M17_0",
          "M18_0",
          "M19_0"
        ]
      },
      "05": {
        "orig_text": "He then took a faculty position at Vladimir State University , in Vladimir , Russia , where he remained for the remainder of his career .",
        "text": "He then took a faculty position at Vladimir State University , in Vladimir , Russia , where he remained for the remainder of his career .",
        "mention_ids": [
          "M20_0",
          "M21_1",
          "M5_1"
        ]
      },
      "06": {
        "orig_text": "He died on January 29 , 2001 , in Vladimir .",
        "text": "He died on January 29 , 2001 , in Vladimir .",
        "mention_ids": [
          "M21_0",
          "M22_0"
        ]
      },
      "07": {
        "orig_text": "Belyi won a prize of the Moscow Mathematical Society in 1981 , and was an invited speaker at the International Congress of Mathematicians in 1986 .",
        "text": "Belyi won a prize of the Moscow Mathematical Society in 1981 , and was an invited speaker at the International Congress of Mathematicians in 1986 .",
