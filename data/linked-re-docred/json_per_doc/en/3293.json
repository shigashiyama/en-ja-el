      },
      "01": {
        "orig_text": "The university was formerly known as the University of Cross River State ( UNICROSS ) .",
        "text": "The university was formerly known as the University of Cross River State ( UNICROSS ) .",
        "mention_ids": [
          "M4_1",
          "M4_2"
        ]
      },
      "02": {
        "orig_text": "On October 1 , 1991 the federal government of Nigeria established it as a federal university and the name was changed to the University of Uyo .",
        "text": "On October 1 , 1991 the federal government of Nigeria established it as a federal university and the name was changed to the University of Uyo .",
        "mention_ids": [
          "M0_2",
          "M3_1",
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "The university inherited students , staff , academic programmes and the entire facilities of the erstwhile University of Cross River State established by Cross River State in 1983 .",
        "text": "The university inherited students , staff , academic programmes and the entire facilities of the erstwhile University of Cross River State established by Cross River State in 1983 .",
        "mention_ids": [
          "M4_0",
          "M6_0",
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "Academic activities commenced during the 1991/92 academic session .",
        "text": "Academic activities commenced during the 1991/92 academic session .",
        "mention_ids": [
          "M8_0"
        ]
      },
      "05": {
        "orig_text": "UNIUYO has about 12 faculties , the postgraduate school and the school of continuing Education .",
        "text": "UNIUYO has about 12 faculties , the postgraduate school and the school of continuing Education .",
        "mention_ids": [
          "M0_3",
          "M9_0"
        ]
      },
