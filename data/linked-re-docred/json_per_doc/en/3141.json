    "sentences": {
      "00": {
        "text": "■■■■■■■■■ ( born April 11 , 1958 ) is an Albanian politician .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "text": "A member of the Democratic Party of Albania , ex - Minister of Agriculture , Food , and Consumer Protection in the cabinet of ■■■■■■■■■■■■ .",
        "mention_ids": [
          "M3_0",
          "M4_0"
        ]
      },
      "02": {
        "text": "■■■■ holds a bachelor 's degree in Economics and a bachelor 's degree in Law from the University of Tirana .",
        "mention_ids": [
          "M0_1",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "He holds a PhD in Economics from the Faculty of Economics , University of Tirana .",
        "text": "He holds a PhD in Economics from the Faculty of Economics , University of Tirana .",
        "mention_ids": [
          "M5_1",
          "M8_0"
        ]
      },
      "04": {
        "text": "■■■■ is given the title Professor from the Faculty of Economics , University of Tirana .",
        "mention_ids": [
          "M0_2",
          "M8_1"
        ]
      },
      "05": {
        "orig_text": "He has served as a Professor of Finance and Accounting in the Faculty of Economics , at the University of Tirana .",
        "text": "He has served as a Professor of Finance and Accounting in the Faculty of Economics , at the University of Tirana .",
        "mention_ids": [
          "M7_3",
          "M9_0"
        ]
      },
      "06": {
        "text": "■■■■ has an extensive experience as the Minister of Finance and Economy in early 90 ’s and as the Minister of Economy , Trade and Energy during 2005 - 2009 .",
        "mention_ids": [
          "M0_3",
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_1",
          "M14_0"
        ]
      },
      "07": {
        "text": "■■■■ resigned from his position as Finance Minister on 9 November 1993 , following allegations of corruption .",
        "mention_ids": [
          "M0_4",
          "M15_0"
        ]
      },
      "08": {
        "text": "■■■■ has also been one of the Members of the National Assembly during the early 90 ’s and again since 2005 .",
        "mention_ids": [
          "M0_5",
          "M11_1",
          "M13_0",
          "M16_0"
        ]
      },
      "09": {
        "orig_text": "He was also the founder and President of the Institute of Contemporary Studies during the years 1996 to 2005 .",
        "text": "He was also the founder and President of the Institute of Contemporary Studies during the years 1996 to 2005 .",
        "mention_ids": [
          "M13_2",
          "M17_0",
          "M18_0"
        ]
      },
      "10": {
        "text": "■■■■ has been engaged in many national and international activities , with well - known organizations and institutions .",
        "mention_ids": [
          "M0_6"
        ]
      },
      "11": {
        "orig_text": "He has been the Chairman of the Supervisory Board , Insurance Institute ( INSIG ) ; Chairman of the Parliamentary Commission of Economy and Finance .",
        "text": "He has been the Chairman of the Supervisory Board , Insurance Institute ( INSIG ) ; Chairman of the Parliamentary Commission of Economy and Finance .",
        "mention_ids": [
          "M19_0",
          "M20_0",
          "M20_1",
          "M21_0"
        ]
      },
      "12": {
        "orig_text": "He is President of the Mountain Areas Development Programme ( MADA ) ; EBRD Governor for Albania .",
        "text": "He is President of the Mountain Areas Development Programme ( MADA ) ; EBRD Governor for Albania .",
        "mention_ids": [
          "M22_0",
          "M22_1",
          "M23_0",
          "M24_0"
        ]
      },
      "13": {
        "text": "■■■■ has written several publications in the areas of economics and public policies .",
        "mention_ids": [
          "M0_7"
        ]
      }
    },
