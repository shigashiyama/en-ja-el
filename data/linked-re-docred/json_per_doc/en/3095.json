    "sentences": {
      "00": {
        "orig_text": "Walter Newman ( 11 February 1916 – 14 October 1993 ) was an American radio writer and screenwriter active from the late 1940s to the early 1990s .",
        "text": "Walter Newman ( 11 February 1916 – 14 October 1993 ) was an American radio writer and screenwriter active from the late 1940s to the early 1990s .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_1",
