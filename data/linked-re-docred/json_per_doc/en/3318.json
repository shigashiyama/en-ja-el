    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■ ( born 29 February 1948 ) is a Polish diplomat .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "He served as Deputy Foreign Minister ( 1991 - 1995 ) , Ambassador to Belgium and Permanent Representative to the European Union .",
        "text": "He served as Deputy Foreign Minister ( 1991 - 1995 ) , Ambassador to Belgium and Permanent Representative to the European Union .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "He is a law graduate from the Adam Mickiewicz University in Poznań and studied at the College of Europe ( 1971 - 1972 , Dante Alighieri promotion ) in Bruges .",
        "text": "He is a law graduate from the Adam Mickiewicz University in Poznań and studied at the College of Europe ( 1971 - 1972 , Dante Alighieri promotion ) in Bruges .",
        "mention_ids": [
          "M10_0",
          "M11_1",
          "M12_0",
          "M13_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "He has a doctorate from the Institute of State and Law of the Polish Academy of Sciences in 1972 .",
        "text": "He has a doctorate from the Institute of State and Law of the Polish Academy of Sciences in 1972 .",
        "mention_ids": [
          "M11_0",
          "M14_0"
        ]
      },
      "04": {
        "orig_text": "He joined the foreign ministry in 1990 when Poland became independent , and worked in business since the mid 90s .",
        "text": "He joined the foreign ministry in 1990 when Poland became independent , and worked in business since the mid 90s .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      },
      "05": {
        "orig_text": "In 2001 , he was appointed Permanent Representative of Poland to the European Union .",
        "text": "In 2001 , he was appointed Permanent Representative of Poland to the European Union .",
        "mention_ids": [
          "M16_1",
          "M18_0",
          "M6_1"
        ]
      },
      "06": {
        "orig_text": "From 2002 to 2007 , he served as Ambassador to Belgium .",
        "text": "From 2002 to 2007 , he served as Ambassador to Belgium .",
        "mention_ids": [
          "M19_0",
          "M20_0",
          "M5_1"
        ]
      }
