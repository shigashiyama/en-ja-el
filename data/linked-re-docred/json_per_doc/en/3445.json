    "sentences": {
      "00": {
        "orig_text": "Uptoi Indian village is located in Columbus , Georgia .",
        "text": "Uptoi Indian village is located in Columbus , Georgia .",
        "mention_ids": [
          "M0_2",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "\" Uptoi \" or \" Utoy \" means \" boundary \" in the Muscogian Creek Language .",
        "text": "\" Uptoi \" or \" Utoy \" means \" boundary \" in the Muscogian Creek Language .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "An early Indian village of the Creek Muscogian Indians was established in the 15th century .",
        "text": "An early Indian village of the Creek Muscogian Indians was established in the 15th century .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "Peaceful farmers traded with local colonists .",
        "text": "Peaceful farmers traded with local colonists .",
        "mention_ids": []
      },
      "04": {
        "orig_text": "Their lands were guaranteed under a treaty with the United States Senate .",
        "text": "Their lands were guaranteed under a treaty with the United States Senate .",
        "mention_ids": [
          "M7_0"
        ]
      },
      "05": {
        "orig_text": "Their lands were forcibly ceded at Indian Springs , Georgia by the US Army influenced by the State of Georgia who wanted more free land to give to immigrants .",
        "text": "Their lands were forcibly ceded at Indian Springs , Georgia by the US Army influenced by the State of Georgia who wanted more free land to give to immigrants .",
        "mention_ids": [
          "M10_0",
          "M2_1",
          "M8_0",
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "In 1821 they were forcibly moved to the current state of Oklahoma by the US Army as ordered by President Andrew Jackson .",
        "text": "In 1821 they were forcibly moved to the current state of Oklahoma by the US Army as ordered by President Andrew Jackson .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0",
          "M9_1"
        ]
      },
      "07": {
        "orig_text": "Today the lands are occupied by the US Army at Fort Benning , Georgia .",
        "text": "Today the lands are occupied by the US Army at Fort Benning , Georgia .",
        "mention_ids": [
          "M14_0",
          "M2_2",
          "M9_2"
        ]
      }
    },
