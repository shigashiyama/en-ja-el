    "sentences": {
      "00": {
        "orig_text": "Concordia Research Station , which opened in 2005 , is a French - Italian research facility that was built above sea level at a location called Dome C on the Antarctic Plateau , Antarctica .",
        "text": "Concordia Research Station , which opened in 2005 , is a French - Italian research facility that was built above sea level at a location called Dome C on the Antarctic Plateau , Antarctica .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "01": {
        "orig_text": "It is located inland from the French research station at Dumont D'Urville , inland from Australia 's Casey Station and inland from the Italian Zucchelli Station at Terra Nova Bay .",
        "text": "It is located inland from the French research station at Dumont D'Urville , inland from Australia 's Casey Station and inland from the Italian Zucchelli Station at Terra Nova Bay .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M2_1",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "02": {
        "orig_text": "Russia 's Vostok Station is away .",
        "text": "Russia 's Vostok Station is away .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      },
      "03": {
        "orig_text": "The Geographic South Pole is away .",
        "text": "The Geographic South Pole is away .",
        "mention_ids": [
          "M14_0"
        ]
      },
      "04": {
        "orig_text": "The facility is also located within Australia 's claim on Antarctica , the Australian Antarctic Territory .",
        "text": "The facility is also located within Australia 's claim on Antarctica , the Australian Antarctic Territory .",
        "mention_ids": [
          "M15_0",
          "M6_1",
          "M8_1"
        ]
      },
      "05": {
        "orig_text": "Concordia Station is the third permanent , all - year research station on the Antarctic Plateau besides Vostok Station ( Russian ) and the Amundsen – Scott South Pole Station ( U.S. ) at the Geographic South Pole .",
        "text": "Concordia Station is the third permanent , all - year research station on the Antarctic Plateau besides Vostok Station ( Russian ) and the Amundsen – Scott South Pole Station ( U.S. ) at the Geographic South Pole .",
        "mention_ids": [
          "M0_1",
          "M13_1",
          "M14_1",
          "M16_0",
          "M17_0",
          "M18_0",
          "M5_1"
        ]
      },
      "06": {
        "orig_text": "It is jointly operated by scientists from France and Italy and regularly hosts ESA scientists .",
        "text": "It is jointly operated by scientists from France and Italy and regularly hosts ESA scientists .",
