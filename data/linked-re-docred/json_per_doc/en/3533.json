    "sentences": {
      "00": {
        "text": "Resident Evil : Degeneration , known in Japan as , is a biopunk action horror film directed by ■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It is the first full - length motion capture CG animation feature in Capcom 's Resident Evil franchise .",
        "text": "It is the first full - length motion capture CG animation feature in Capcom 's Resident Evil franchise .",
        "mention_ids": [
          "M3_0",
          "M4_1",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "The film was made by Capcom Studios in cooperation with Sony Pictures Entertainment .",
        "text": "The film was made by Capcom Studios in cooperation with Sony Pictures Entertainment .",
        "mention_ids": [
          "M4_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "Degeneration made its premiere in Japan on October 11 , 2008 at the Tokyo Game Show , and was released nationwide one week later on October 18 .",
        "text": "Degeneration made its premiere in Japan on October 11 , 2008 at the Tokyo Game Show , and was released nationwide one week later on October 18 .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M1_1",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Unlike the Resident Evil live - action film series , Degeneration is set within the same universe as the original video game series .",
        "text": "Unlike the Resident Evil live - action film series , Degeneration is set within the same universe as the original video game series .",
        "mention_ids": [
          "M0_1",
          "M5_1"
        ]
      },
      "05": {
        "text": "The main characters are ■■■■■■■■■■■■■■■ and ■■■■■■■■■■■■■■■ , who appear together for the first time since the 1998 game Resident Evil 2 .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      }
    },
