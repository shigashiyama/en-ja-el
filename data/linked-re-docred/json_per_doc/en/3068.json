    "sentences": {
      "00": {
        "orig_text": "Latourell Falls is a waterfall along the Columbia River Gorge in the U.S. state of Oregon , within Guy W. Talbot State Park .",
        "text": "Latourell Falls is a waterfall along the Columbia River Gorge in the U.S. state of Oregon , within Guy W. Talbot State Park .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "The Historic Columbia River Highway passes nearby , and at certain locations the Lower falls are visible from the road .",
        "text": "The Historic Columbia River Highway passes nearby , and at certain locations the Lower falls are visible from the road .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Near the base of the falls , a parking lot and path were erected to assist visitors to the site .",
        "text": "Near the base of the falls , a parking lot and path were erected to assist visitors to the site .",
        "mention_ids": []
      },
      "03": {
        "orig_text": "Visitors must hike along the loop trail to see the upper falls .",
        "text": "Visitors must hike along the loop trail to see the upper falls .",
        "mention_ids": []
      },
      "04": {
        "orig_text": "Latourell is unique among the best - known Columbia Gorge waterfalls , in the way that it drops straight down from an overhanging basalt cliff .",
        "text": "Latourell is unique among the best - known Columbia Gorge waterfalls , in the way that it drops straight down from an overhanging basalt cliff .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "05": {
        "orig_text": "Most of those falls ( even the famous Multnomah Falls ) tumble to some degree .",
        "text": "Most of those falls ( even the famous Multnomah Falls ) tumble to some degree .",
        "mention_ids": [
          "M8_0"
        ]
      },
      "06": {
        "orig_text": "Latourell Falls is an excellent example of columnar basalt formations .",
        "text": "Latourell Falls is an excellent example of columnar basalt formations .",
        "mention_ids": [
          "M0_1"
        ]
      }
    },
