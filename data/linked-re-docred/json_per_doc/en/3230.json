    "sentences": {
      "00": {
        "orig_text": "Los Alerces National Park ( ) is located in the Andes in Chubut Province in the Patagonian region of Argentina .",
        "text": "Los Alerces National Park ( ) is located in the Andes in Chubut Province in the Patagonian region of Argentina .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "Its western boundary coincides with the Chilean border .",
        "text": "Its western boundary coincides with the Chilean border .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Successive glaciations have molded the landscape in the region creating spectacular features such as moraines , glacial cirques and clear - water lakes .",
        "text": "Successive glaciations have molded the landscape in the region creating spectacular features such as moraines , glacial cirques and clear - water lakes .",
        "mention_ids": []
      },
      "03": {
        "orig_text": "The vegetation is dominated by dense temperate forests , which give way to alpine meadows higher up under the rocky Andean peaks .",
        "text": "The vegetation is dominated by dense temperate forests , which give way to alpine meadows higher up under the rocky Andean peaks .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "04": {
        "orig_text": "A highly distinctive and emblematic feature is its alerce forest ; the globally threatened alerce tree is the second longest living tree species in the world ( > 3,600 years ) .",
        "text": "A highly distinctive and emblematic feature is its alerce forest ; the globally threatened alerce tree is the second longest living tree species in the world ( > 3,600 years ) .",
        "mention_ids": [
          "M7_0"
        ]
      },
      "05": {
        "orig_text": "The alerce forests in the park are in an excellent state of conservation .",
        "text": "The alerce forests in the park are in an excellent state of conservation .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "The property is vital for the protection of some of the last portions of continuous Patagonian Forest in an almost pristine state and is the habitat for a number of endemic and threatened species of flora and fauna .",
        "text": "The property is vital for the protection of some of the last portions of continuous Patagonian Forest in an almost pristine state and is the habitat for a number of endemic and threatened species of flora and fauna .",
        "mention_ids": [
          "M8_0"
        ]
      }
    },
