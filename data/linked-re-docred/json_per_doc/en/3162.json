    "sentences": {
      "00": {
        "orig_text": "The Asian Men 's Volleyball Championship is an international volleyball competition in Asia and Oceania contested by the senior men 's national teams of the members of Asian Volleyball Confederation ( AVC ) , the sport 's continent governing body .",
        "text": "The Asian Men 's Volleyball Championship is an international volleyball competition in Asia and Oceania contested by the senior men 's national teams of the members of Asian Volleyball Confederation ( AVC ) , the sport 's continent governing body .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M3_1"
        ]
      },
      "01": {
        "orig_text": "The initial gap between championships was four years , but since 1987 they have been awarded every two years .",
        "text": "The initial gap between championships was four years , but since 1987 they have been awarded every two years .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "The current champion is Japan , which won its ninth title at the 2017 tournament .",
        "text": "The current champion is Japan , which won its ninth title at the 2017 tournament .",
        "mention_ids": [
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "orig_text": "The 19 Asian Championship tournaments have been won by five different national teams .",
        "text": "The 19 Asian Championship tournaments have been won by five different national teams .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Japan have won nine times .",
        "text": "Japan have won nine times .",
        "mention_ids": [
          "M11_0",
          "M7_1"
        ]
      },
      "05": {
        "orig_text": "The other Asian Championship winners are South Korea , with four titles ; China with three titles ; Iran with two titles ; and Australia , with one title .",
        "text": "The other Asian Championship winners are South Korea , with four titles ; China with three titles ; Iran with two titles ; and Australia , with one title .",
