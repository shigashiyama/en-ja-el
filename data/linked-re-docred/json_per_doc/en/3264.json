      },
      "01": {
        "orig_text": "He also helped create the Rallye Monte - Carlo in 1911 .",
        "text": "He also helped create the Rallye Monte - Carlo in 1911 .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "He suggested the international adoption of the checkered flag to end races .",
        "text": "He suggested the international adoption of the checkered flag to end races .",
        "mention_ids": []
      },
      "03": {
        "orig_text": "Since 1979 , the last turn of the Monaco circuit ( the former \" Gazometer turn \" ) just before the finish line , has been named \" Virage Antony Noghès \" after him .",
        "text": "Since 1979 , the last turn of the Monaco circuit ( the former \" Gazometer turn \" ) just before the finish line , has been named \" Virage Antony Noghès \" after him .",
        "mention_ids": [
          "M8_0",
          "M9_0",
          "M9_1",
          "M9_2"
        ]
      },
      "04": {
        "orig_text": "As \" Agent general de la Regie des tabacs \" he was the Director of the Public administration responsible for the management of the monopoly of procurement , manufacturing , and selling of tobacco in the Principality .",
        "text": "As \" Agent general de la Regie des tabacs \" he was the Director of the Public administration responsible for the management of the monopoly of procurement , manufacturing , and selling of tobacco in the Principality .",
        "mention_ids": []
      },
