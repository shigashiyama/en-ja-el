    "sentences": {
      "00": {
        "text": "Microserfs , published by HarperCollins in 1995 , is an epistolary novel by ■■■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It first appeared in short story form as the cover article for the January 1994 issue of Wired magazine and was subsequently expanded to full novel length .",
        "text": "It first appeared in short story form as the cover article for the January 1994 issue of Wired magazine and was subsequently expanded to full novel length .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Set in the early 1990s , it captures the state of the technology industry before Windows 95 , and anticipates the dot - com bubble of the late 1990s .",
        "text": "Set in the early 1990s , it captures the state of the technology industry before Windows 95 , and anticipates the dot - com bubble of the late 1990s .",
        "mention_ids": [
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "text": "The novel is presented in the form of diary entries maintained on a PowerBook by the narrator , ■■■■■■ .",
        "mention_ids": [
          "M10_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Because of this , as well as its formatting and usage of emoticons , this novel is similar to what emerged a decade later as the blog format .",
        "text": "Because of this , as well as its formatting and usage of emoticons , this novel is similar to what emerged a decade later as the blog format .",
        "mention_ids": []
      },
      "05": {
        "text": "■■■■■■■■ revisited many of the ideas in Microserfs in his 2006 novel JPod , which has been labeled \" Microserfs for the Google generation \" .",
        "mention_ids": [
          "M0_1",
          "M11_0",
          "M12_0",
          "M13_0",
          "M3_1"
        ]
      }
    },
