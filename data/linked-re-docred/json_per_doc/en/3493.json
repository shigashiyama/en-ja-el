    "sentences": {
      "00": {
        "orig_text": "Everard Burnside Butler ( December 28 , 1885 – November 23 , 1958 ) was a Canadian rower who won a bronze medal in the single sculls at the 1912 Summer Olympics .",
        "text": "Everard Burnside Butler ( December 28 , 1885 – November 23 , 1958 ) was a Canadian rower who won a bronze medal in the single sculls at the 1912 Summer Olympics .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "Butler started training in rowing in 1908 , and next year won his first junior race .",
        "text": "Butler started training in rowing in 1908 , and next year won his first junior race .",
        "mention_ids": [
          "M0_2",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "By 1910 he rowed as a senior in the United States and Canada , and in 1911 won two US national titles , in the single sculls and quarter - mile dash .",
        "text": "By 1910 he rowed as a senior in the United States and Canada , and in 1911 won two US national titles , in the single sculls and quarter - mile dash .",
        "mention_ids": [
          "M10_0",
          "M6_0",
          "M7_0",
          "M7_1",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "He defended those titles in 1912 , and won the quarter - mile dash again in 1914 .",
        "text": "He defended those titles in 1912 , and won the quarter - mile dash again in 1914 .",
        "mention_ids": [
          "M11_0",
          "M12_0"
        ]
      },
      "04": {
        "orig_text": "Butler fought in World War I with the 12th Artillery Brigade , and suffered extensive injuries in a mustard gas attack in France .",
        "text": "Butler fought in World War I with the 12th Artillery Brigade , and suffered extensive injuries in a mustard gas attack in France .",
        "mention_ids": [
          "M0_1",
          "M13_0",
          "M14_0",
          "M15_0"
        ]
      },
      "05": {
        "orig_text": "Consequently , after the war he retired from major rowing competitions and worked as an accountant .",
        "text": "Consequently , after the war he retired from major rowing competitions and worked as an accountant .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "He returned to the army during World War II and served with the 48th Highlanders and Royal Canadian Ordnance Corps .",
        "text": "He returned to the army during World War II and served with the 48th Highlanders and Royal Canadian Ordnance Corps .",
        "mention_ids": [
          "M16_0",
          "M17_0",
          "M18_0"
        ]
      }
