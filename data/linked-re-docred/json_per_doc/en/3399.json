    "sentences": {
      "00": {
        "orig_text": "The Prinzenpalais is a palace , now used as an art museum , in the city of Oldenburg , Lower Saxony , Germany .",
        "text": "The Prinzenpalais is a palace , now used as an art museum , in the city of Oldenburg , Lower Saxony , Germany .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "The museum houses the modern art collection of the State Museum for Art and Cultural History .",
        "text": "The museum houses the modern art collection of the State Museum for Art and Cultural History .",
        "mention_ids": [
          "M4_0"
        ]
      },
      "02": {
        "orig_text": "The building dates from 1826 and is in the classical style .",
        "text": "The building dates from 1826 and is in the classical style .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "03": {
        "text": "It was the residence of the Russian princes Alexander and ■■■■■ .",
        "mention_ids": [
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "Subsequently Grand Duke Niklaus Friedrich Peter occupied the building .",
        "text": "Subsequently Grand Duke Niklaus Friedrich Peter occupied the building .",
        "mention_ids": [
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "In 2003 , it became part of the State Museum of Art and Cultural History ( with the Augusteum and Schloss Oldenburg ) and is an art gallery .",
        "text": "In 2003 , it became part of the State Museum of Art and Cultural History ( with the Augusteum and Schloss Oldenburg ) and is an art gallery .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "The museum concentrates on German artists , ranging from neoclassicism and Romanticism in the mid-19th century to the post-1945 era .",
        "text": "The museum concentrates on German artists , ranging from neoclassicism and Romanticism in the mid-19th century to the post-1945 era .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0",
          "M3_1"
        ]
      },
      "07": {
        "orig_text": "The Prinzenpalais building is near the northeast corner of the Schlossgarten Oldenburg .",
        "text": "The Prinzenpalais building is near the northeast corner of the Schlossgarten Oldenburg .",
        "mention_ids": [
          "M0_1",
          "M17_0"
        ]
      },
      "08": {
        "orig_text": "The Augusteum , Elisabeth - Anna - Palais , and Schloss Oldenburg are all close to the museum .",
        "text": "The Augusteum , Elisabeth - Anna - Palais , and Schloss Oldenburg are all close to the museum .",
        "mention_ids": [
          "M12_1",
          "M13_1",
          "M18_0"
        ]
      }
    },
