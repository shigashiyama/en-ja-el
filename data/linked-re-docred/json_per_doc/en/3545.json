    "sentences": {
      "00": {
        "orig_text": "Allen County is a county in the U.S. state of Ohio .",
        "text": "Allen County is a county in the U.S. state of Ohio .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "As of the 2010 census , the population was 106,331 .",
        "text": "As of the 2010 census , the population was 106,331 .",
        "mention_ids": [
          "M3_0",
          "M4_0"
        ]
      },
      "02": {
        "orig_text": "The county seat is Lima .",
        "text": "The county seat is Lima .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "The county was created in 1820 and organized in 1831 .",
        "text": "The county was created in 1820 and organized in 1831 .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "The county is named for Colonel John Allen , who was killed leading his men at the Battle of Frenchtown , during the War of 1812 .",
        "text": "The county is named for Colonel John Allen , who was killed leading his men at the Battle of Frenchtown , during the War of 1812 .",
        "mention_ids": [
          "M10_0",
          "M8_0",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "It has also been claimed the county was named for Revolutionary War soldier Ethan Allen , but the weight of the evidence in favor of John Allen led the General Assembly to declare in 1976 that the county was named for him .",
        "text": "It has also been claimed the county was named for Revolutionary War soldier Ethan Allen , but the weight of the evidence in favor of John Allen led the General Assembly to declare in 1976 that the county was named for him .",
        "mention_ids": [
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0",
          "M8_1"
        ]
      },
      "06": {
        "orig_text": "Allen County comprises the Lima , OH Metropolitan Statistical Area , which is also part of the Lima - Van Wert - Wapakoneta , OH Combined Statistical Area .",
