    "sentences": {
      "00": {
        "orig_text": "The Chapel of St. Francis of Assisi ( ) is a chapel of the Roman Catholic Church located on the Esperanza Base administered by Argentina , on the northern tip of the Antarctic Peninsula in Antarctica .",
        "text": "The Chapel of St. Francis of Assisi ( ) is a chapel of the Roman Catholic Church located on the Esperanza Base administered by Argentina , on the northern tip of the Antarctic Peninsula in Antarctica .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "It is one of eight churches on Antarctica .",
        "text": "It is one of eight churches on Antarctica .",
        "mention_ids": [
          "M5_1",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "Founded on February 18 , 1976 , and installed by the Argentine Army , it was the first Catholic chapel of the Antarctic continent .",
        "text": "Founded on February 18 , 1976 , and installed by the Argentine Army , it was the first Catholic chapel of the Antarctic continent .",
        "mention_ids": [
          "M10_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The chapel provides spiritual support to the residents and staff of the base .",
        "text": "The chapel provides spiritual support to the residents and staff of the base .",
        "mention_ids": []
      },
      "04": {
        "text": "Its first priest was the Jesuit ■■■■■■■■■■■■■■■■■■■■■■■■ , born in Italy .",
        "mention_ids": [
          "M11_0",
          "M12_0"
        ]
      },
      "05": {
        "text": "This site has been part of several Antarctic milestones , including the first religious marriage , the baptism of ■■■■■■■■■■■■ , who was the first recorded birth in Antarctica , and first communion ceremony .",
        "mention_ids": [
          "M10_1",
          "M13_0",
          "M5_2"
        ]
      }
