    "sentences": {
      "00": {
        "orig_text": "The Waterloo Moraine is a landform and sediment body that was created as a moraine in the Regional Municipality of Waterloo , in Ontario , Canada .",
        "text": "The Waterloo Moraine is a landform and sediment body that was created as a moraine in the Regional Municipality of Waterloo , in Ontario , Canada .",
        "mention_ids": [
          "M0_1",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It covers a large portion of the cities of Waterloo and Kitchener and the township of Wilmot , and some parts of the townships of Wellesley and North Dumfries .",
        "text": "It covers a large portion of the cities of Waterloo and Kitchener and the township of Wilmot , and some parts of the townships of Wellesley and North Dumfries .",
        "mention_ids": [
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "02": {
        "orig_text": "About 90 % of the water supply of the Regional Municipality of Waterloo is derived from groundwater of the Waterloo Moraine aquifer system .",
        "text": "About 90 % of the water supply of the Regional Municipality of Waterloo is derived from groundwater of the Waterloo Moraine aquifer system .",
        "mention_ids": [
          "M0_0",
          "M1_1",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "During late 1989 and early 1990 , groundwater contamination in Elmira forced the Region to shut down some well fields .",
        "text": "During late 1989 and early 1990 , groundwater contamination in Elmira forced the Region to shut down some well fields .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0"
        ]
      },
      "04": {
        "orig_text": "As a result , new land use management guidelines and water protection measures have been enacted .",
        "text": "As a result , new land use management guidelines and water protection measures have been enacted .",
        "mention_ids": []
      },
      "05": {
        "orig_text": "The Waterloo Moraine is the largest of fourteen moraines in the Region , spanning approximately 400 square kilometres .",
        "text": "The Waterloo Moraine is the largest of fourteen moraines in the Region , spanning approximately 400 square kilometres .",
        "mention_ids": [
          "M0_2",
          "M13_0"
        ]
      },
      "06": {
        "orig_text": "It is an interlobate moraine , consisting primarily of sand and gravel .",
        "text": "It is an interlobate moraine , consisting primarily of sand and gravel .",
        "mention_ids": []
      },
      "07": {
        "orig_text": "It contains large aquifers , which discharge into the Grand River and its tributaries and maintain a base water flow rate into that system .",
        "text": "It contains large aquifers , which discharge into the Grand River and its tributaries and maintain a base water flow rate into that system .",
        "mention_ids": [
          "M14_0"
        ]
      }
    },
