    "sentences": {
      "00": {
        "orig_text": "Maggie Simpson in \" The Longest Daycare \" , or simply The Longest Daycare , is a 2012 American traditionally animated 3D comedy short film based on the animated television series The Simpsons .",
        "text": "Maggie Simpson in \" The Longest Daycare \" , or simply The Longest Daycare , is a 2012 American traditionally animated 3D comedy short film based on the animated television series The Simpsons .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M1_0",
          "M2_1",
          "M3_0"
        ]
      },
      "01": {
        "text": "In the film , ■■■■■■■■■■■■■■ is enrolled at a new daycare facility where she squares off with the foul - tempered ■■■■■■■■■■■ when she befriends a caterpillar .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "text": "The short originated with Simpsons producer ■■■■■■■■■■■■■■■ , who enlisted long - time veteran of the series , ■■■■■■■■■■■■■■■ , to direct the film .",
        "mention_ids": [
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "03": {
        "text": "The picture was written by producers ■■■■■■ , ■■■■■■■ , ■■■■■■■■■■■■ , writers ■■■■■■■■■■■■■ and ■■■■■■■■■■■■■ , as well as show creator ■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The events of the short film take place between the twenty - third and twenty - fourth seasons of The Simpsons .",
        "text": "The events of the short film take place between the twenty - third and twenty - fourth seasons of The Simpsons .",
        "mention_ids": [
          "M3_1"
        ]
      },
      "05": {
        "orig_text": "The film premiered on July 13 , 2012 , where it was attached to screenings of the 20th Century Fox release .",
        "text": "The film premiered on July 13 , 2012 , where it was attached to screenings of the 20th Century Fox release .",
        "mention_ids": [
          "M15_0",
          "M16_0"
        ]
      },
      "06": {
        "orig_text": "The film is the second Simpsons theatrical release .",
        "text": "The film is the second Simpsons theatrical release .",
        "mention_ids": [
          "M6_1"
        ]
      },
      "07": {
        "orig_text": "The short was re - released on February 15 , 2013 and played before the film Life of Pi in selected theaters in USA .",
        "text": "The short was re - released on February 15 , 2013 and played before the film Life of Pi in selected theaters in USA .",
        "mention_ids": [
          "M17_0",
          "M18_0",
          "M2_0"
        ]
      },
      "08": {
        "orig_text": "Reception has been positive , praising the storytelling and animation .",
        "text": "Reception has been positive , praising the storytelling and animation .",
        "mention_ids": []
      },
      "09": {
        "orig_text": "The film was nominated for an Academy Award for Best Animated Short Film in 2013 , losing to Paperman .",
        "text": "The film was nominated for an Academy Award for Best Animated Short Film in 2013 , losing to Paperman .",
