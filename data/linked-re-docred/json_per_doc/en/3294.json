      "title": "UNESCO Confucius Prize for Literacy",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "The UNESCO Confucius Prize for Literacy recognizes the activities of outstanding individuals , governments or governmental agencies and non - governmental organizations ( NGOs ) working in literacy serving rural adults and out - of - school youth , particularly women and girls .",
        "text": "The UNESCO Confucius Prize for Literacy recognizes the activities of outstanding individuals , governments or governmental agencies and non - governmental organizations ( NGOs ) working in literacy serving rural adults and out - of - school youth , particularly women and girls .",
        "mention_ids": [
          "M0_1"
        ]
      },
      "01": {
        "orig_text": "The Prize was established in 2005 through the support of the Government of the People 's Republic of China in honour of the great Chinese scholar Confucius .",
        "text": "The Prize was established in 2005 through the support of the Government of the People 's Republic of China in honour of the great Chinese scholar Confucius .",
        "mention_ids": [
          "M0_4",
          "M1_0",
          "M2_0",
          "M2_2",
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "It is part of the International Literacy Prizes , which UNESCO awards every year in recognition of excellence and inspiring experiences in the field of literacy throughout the world .",
        "text": "It is part of the International Literacy Prizes , which UNESCO awards every year in recognition of excellence and inspiring experiences in the field of literacy throughout the world .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "The Confucius Prize offers two awards of US$ 20,000 each , a medal and a diploma , as well as a study visit to literacy project sites in China .",
        "text": "The Confucius Prize offers two awards of US$ 20,000 each , a medal and a diploma , as well as a study visit to literacy project sites in China .",
        "mention_ids": [
          "M0_0",
          "M2_1",
          "M6_0",
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "The Prize is open to institutions , organizations or individuals displaying outstanding merit in literacy , achieving particularly effective results and promoting innovative approaches .",
        "text": "The Prize is open to institutions , organizations or individuals displaying outstanding merit in literacy , achieving particularly effective results and promoting innovative approaches .",
        "mention_ids": [
          "M0_3"
        ]
      },
      "05": {
        "orig_text": "The selection of prizewinners is made by an International Jury appointed by UNESCO ’s Director - General , which meets in Paris once a year .",
        "text": "The selection of prizewinners is made by an International Jury appointed by UNESCO ’s Director - General , which meets in Paris once a year .",
        "mention_ids": [
          "M5_1",
          "M8_0",
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "The Prize is awarded at an official ceremony held for that purpose at UNESCO Headquarters in Paris on the occasion of International Literacy Day ( 8 September ) .",
        "text": "The Prize is awarded at an official ceremony held for that purpose at UNESCO Headquarters in Paris on the occasion of International Literacy Day ( 8 September ) .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M11_0",
          "M12_0",
          "M9_1"
        ]
      }
