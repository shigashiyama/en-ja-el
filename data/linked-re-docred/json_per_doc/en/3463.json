    "sentences": {
      "00": {
        "orig_text": "\" Portugal no coração \" ( \" Portugal in my heart \" ) was the Portuguese entry in the Eurovision Song Contest 1977 , performed in Portuguese by Os Amigos .",
        "text": "\" Portugal no coração \" ( \" Portugal in my heart \" ) was the Portuguese entry in the Eurovision Song Contest 1977 , performed in Portuguese by Os Amigos .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_1",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "The song is a ballad in praise of Portugal itself .",
        "text": "The song is a ballad in praise of Portugal itself .",
        "mention_ids": [
          "M2_4"
        ]
      },
      "02": {
        "orig_text": "The song is about freedom , brotherhood and hope .",
        "text": "The song is about freedom , brotherhood and hope .",
        "mention_ids": []
      },
      "03": {
        "orig_text": "Many of the verses of the lyrics are acclamations of the end of the Estado Novo dictatorship that lasted for 48 years , and the end of the wars that the Portuguese people had been forced to fight on the African colonies by that regime .",
        "text": "Many of the verses of the lyrics are acclamations of the end of the Estado Novo dictatorship that lasted for 48 years , and the end of the wars that the Portuguese people had been forced to fight on the African colonies by that regime .",
        "mention_ids": [
          "M2_2",
          "M6_0",
          "M7_0",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "It 's also a love hymn from Portuguese people to their ex - colonies people , whom they had fought .",
        "text": "It 's also a love hymn from Portuguese people to their ex - colonies people , whom they had fought .",
        "mention_ids": [
          "M2_3"
        ]
      },
      "05": {
        "text": "The song was performed eighth on the night , following Luxembourg 's ■■■■■■■■■■■■■■■ with \" Frère Jacques \" and preceding the United Kingdom 's Lynsey de Paul & ■■■■■■■■■■ with \" Rock Bottom \" .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0",
          "M14_0",
          "M15_0",
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "At the close of voting , it had received 18 points , placing 14th in a field of 18 .",
        "text": "At the close of voting , it had received 18 points , placing 14th in a field of 18 .",
        "mention_ids": [
          "M16_0",
          "M16_1"
        ]
      },
      "07": {
        "orig_text": "It was succeeded as Portuguese representative at the 1978 Contest by Gemini with \" Dai li dou \" .",
        "text": "It was succeeded as Portuguese representative at the 1978 Contest by Gemini with \" Dai li dou \" .",
        "mention_ids": [
          "M17_0",
          "M18_0",
          "M19_0",
          "M2_0"
        ]
      }
