    "sentences": {
      "00": {
        "orig_text": "Anna Margaret Frances Caselberg ( née Woollaston , 1942 – 2004 ) was a New Zealand painter .",
        "text": "Anna Margaret Frances Caselberg ( née Woollaston , 1942 – 2004 ) was a New Zealand painter .",
        "mention_ids": [
          "M0_0",
          "M0_5",
          "M1_1",
          "M2_1",
          "M3_0"
        ]
      },
      "01": {
        "text": "Born in 1942 , Caselberg was the daughter of ■■■■■■■■■■■■■■■■■■■■■■■■■ ( ■■■■■■■■■■■■■ ) and the painter Toss Woollaston .",
        "mention_ids": [
          "M0_3",
          "M1_0",
          "M4_0",
          "M4_1",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "She studied at the University of Auckland , spending a year living with Colin McCahon and his family during this time .",
        "text": "She studied at the University of Auckland , spending a year living with Colin McCahon and his family during this time .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "In 1960 she married poet John Caselberg , who—12 years older than her — was friends with both her father and McCahon .",
        "text": "In 1960 she married poet John Caselberg , who—12 years older than her — was friends with both her father and McCahon .",
        "mention_ids": [
          "M7_1",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Anna Caselberg worked in oils and watercolour , mostly painting landscapes , and her style is said to show the influences of Colin McCahon and her father .",
        "text": "Anna Caselberg worked in oils and watercolour , mostly painting landscapes , and her style is said to show the influences of Colin McCahon and her father .",
        "mention_ids": [
          "M0_1",
          "M7_2"
        ]
      },
      "05": {
        "orig_text": "Her work is held in public collections in New Zealand , including those of the Museum of New Zealand Te Papa Tongarewa and the Dunedin Public Art Gallery .",
        "text": "Her work is held in public collections in New Zealand , including those of the Museum of New Zealand Te Papa Tongarewa and the Dunedin Public Art Gallery .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M3_1"
        ]
      },
      "06": {
        "orig_text": "Caselberg died of cancer in late 2004 , six months after her husband 's death , also from cancer .",
        "text": "Caselberg died of cancer in late 2004 , six months after her husband 's death , also from cancer .",
        "mention_ids": [
          "M0_4",
          "M12_0",
          "M2_0"
        ]
      },
      "07": {
        "orig_text": "The Caselberg Trust , a charitable trust supporting artists , is named in honour of John and Anna Caselberg .",
        "text": "The Caselberg Trust , a charitable trust supporting artists , is named in honour of John and Anna Caselberg .",
        "mention_ids": [
          "M0_2",
          "M13_0",
          "M9_1"
        ]
      }
