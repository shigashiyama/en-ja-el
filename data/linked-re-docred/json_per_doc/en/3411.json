    "sentences": {
      "00": {
        "orig_text": "\" Charles Guiteau \" ( LAWS E11 ) Roud 444 is a traditional song about the assassination of US President James A. Garfield by Charles J. Guiteau .",
        "text": "\" Charles Guiteau \" ( LAWS E11 ) Roud 444 is a traditional song about the assassination of US President James A. Garfield by Charles J. Guiteau .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "It is based on another old ballad , \" James A. Rogers \" .",
        "text": "It is based on another old ballad , \" James A. Rogers \" .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "The song is told from the point of view of the assassin himself .",
        "text": "The song is told from the point of view of the assassin himself .",
        "mention_ids": []
      },
      "03": {
        "orig_text": "For a while , it was believed that Guiteau wrote the song himself , possibly because of the poem \" I am Going to the Lordy \" , which Guiteau actually did write on the day of his execution .",
        "text": "For a while , it was believed that Guiteau wrote the song himself , possibly because of the poem \" I am Going to the Lordy \" , which Guiteau actually did write on the day of his execution .",
        "mention_ids": [
          "M5_1",
          "M5_2",
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "It is not to be confused with another ballad about the assassination , \" Mr. Garfield , \" which was popularized by Johnny Cash .",
        "text": "It is not to be confused with another ballad about the assassination , \" Mr. Garfield , \" which was popularized by Johnny Cash .",
        "mention_ids": [
          "M8_0",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "Bascom Lamar Lunsford recorded both songs in 1949 for the Library of Congress .",
        "text": "Bascom Lamar Lunsford recorded both songs in 1949 for the Library of Congress .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0"
        ]
      }
