    "sentences": {
      "00": {
        "orig_text": "Matthew de Glendonwyn ( died 10 May 1408 ) was a late 14th and early 15th century bishop of Glasgow .",
        "text": "Matthew de Glendonwyn ( died 10 May 1408 ) was a late 14th and early 15th century bishop of Glasgow .",
        "mention_ids": [
          "M0_3",
          "M1_1",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "He was elected to the see after the death of Cardinal Walter Wardlaw , his predecessor as bishop .",
        "text": "He was elected to the see after the death of Cardinal Walter Wardlaw , his predecessor as bishop .",
        "mention_ids": [
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "He was elected sometime between Wardlaw 's death in September and Matthew 's first appearance as bishop - elect in December .",
        "text": "He was elected sometime between Wardlaw 's death in September and Matthew 's first appearance as bishop - elect in December .",
        "mention_ids": [
          "M0_0",
          "M5_1",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "Matthew was consecrated some months after his election , either in late 1387 or early 1388 .",
        "text": "Matthew was consecrated some months after his election , either in late 1387 or early 1388 .",
        "mention_ids": [
          "M0_1",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "Matthew was a witness to charters of kings Robert II and Robert III , an occasional ambassador of the Scottish crown to England , and a frequent arbiter in disputes concerning various religious establishments .",
        "text": "Matthew was a witness to charters of kings Robert II and Robert III , an occasional ambassador of the Scottish crown to England , and a frequent arbiter in disputes concerning various religious establishments .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M11_0",
          "M12_0",
          "M13_0"
        ]
      },
      "05-06": {
        "orig_text": "On 21 May 1401 , he introduced a tax in his diocese to improve the deficient ornamenta of the diocese ( i.e. chasubles , copes , dalmatics , etc . ) .",
        "text": "On 21 May 1401 , he introduced a tax in his diocese to improve the deficient ornamenta of the diocese ( i.e. chasubles , copes , dalmatics , etc . ) .",
        "mention_ids": [
          "M14_0"
        ]
      },
      "07": {
        "orig_text": "According to the Martyrology of Glasgow , he died on 10 May 1408 .",
        "text": "According to the Martyrology of Glasgow , he died on 10 May 1408 .",
        "mention_ids": [
          "M15_0",
          "M1_0"
        ]
      }
    },
