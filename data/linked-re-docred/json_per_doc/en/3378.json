    "sentences": {
      "00": {
        "text": "\" Soldier \" is a song by American recording artist ■■■■■■■■■■■■ , taken from his fourth studio album , Sweeter .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It was released into the iTunes Store on September 6 , 2011 as a promotional single .",
        "text": "It was released into the iTunes Store on September 6 , 2011 as a promotional single .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "It was released as the third and final single from the album in the United States , on September 24 , 2012 .",
        "text": "It was released as the third and final single from the album in the United States , on September 24 , 2012 .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "text": "The song was written by ■■■■■■ and produced by ■■■■■■■■■■■■ .",
        "mention_ids": [
          "M2_2",
          "M8_0"
        ]
      },
      "04": {
        "text": "The song finds ■■■■■■ promising his girl everlasting love , and when she needs someone he 'll always be there fighting for her .",
        "mention_ids": [
          "M2_3"
        ]
      },
      "05": {
        "orig_text": "The song has charted inside the top - forty on the Dutch chart and has also charted on the UK Singles Chart and the Adult Pop Songs chart .",
        "text": "The song has charted inside the top - forty on the Dutch chart and has also charted on the UK Singles Chart and the Adult Pop Songs chart .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M9_0"
        ]
      },
      "06": {
        "text": "■■■■■■ appeared on the last episode of One Tree Hill and performed the track .",
        "mention_ids": [
          "M13_0",
          "M2_1"
        ]
      }
    },
