    "sentences": {
      "00": {
        "orig_text": "Northern Territory Force was an Australian Army force responsible for protecting the Northern Territory during World War II .",
        "text": "Northern Territory Force was an Australian Army force responsible for protecting the Northern Territory during World War II .",
        "mention_ids": [
          "M0_0",
          "M1_1",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "Most units assigned to the Northern Territory Force were based near Darwin and were responsible for defending the important naval and air bases in and around the town against a feared Japanese invasion .",
        "text": "Most units assigned to the Northern Territory Force were based near Darwin and were responsible for defending the important naval and air bases in and around the town against a feared Japanese invasion .",
        "mention_ids": [
          "M0_1",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "Northern Territory Force was briefly re - named the 12th Division in late 1942 but this was short - lived .",
        "text": "Northern Territory Force was briefly re - named the 12th Division in late 1942 but this was short - lived .",
        "mention_ids": [
          "M0_2",
          "M0_4",
          "M6_1"
        ]
      },
      "03": {
        "orig_text": "Australian Army units were rotated through northern Australia during the war and six infantry brigades served as part of Northern Territory Force between 1942 and 1945 .",
        "text": "Australian Army units were rotated through northern Australia during the war and six infantry brigades served as part of Northern Territory Force between 1942 and 1945 .",
        "mention_ids": [
          "M0_3",
          "M1_0",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "orig_text": "The formation was reduced over the course of the war as the strategic situation in the Pacific turned in the Allies ' favour , although remnants remained until the end of the war .",
        "text": "The formation was reduced over the course of the war as the strategic situation in the Pacific turned in the Allies ' favour , although remnants remained until the end of the war .",
        "mention_ids": [
          "M10_0",
          "M11_0"
        ]
      },
      "05": {
        "orig_text": "In early 1946 , it was converted back to the 7th Military District .",
        "text": "In early 1946 , it was converted back to the 7th Military District .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      }
