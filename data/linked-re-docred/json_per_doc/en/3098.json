    "sentences": {
      "00": {
        "text": "Eclipse is the third novel in the Twilight Saga by ■■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "text": "It continues the story of Bella Swan and her vampire love , ■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M3_0",
          "M4_0"
        ]
      },
      "02": {
        "text": "The novel explores Bella 's compromise between her love for ■■■■■■ and her friendship with shape - shifter ■■■■■■■■■■■ , along with her dilemma of leaving her mortality behind in a terrorized atmosphere , a result of mysterious vampire attacks in Seattle .",
        "mention_ids": [
          "M3_1",
          "M4_1",
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "Eclipse is preceded by New Moon and followed by Breaking Dawn .",
        "text": "Eclipse is preceded by New Moon and followed by Breaking Dawn .",
        "mention_ids": [
          "M0_1",
          "M7_0",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "The book was released on August 7 , 2007 , with an initial print run of one million copies , and sold more than 150,000 copies in the first 24 hours alone .",
        "text": "The book was released on August 7 , 2007 , with an initial print run of one million copies , and sold more than 150,000 copies in the first 24 hours alone .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "Eclipse was the fourth bestselling book of 2008 , only behind Twilight , New Moon , and Breaking Dawn .",
        "text": "Eclipse was the fourth bestselling book of 2008 , only behind Twilight , New Moon , and Breaking Dawn .",
        "mention_ids": [
          "M0_2",
          "M13_0",
          "M14_0",
          "M7_1",
          "M8_1"
        ]
      },
      "06": {
        "orig_text": "A was released on June 30 , 2010 .",
        "text": "A was released on June 30 , 2010 .",
        "mention_ids": [
          "M15_0"
        ]
      },
      "07": {
        "orig_text": "Eclipse received generally positive reviews .",
        "text": "Eclipse received generally positive reviews .",
        "mention_ids": [
          "M0_3"
        ]
      },
      "08": {
        "orig_text": "Critics noted its exploration of more mature themes than those of its predecessors , while praising the novel 's love triangle and plotting .",
        "text": "Critics noted its exploration of more mature themes than those of its predecessors , while praising the novel 's love triangle and plotting .",
