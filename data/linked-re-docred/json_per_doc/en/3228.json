    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■■■■■■■■■■■■■■ ( ) ( born 13 March 1947 , in Moscow , Russia ) is a Russian career diplomat .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M3_4"
        ]
      },
      "01": {
        "text": "■■■■■■■ is a graduate of Moscow State Institute of International Relations ( MGIMO ) and was the Russian Ambassador to OSCE from 1996 to 1998 .",
        "mention_ids": [
          "M0_2",
          "M3_1",
          "M4_0",
          "M4_1",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "text": "He was appointed Ambassador of the Russian Federation to the United States in January 1999 , and he was released from his posting by the Russian President , ■■■■■■■■■■■■■■■ , on 2 June 2008 .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M3_2",
          "M3_6",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "text": "From June 2008 to May 2012 ■■■■■■■ was Deputy Chief of the Government Staff of the Russian Federation .",
        "mention_ids": [
          "M0_1",
          "M12_0",
          "M13_0",
          "M14_0",
          "M3_5"
        ]
      },
      "04": {
        "orig_text": "Since May 2012 he has been Aide to the President of the Russian Federation responsible for international affairs in the Presidential Administration .",
        "text": "Since May 2012 he has been Aide to the President of the Russian Federation responsible for international affairs in the Presidential Administration .",
        "mention_ids": [
          "M13_1",
          "M15_0",
          "M3_7"
        ]
      },
