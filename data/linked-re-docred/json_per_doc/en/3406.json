    "sentences": {
      "00": {
        "orig_text": "Robert Walter Moevs ( 2 December 1920 , in La Crosse , Wisconsin – 10 December 2007 ) was an American composer of contemporary classical music .",
        "text": "Robert Walter Moevs ( 2 December 1920 , in La Crosse , Wisconsin – 10 December 2007 ) was an American composer of contemporary classical music .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "He was known for his highly chromatic music .",
        "text": "He was known for his highly chromatic music .",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Moevs served in the United States Army Air Forces as a pilot during World War II .",
        "text": "Moevs served in the United States Army Air Forces as a pilot during World War II .",
        "mention_ids": [
          "M0_2",
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "orig_text": "He then received his degree from Harvard University .",
        "text": "He then received his degree from Harvard University .",
        "mention_ids": [
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "Moevs was a student of Walter Piston and Nadia Boulanger .",
        "text": "Moevs was a student of Walter Piston and Nadia Boulanger .",
        "mention_ids": [
          "M0_1",
          "M10_0",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "He taught at Harvard University and Rutgers University .",
        "text": "He taught at Harvard University and Rutgers University .",
        "mention_ids": [
          "M11_0",
          "M8_1"
        ]
      },
      "06": {
        "orig_text": "He received the Rome Prize and a Guggenheim Fellowship ( 1962 ) .",
        "text": "He received the Rome Prize and a Guggenheim Fellowship ( 1962 ) .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "07": {
        "orig_text": "In 1978 his Concerto Grosso was awarded the Stockhausen International Prize in Composition .",
        "text": "In 1978 his Concerto Grosso was awarded the Stockhausen International Prize in Composition .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      },
      "08": {
        "orig_text": "His music has been performed by the Cleveland Orchestra , the Boston Symphony Orchestra and the Symphony of the Air .",
        "text": "His music has been performed by the Cleveland Orchestra , the Boston Symphony Orchestra and the Symphony of the Air .",
        "mention_ids": [
          "M18_0",
          "M19_0",
          "M20_0"
        ]
      },
      "09": {
        "orig_text": "His papers , including unpublished scores and recordings , are held by the Rutgers Music Library .",
        "text": "His papers , including unpublished scores and recordings , are held by the Rutgers Music Library .",
        "mention_ids": [
          "M21_0"
        ]
      },
      "10": {
        "orig_text": "He died in Hillsborough , New Jersey .",
        "text": "He died in Hillsborough , New Jersey .",
        "mention_ids": [
          "M22_0",
          "M23_0"
        ]
      }
