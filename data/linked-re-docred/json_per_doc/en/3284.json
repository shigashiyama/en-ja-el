    "sentences": {
      "00": {
        "orig_text": "King Haakon Bay , or King Haakon Sound , is an inlet on the southern coast of the island of South Georgia .",
        "text": "King Haakon Bay , or King Haakon Sound , is an inlet on the southern coast of the island of South Georgia .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M1_0"
        ]
      },
      "01": {
        "orig_text": "The inlet is approximately long and wide .",
        "text": "The inlet is approximately long and wide .",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The inlet was named for King Haakon VII of Norway by Carl Anton Larsen the founder of Grytviken .",
        "text": "The inlet was named for King Haakon VII of Norway by Carl Anton Larsen the founder of Grytviken .",
        "mention_ids": [
          "M2_0",
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "03": {
        "orig_text": "Queen Maud Bay , named for his queen , is nearby .",
        "text": "Queen Maud Bay , named for his queen , is nearby .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "04": {
        "orig_text": "Cave Cove , which forms part of the bay , is best known as the landing place of Ernest Shackleton in May 1916 as he sought help for his shipwrecked crew marooned in the Antarctic with the Imperial Trans - Antarctic Expedition .",
        "text": "Cave Cove , which forms part of the bay , is best known as the landing place of Ernest Shackleton in May 1916 as he sought help for his shipwrecked crew marooned in the Antarctic with the Imperial Trans - Antarctic Expedition .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "05": {
        "orig_text": "They also camped at Peggotty Bluff in the bay .",
        "text": "They also camped at Peggotty Bluff in the bay .",
        "mention_ids": [
          "M12_0"
        ]
      },
      "06": {
        "orig_text": "Shackleton Gap , a mountain pass , connects King Haakon Bay to Possession Bay .",
        "text": "Shackleton Gap , a mountain pass , connects King Haakon Bay to Possession Bay .",
        "mention_ids": [
          "M0_2",
          "M13_0",
          "M14_0"
        ]
      }
    },
