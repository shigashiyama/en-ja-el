    "sentences": {
      "00": {
        "orig_text": "Watts Station is a train station built in 1904 in Watts , Los Angeles , California .",
        "text": "Watts Station is a train station built in 1904 in Watts , Los Angeles , California .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_0"
        ]
      },
      "01": {
        "orig_text": "It was one of the first buildings in Watts , and for many years , it was a major stop for the Pacific Electric Railway 's \" Red Car \" service between Los Angeles and Long Beach .",
        "text": "It was one of the first buildings in Watts , and for many years , it was a major stop for the Pacific Electric Railway 's \" Red Car \" service between Los Angeles and Long Beach .",
        "mention_ids": [
          "M2_1",
          "M3_1",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "orig_text": "It was the only structure that remained intact when stores along 103rd Street in Watts were burned in the 1965 Watts Riots .",
        "text": "It was the only structure that remained intact when stores along 103rd Street in Watts were burned in the 1965 Watts Riots .",
        "mention_ids": [
          "M10_0",
          "M2_2",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "Remaining untouched in the middle of the stretch of street that came to be known as \" Charcoal Alley \" , the station became a symbol of continuity , hope , and renewal for the Watts community .",
        "text": "Remaining untouched in the middle of the stretch of street that came to be known as \" Charcoal Alley \" , the station became a symbol of continuity , hope , and renewal for the Watts community .",
        "mention_ids": [
          "M11_0",
          "M2_3"
        ]
      },
      "04": {
        "orig_text": "It has since been declared a Historic - Cultural Monument and is listed on the National Register of Historic Places .",
        "text": "It has since been declared a Historic - Cultural Monument and is listed on the National Register of Historic Places .",
        "mention_ids": [
          "M12_0",
          "M13_0"
        ]
      }
    },
