    "sentences": {
      "00": {
        "text": "The Merchants of Bollywood is an Australian musical written and directed by ■■■■■■■■■■ .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "text": "The show is about the history of the Bollywood film industry , and it is named after ■■■■■■■■■■■■■■■■■■ and his grand daughter ■■■■■■■■■■■■■■■■■ , two notable Indian choreographers .",
        "mention_ids": [
          "M3_1",
          "M4_0",
          "M5_0",
          "M6_0",
          "M7_0"
        ]
      },
      "02": {
        "text": "The set and lighting design was by ■■■■■■■■■ and the costumes were designed by ■■■■■■■■■■■■■■■ and ■■■■■ .",
        "mention_ids": [
          "M10_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "text": "The musical has been described as \" an Indian version of the ■■■■■■■■■■■■ story \" and is choreographed by ■■■■■■■■■■■■■■■■■ .",
        "mention_ids": [
          "M11_0",
          "M5_1",
          "M7_1"
        ]
      },
      "04": {
        "orig_text": "It was the first ever Bollywood production to tour straight from Film City in Mumbai .",
        "text": "It was the first ever Bollywood production to tour straight from Film City in Mumbai .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M3_0"
        ]
      },
      "05": {
        "orig_text": "When the show reached Australia in February 2008 , there had been 400 performances seen by 500,000 patrons .",
        "text": "When the show reached Australia in February 2008 , there had been 400 performances seen by 500,000 patrons .",
        "mention_ids": [
          "M14_0",
          "M15_0",
          "M16_0",
          "M17_0"
        ]
      },
      "06": {
        "orig_text": "The show toured the United Kingdom , various parts of Europe , the United States , Australia , Canada and various parts of Asia .",
        "text": "The show toured the United Kingdom , various parts of Europe , the United States , Australia , Canada and various parts of Asia .",
        "mention_ids": [
          "M14_1",
