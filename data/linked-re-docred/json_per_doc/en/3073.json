      "02": {
        "orig_text": "The watershed of the creek has an area of .",
        "text": "The watershed of the creek has an area of 15.1 square miles (39 km2).",
        "mention_ids": []
      },
      "03": {
        "orig_text": "The creek has many unnamed tributaries and sub - tributaries ; all of them , as well as the main stem , are impaired .",
        "text": "The creek has many unnamed tributaries and sub - tributaries ; all of them , as well as the main stem , are impaired .",
        "mention_ids": []
      },
      "04": {
        "orig_text": "The cause of impairment in Little Mahantango Creek is sedimentation / siltation and the probable source of impairment is agriculture .",
        "text": "The cause of impairment in Little Mahantango Creek is sedimentation / siltation and the probable source of impairment is agriculture .",
        "mention_ids": [
          "M0_0"
        ]
      },
      "05": {
        "orig_text": "The main rock formations in the vicinity of Little Mahantango Creek include the Irish Valley Member , the Trimmers Rock Formation , the Duncannon Member , and the Sherman Creek Member .",
        "text": "The main rock formations in the vicinity of Little Mahantango Creek include the Irish Valley Member , the Trimmers Rock Formation , the Duncannon Member , and the Sherman Creek Member .",
        "mention_ids": [
          "M0_2",
          "M10_0",
          "M11_0",
          "M8_0",
          "M9_0"
        ]
      },
      "06": {
        "orig_text": "The main soils in the vicinity of the creek include the Leck Kill - Minersville - Calvin soil , the Berks - Weikert - Beddington soil , and the Hazelton - Dekalb - Buchnnan soil .",
        "text": "The main soils in the vicinity of the creek include the Leck Kill - Minersville - Calvin soil , the Berks - Weikert - Beddington soil , and the Hazelton - Dekalb - Buchnnan soil .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "07": {
        "orig_text": "The main land use near it is agricultural land , but forested land and developed land are also present .",
        "text": "The main land use near it is agricultural land , but forested land and developed land are also present .",
