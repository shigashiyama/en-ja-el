    "sentences": {
      "00": {
        "text": "■■■■■■■■■■■ ( born 1941 , United States ) , known by the pseudonym ■■■■■■■■■■■ , is a critic of Islam , a writer and the founder of the Center for the Study of Political Islam .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0",
          "M4_2",
          "M5_0"
        ]
      },
      "01": {
        "orig_text": "He is a former Tennessee State University physics professor .",
        "text": "He is a former Tennessee State University physics professor .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "He is listed by the Southern Poverty Law Center as part of a core group of 10 Anti - Muslim hard - liners .",
        "text": "He is listed by the Southern Poverty Law Center as part of a core group of 10 Anti - Muslim hard - liners .",
        "mention_ids": [
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "text": "■■■■■■ has said that his focus is on the political aspects of Islamic doctrine related to kafirs ( non - Muslims ) rather than on the beliefs of contemporary Muslims .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M12_0",
          "M12_1",
          "M3_1"
        ]
      },
      "04": {
        "orig_text": "His books are based on the premise that Islam as a religion and what he refers to as Political Islam should be clearly delineated .",
        "text": "His books are based on the premise that Islam as a religion and what he refers to as Political Islam should be clearly delineated .",
        "mention_ids": [
          "M13_1",
          "M4_3"
        ]
      },
      "05": {
        "orig_text": "Islam as a faith is an individual ’s private decision and it should be treated with respect and tolerance .",
        "text": "Islam as a faith is an individual ’s private decision and it should be treated with respect and tolerance .",
        "mention_ids": [
          "M4_1"
        ]
      },
      "06": {
        "text": "■■■■■■ defines \" political Islam , \" which he also calls Islamism , \" as a belief that Islam should control society and politics , not simply personal religious life . \"",
        "mention_ids": [
          "M13_0",
          "M14_0",
          "M3_2",
          "M4_0"
        ]
      }
    },
