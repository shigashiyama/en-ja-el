    "sentences": {
      "00": {
        "orig_text": "Gambier Island is an island located in Howe Sound near Vancouver , British Columbia .",
        "text": "Gambier Island is an island located in Howe Sound near Vancouver , British Columbia .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "It is about in size and is located about north of the Horseshoe Bay community and ferry terminal in westernmost West Vancouver .",
        "text": "It is about in size and is located about north of the Horseshoe Bay community and ferry terminal in westernmost West Vancouver .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "A rugged and sparsely populated island , it is far quieter than its neighbour Bowen Island , which is popular with day - trippers and summer vacationers .",
        "text": "A rugged and sparsely populated island , it is far quieter than its neighbour Bowen Island , which is popular with day - trippers and summer vacationers .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "Gambier Island is accessible only by B.C.",
        "text": "Gambier Island is accessible only by B.C.",
        "mention_ids": [
          "M0_1",
          "M7_0"
        ]
      },
      "04": {
        "orig_text": "Ferries passenger service , water taxi or other boats .",
        "text": "Ferries passenger service , water taxi or other boats .",
        "mention_ids": []
      },
      "05": {
        "orig_text": "There is no central road network .",
        "text": "There is no central road network .",
        "mention_ids": []
      },
      "06": {
        "orig_text": "The island elects two trustees to the Islands Trust , an organization that unites small island communities in British Columbia to oversee development and land use .",
        "text": "The island elects two trustees to the Islands Trust , an organization that unites small island communities in British Columbia to oversee development and land use .",
        "mention_ids": [
          "M3_1",
          "M8_0",
          "M9_0"
        ]
      },
      "07": {
        "orig_text": "Other islands in Howe Sound include Keats Island and Anvil Island .",
        "text": "Other islands in Howe Sound include Keats Island and Anvil Island .",
