    "sentences": {
      "00": {
        "orig_text": "Celtic Football Club is a Scottish professional association football club based in Glasgow .",
        "text": "Celtic Football Club is a Scottish professional association football club based in Glasgow .",
        "mention_ids": [
          "M0_5",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "They have played at their home ground , Celtic Park , since 1892 .",
        "text": "They have played at their home ground , Celtic Park , since 1892 .",
        "mention_ids": [
          "M3_0",
          "M4_0"
        ]
      },
      "02": {
        "orig_text": "Celtic were founding members of the Scottish Football League in 1890 , and the Scottish Premier League in 1998 as well as the Scottish Professional Football League in 2013 .",
        "text": "Celtic were founding members of the Scottish Football League in 1890 , and the Scottish Premier League in 1998 as well as the Scottish Professional Football League in 2013 .",
        "mention_ids": [
          "M0_0",
          "M10_0",
          "M5_0",
          "M6_0",
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "The list encompasses the major honours won by Celtic , records set by the club , their managers and their players .",
        "text": "The list encompasses the major honours won by Celtic , records set by the club , their managers and their players .",
        "mention_ids": [
          "M0_1"
        ]
      },
      "04": {
        "orig_text": "The player records section includes details of the club 's leading goalscorers and those who have made most appearances in first - team competitions .",
        "text": "The player records section includes details of the club 's leading goalscorers and those who have made most appearances in first - team competitions .",
        "mention_ids": []
      },
      "05": {
        "orig_text": "It also records notable achievements by Celtic players on the international stage , and the highest transfer fees paid and received by the club .",
        "text": "It also records notable achievements by Celtic players on the international stage , and the highest transfer fees paid and received by the club .",
        "mention_ids": [
          "M0_2"
        ]
      },
      "06": {
        "orig_text": "Attendance records at Celtic Park , and also at Hampden Park which has on occasion been used for home games , are also included .",
        "text": "Attendance records at Celtic Park , and also at Hampden Park which has on occasion been used for home games , are also included .",
        "mention_ids": [
          "M11_0",
          "M3_1"
        ]
      },
      "07": {
        "orig_text": "Celtic have won 49 top - flight titles , and hold the record for most Scottish Cup wins with 38 .",
        "text": "Celtic have won 49 top - flight titles , and hold the record for most Scottish Cup wins with 38 .",
        "mention_ids": [
          "M0_3",
          "M12_0",
          "M13_0",
          "M14_0"
        ]
      },
      "08": {
        "orig_text": "The club 's record appearance maker is Billy McNeill , who made 822 appearances between 1957 and 1975 .",
        "text": "The club 's record appearance maker is Billy McNeill , who made 822 appearances between 1957 and 1975 .",
        "mention_ids": [
          "M15_0",
          "M16_0",
          "M17_0",
          "M18_0"
        ]
      },
      "09": {
        "orig_text": "Jimmy McGrory is the club 's record goalscorer , scoring 522 goals during his Celtic career .",
        "text": "Jimmy McGrory is the club 's record goalscorer , scoring 522 goals during his Celtic career .",
        "mention_ids": [
          "M0_4",
          "M19_0",
          "M20_0"
        ]
      },
      "10": {
        "orig_text": "All figures are correct as of 20 September 2018 .",
        "text": "All figures are correct as of 20 September 2018 .",
