      "title": "Holmenkollen Ski Museum",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Holmenkollen Ski Museum ( ) is located at the base of the Holmenkollen ski jump in Oslo , Norway .",
        "text": "Holmenkollen Ski Museum ( ) is located at the base of the Holmenkollen ski jump in Oslo , Norway .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "The Ski Museum was founded in 1923 and is the world 's oldest ski museum .",
        "text": "The Ski Museum was founded in 1923 and is the world 's oldest ski museum .",
        "mention_ids": [
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "orig_text": "The initiator of the museum was the architect Hjalmar Welhaven .",
        "text": "The initiator of the museum was the architect Hjalmar Welhaven .",
        "mention_ids": [
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "His private collection formed the foundation of the antique and historic skis on display at the museum .",
        "text": "His private collection formed the foundation of the antique and historic skis on display at the museum .",
        "mention_ids": []
      },
      "04": {
        "orig_text": "Until 1951 , the collection was housed in a building in the Oslo neighborhood of Frognerseteren .",
        "text": "Until 1951 , the collection was housed in a building in the Oslo neighborhood of Frognerseteren .",
        "mention_ids": [
          "M2_1",
          "M7_0",
          "M8_0"
        ]
      },
      "05": {
        "orig_text": "In connection with the rebuilding of Holmenkollbakken in 1951 , the museum collection was moved to new premises at Holmenkollbakken .",
        "text": "In connection with the rebuilding of Holmenkollbakken in 1951 , the museum collection was moved to new premises at Holmenkollbakken .",
        "mention_ids": [
          "M7_1",
          "M9_0",
          "M9_1"
        ]
      },
      "06": {
        "orig_text": "The museum contains 4,000 years of skiing history , starting with rock carvings dating from the Stone Age .",
        "text": "The museum contains 4,000 years of skiing history , starting with rock carvings dating from the Stone Age .",
        "mention_ids": [
          "M10_0",
          "M11_0"
        ]
      },
      "07": {
        "orig_text": "It also displays skiers and skis from the Viking Age .",
        "text": "It also displays skiers and skis from the Viking Age .",
        "mention_ids": [
          "M12_0"
        ]
      },
      "08": {
        "orig_text": "Additionally it displays equipment used in the polar expeditions of both Fridtjof Nansen and Roald Amundsen .",
        "text": "Additionally it displays equipment used in the polar expeditions of both Fridtjof Nansen and Roald Amundsen .",
        "mention_ids": [
          "M13_0",
          "M14_0"
        ]
      },
      "09": {
        "orig_text": "The Ski Museum is owned and operated by Skiforeningen .",
        "text": "The Ski Museum is owned and operated by Skiforeningen .",
        "mention_ids": [
          "M15_0",
          "M4_1"
        ]
      },
      "10": {
        "orig_text": "This association is responsible for the organization of sports , activities and events in Holmenkollen National Park .",
        "text": "This association is responsible for the organization of sports , activities and events in Holmenkollen National Park .",
        "mention_ids": [
          "M16_0"
        ]
      }
    },
