    "sentences": {
      "00": {
        "orig_text": "The Dante Alighieri Society ( Italian : Società Dante Alighieri ) is a society that promotes Italian culture and language around the world .",
        "text": "The Dante Alighieri Society ( Italian : Società Dante Alighieri ) is a society that promotes Italian culture and language around the world .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "Today this society is present in more than 60 countries .",
        "text": "Today this society is present in more than 60 countries .",
        "mention_ids": [
          "M4_0"
        ]
      },
      "02": {
        "orig_text": "It was formed in Italy in July 1889 .",
        "text": "It was formed in Italy in July 1889 .",
        "mention_ids": [
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "The society was named after Dante Alighieri ( 1265–1321 ) , a pre - Renaissance poet from Florence and the author of The Divine Comedy .",
        "text": "The society was named after Dante Alighieri ( 1265–1321 ) , a pre - Renaissance poet from Florence and the author of The Divine Comedy .",
        "mention_ids": [
