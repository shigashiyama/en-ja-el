    "sentences": {
      "00": {
        "orig_text": "\" More \" is a song by The Sisters of Mercy , from their album Vision Thing .",
        "text": "\" More \" is a song by The Sisters of Mercy , from their album Vision Thing .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "It was the first single from the album , reaching number one on the Billboard Modern Rock Tracks chart for five weeks , starting 15 December 1990 .",
        "text": "It was the first single from the album , reaching number one on the Billboard Modern Rock Tracks chart for five weeks , starting 15 December 1990 .",
        "mention_ids": [
          "M3_0",
          "M4_0",
          "M5_0"
        ]
      },
      "02": {
        "text": "The song was co - written and co - produced by ■■■■■■■■■■■■■■■ and Jim Steinman .",
        "mention_ids": [
          "M6_0",
          "M7_0"
        ]
      },
      "03": {
        "text": "It was covered by ■■■■■■■ on their album Reason , and Gregorian for their album The Dark Side .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M8_0",
          "M9_0"
        ]
      },
      "04": {
        "text": "Steinman produced a cover of the song , by ■■■■■■■■■■ and ■■■■■■■■■■■■■■■■■ , for the soundtrack of the MTV film Wuthering Heights .",
        "mention_ids": [
          "M12_0",
          "M13_0",
          "M14_0",
          "M15_0",
          "M16_0"
        ]
      },
      "05": {
        "orig_text": "He also used the song 's main guitar riff and the \" I need all the love I can get \" vocal in a song for his musical Batman .",
        "text": "He also used the song 's main guitar riff and the \" I need all the love I can get \" vocal in a song for his musical Batman .",
        "mention_ids": [
          "M17_0"
        ]
      },
      "06": {
        "orig_text": "The song has also been re - recorded by Meat Loaf for his 2016 album Braver Than We Are .",
        "text": "The song has also been re - recorded by Meat Loaf for his 2016 album Braver Than We Are .",
        "mention_ids": [
          "M18_0",
          "M19_0",
          "M20_0"
        ]
      }
    },
