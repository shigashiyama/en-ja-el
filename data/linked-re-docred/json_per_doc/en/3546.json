    "sentences": {
      "00": {
        "orig_text": "Zarir ( also spelled Zarih ) was a Sasanian prince who was the leader of a rebellion in northern Iran in 485 .",
        "text": "Zarir ( also spelled Zarih ) was a Sasanian prince who was the leader of a rebellion in northern Iran in 485 .",
        "mention_ids": [
          "M0_0",
          "M0_1",
          "M1_0",
          "M2_0",
          "M3_0"
        ]
      },
      "01": {
        "orig_text": "According to Armenian historian Ghazar Parpetsi , who is the only source about the life of Zarir , the latter was a son of the Sasanian shah Yazdegerd II .",
        "text": "According to Armenian historian Ghazar Parpetsi , who is the only source about the life of Zarir , the latter was a son of the Sasanian shah Yazdegerd II .",
        "mention_ids": [
          "M0_5",
          "M1_1",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "02": {
        "orig_text": "He had several brothers named Balash , Hormizd III , and Peroz I.",
        "text": "He had several brothers named Balash , Hormizd III , and Peroz I.",
        "mention_ids": [
          "M7_0",
          "M8_0",
          "M9_0"
        ]
      },
      "03": {
        "orig_text": "After the death of Peroz I ( who had succeeded his father as king ) , Balash was elected as king by the nobility and clergy .",
        "text": "After the death of Peroz I ( who had succeeded his father as king ) , Balash was elected as king by the nobility and clergy .",
        "mention_ids": [
          "M10_0",
          "M7_1"
        ]
      },
      "04": {
        "orig_text": "Zarir , dissatisfied with the election , rebelled .",
        "text": "Zarir , dissatisfied with the election , rebelled .",
        "mention_ids": [
          "M0_2"
        ]
      },
      "05": {
        "orig_text": "Balash was thus forced to make peace with his enemy Vahan Mamikonian and sent him at the head of an army to suppress the rebellion of Zarir .",
        "text": "Balash was thus forced to make peace with his enemy Vahan Mamikonian and sent him at the head of an army to suppress the rebellion of Zarir .",
        "mention_ids": [
          "M0_3",
          "M11_0",
          "M7_2"
        ]
      },
      "06": {
        "orig_text": "Zarir was shortly defeated , and fled to the mountains , but was quickly captured and \" shot down like an animal \" .",
        "text": "Zarir was shortly defeated , and fled to the mountains , but was quickly captured and \" shot down like an animal \" .",
        "mention_ids": [
          "M0_4"
        ]
      }
    },
