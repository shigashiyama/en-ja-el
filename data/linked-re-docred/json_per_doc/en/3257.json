      "title": "List of Presidents of Ethiopia",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "This is a list of Presidents of Ethiopia and also a list of heads of state after the fall of the Ethiopian Empire in 1974 .",
        "text": "This is a list of Presidents of Ethiopia and also a list of heads of state after the fall of the Ethiopian Empire in 1974 .",
        "mention_ids": [
          "M0_0",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "Until 1974 , the heads of state of the Ethiopian Empire were either Emperors or regents .",
        "text": "Until 1974 , the heads of state of the Ethiopian Empire were either Emperors or regents .",
        "mention_ids": [
          "M1_1",
          "M2_1"
        ]
      },
      "02": {
        "text": "From the coup d'état of the Derg leading to the fall of the Empire in September 1974 until March 1975 , the Derg considered the Crown Prince ■■■■■■■■■■■■ as the nominal head of state – which the Crown Prince refused to accept .",
        "mention_ids": [
          "M1_2",
          "M2_2",
          "M3_0",
          "M3_5",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "During this time , the Chairmen of the Derg , the leaders of the Derg , were to be considered as acting heads of state .",
        "text": "During this time , the Chairmen of the Derg , the leaders of the Derg , were to be considered as acting heads of state .",
        "mention_ids": [
          "M3_6",
          "M3_7"
        ]
      },
      "04": {
        "orig_text": "After 21 March 1975 , the Derg military junta fully took over .",
        "text": "After 21 March 1975 , the Derg military junta fully took over .",
        "mention_ids": [
          "M3_4",
          "M4_1"
        ]
      },
      "05": {
        "orig_text": "Until the establishment of the People 's Democratic Republic of Ethiopia in 1987 , still dominated by Derg figures , Chairmen of the Derg have to be considered heads of state – but not presidents .",
        "text": "Until the establishment of the People 's Democratic Republic of Ethiopia in 1987 , still dominated by Derg figures , Chairmen of the Derg have to be considered heads of state – but not presidents .",
        "mention_ids": [
          "M3_2",
          "M3_3",
          "M7_0",
          "M8_0"
        ]
      },
      "06": {
        "orig_text": "After the fall of the Derg and the establishment of the Transitional Government of Ethiopia in 1991 , the first immediate President ( Meles Zenawi ) has to be considered an Interim President .",
        "text": "After the fall of the Derg and the establishment of the Transitional Government of Ethiopia in 1991 , the first immediate President ( Meles Zenawi ) has to be considered an Interim President .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M3_1",
          "M9_0"
        ]
      },
      "07": {
        "orig_text": "Since the formal establishment of the office of President in 1987 , there have been 6 official presidents .",
        "text": "Since the formal establishment of the office of President in 1987 , there have been 6 official presidents .",
        "mention_ids": [
          "M12_0",
          "M8_1"
        ]
      },
      "08": {
        "orig_text": "The President is the head of state of Ethiopia .",
        "text": "The President is the head of state of Ethiopia .",
        "mention_ids": [
          "M0_1"
        ]
      },
      "09": {
        "text": "The current president is ■■■■■■■■■■■■■■■■■■ , who is also the first female president of Ethiopia , elected on 25 October 2018 by members of the Federal Parliamentary Assembly .",
        "mention_ids": [
          "M0_2",
          "M13_0",
          "M14_0",
          "M15_0"
        ]
      }
