  "3543": {
    "doc_info": {
      "title": "Lee Hall Mansion",
      "url": null
    },
    "sentences": {
      "00": {
        "orig_text": "Lee Hall or Lee Hall Mansion is a historic brick plantation house that was built during the period from 1848 to 1859 .",
        "text": "Lee Hall or Lee Hall Mansion is a historic brick plantation house that was built during the period from 1848 to 1859 .",
        "mention_ids": [
          "M0_0",
          "M0_5",
          "M1_0",
          "M2_0"
        ]
      },
      "01": {
        "orig_text": "The community of Lee Hall , Virginia is named for it .",
        "text": "The community of Lee Hall , Virginia is named for it .",
        "mention_ids": [
          "M0_2",
          "M3_0"
        ]
      },
      "02": {
        "orig_text": "The house and village are located near the junction of U.S. 60 and VA 238 , in Newport News , Virginia .",
        "text": "The house and village are located near the junction of U.S. 60 and VA 238 , in Newport News , Virginia .",
        "mention_ids": [
          "M3_1",
          "M4_0",
          "M5_0",
          "M6_0"
        ]
      },
      "03": {
        "orig_text": "The current property was listed on the National Register of Historic Places in 1972 .",
        "text": "The current property was listed on the National Register of Historic Places in 1972 .",
        "mention_ids": [
          "M7_0",
          "M8_0"
        ]
      },
      "04": {
        "orig_text": "The listed property includes two contributing buildings : The Main House ( open to the public ) and the former Kitchen ( closed to public , used as staff offices ) .",
        "text": "The listed property includes two contributing buildings : The Main House ( open to the public ) and the former Kitchen ( closed to public , used as staff offices ) .",
        "mention_ids": [
          "M10_0",
          "M11_0",
          "M9_0"
        ]
      },
      "05": {
        "text": "The house as well as the nearby community of Lee Hall were named for ■■■■■■■■■■■■■■■■■■■ the original owner and builder of Lee Hall Mansion .",
        "mention_ids": [
          "M0_1",
          "M0_3",
          "M12_0"
        ]
      },
      "06": {
        "text": "■■■ was a prominent local planter .",
        "mention_ids": [
          "M13_0"
        ]
      },
      "07": {
        "text": "Despite having the same last name as Virginia native and Confederate general Robert E. Lee , a direct lineage to General ■■■ can not be traced .",
        "mention_ids": [
          "M13_1",
          "M14_0",
          "M3_2"
        ]
      },
      "08": {
        "orig_text": "Lee Hall Mansion was used as headquarters for Confederate generals Joseph E. Johnston and John B. Magruder during the Peninsula Campaign of the American Civil War in 1862 .",
        "text": "Lee Hall Mansion was used as headquarters for Confederate generals Joseph E. Johnston and John B. Magruder during the Peninsula Campaign of the American Civil War in 1862 .",
        "mention_ids": [
          "M0_4",
          "M15_0",
          "M16_0",
          "M17_0",
          "M18_0",
          "M19_0"
        ]
      },
      "09": {
        "orig_text": "Nearby is Endview Plantation , a 238-year - old house .",
        "text": "Nearby is Endview Plantation , a 238-year - old house .",
        "mention_ids": [
          "M20_0",
          "M21_0"
        ]
      },
      "10": {
        "orig_text": "Endview was used as a hospital during the Civil War and as a campground during the Revolutionary War , the War of 1812 and the Civil War .",
        "text": "Endview was used as a hospital during the Civil War and as a campground during the Revolutionary War , the War of 1812 and the Civil War .",
        "mention_ids": [
          "M18_1",
          "M18_2",
          "M20_1",
          "M22_0",
          "M23_0"
        ]
      }
    },
