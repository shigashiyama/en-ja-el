    "sentences": {
      "00": {
        "orig_text": "Beatles' Apple Corps sues Apple Computer",
        "text": "Beatles' Apple Corps sues Apple Computer",
        "mention_ids": [
          "M016",
          "M018"
        ]
      },
      "01": {
        "orig_text": "March 28, 2006",
        "text": "March 28, 2006",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The Beatles' Apple Corps has filed a lawsuit against computer and electronics maker Apple Computer in a London court this week, the third such lawsuit in a long running trademark dispute between the two companies.",
        "text": "The Beatles' Apple Corps has filed a lawsuit against computer and electronics maker Apple Computer in a London court this week, the third such lawsuit in a long running trademark dispute between the two companies.",
        "mention_ids": [
          "M002",
          "M010",
          "M012",
          "M022",
          "M029"
        ]
      },
      "03": {
        "orig_text": "Apple Corps claims that Apple Computer's iTunes Music Store violates an agreement reached between the two companies in 1991, which barred Apple Computer from using the ``Apple`` brand in certain uses in the music business.",
        "text": "Apple Corps claims that Apple Computer's iTunes Music Store violates an agreement reached between the two companies in 1991, which barred Apple Computer from using the ``Apple`` brand in certain uses in the music business.",
        "mention_ids": [
          "M003",
          "M023",
          "M026",
          "M028",
          "M030",
          "M031"
        ]
      },
      "04": {
        "orig_text": "The trial is set to begin on Wednesday in the Royal Courts of Justice, located in central London.",
        "text": "The trial is set to begin on Wednesday in the Royal Courts of Justice, located in central London.",
        "mention_ids": [
          "M004"
        ]
      },
      "05": {
        "orig_text": "Presiding Judge Martin Mann has stated that he owns an iPod portable music player, which is made by Apple Computer and marketed alongside the iTunes Music Store.",
        "text": "Presiding Judge Martin Mann has stated that he owns an iPod portable music player, which is made by Apple Computer and marketed alongside the iTunes Music Store.",
        "mention_ids": [
          "M006",
          "M008",
          "M033"
        ]
      },
      "06": {
        "orig_text": "Apple Corps has twice before sued Apple Computer over its use of the ``Apple`` name.",
        "text": "Apple Corps has twice before sued Apple Computer over its use of the ``Apple`` name.",
        "mention_ids": [
          "M178"
        ]
      },
      "07": {
        "orig_text": "The first lawsuit was settled out of court in 1981, with the young computer maker paying $80,000 and agreeing to stay out of the music business.",
        "text": "The first lawsuit was settled out of court in 1981, with the young computer maker paying $80,000 and agreeing to stay out of the music business.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "In the late 1980's Apple Computer added audio recording abilities to its Macintosh computers, prompting Apple Corps to file suit again in 1989.",
        "text": "In the late 1980's Apple Computer added audio recording abilities to its Macintosh computers, prompting Apple Corps to file suit again in 1989.",
        "mention_ids": [
          "M177"
        ]
      },
      "09": {
        "orig_text": "That lawsuit was settled in 1991, also out of court, with Apple paying $26.5 million.",
        "text": "That lawsuit was settled in 1991, also out of court, with Apple paying $26.5 million.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "The settlement included a more specific agreement over the boundaries between the two brands: Apple Computer was allowed to use its name to market ``goods or services...used to reproduce, run, play or otherwise deliver such [music] content,`` but barred from distributing music on a physical medium such as CD or cassette.",
        "text": "The settlement included a more specific agreement over the boundaries between the two brands: Apple Computer was allowed to use its name to market ``goods or services...used to reproduce, run, play or otherwise deliver such [music] content,`` but barred from distributing music on a physical medium such as CD or cassette.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "Apple Corps' latest suit, filed in September 2003, claims that the computer company violated the 1991 agreement with its iTunes Music Store, which sells digital music that can be downloaded to personal computers.",
        "text": "Apple Corps' latest suit, filed in September 2003, claims that the computer company violated the 1991 agreement with its iTunes Music Store, which sells digital music that can be downloaded to personal computers.",
        "mention_ids": [
          "M173",
          "M174",
          "M175"
        ]
      },
      "12": {
        "orig_text": "A statement from Apple Computer stated that ``Apple and Apple Corps now have differing interpretations of this agreement and will need to ask a court to resolve this dispute.``",
        "text": "A statement from Apple Computer stated that ``Apple and Apple Corps now have differing interpretations of this agreement and will need to ask a court to resolve this dispute.``",
        "mention_ids": [
          "M179"
        ]
      },
      "13": {
        "orig_text": "Some observers have suggested that the wording of the 1991 agreement, which did not explicitly bar digital music distribution, could be to Apple Computer's advantage.",
        "text": "Some observers have suggested that the wording of the 1991 agreement, which did not explicitly bar digital music distribution, could be to Apple Computer's advantage.",
        "mention_ids": []
      },
      "14": {
        "orig_text": "A similar suit was filed simultaneously in California, Apple Computer's home state, but on September 21, 2004 the parties agreed to have the case heard by the UK court.",
        "text": "A similar suit was filed simultaneously in California, Apple Computer's home state, but on September 21, 2004 the parties agreed to have the case heard by the UK court.",
        "mention_ids": []
      },
      "15": {
        "orig_text": "Beatles songs have not been licensed for digital download on any of the online music services.",
        "text": "Beatles songs have not been licensed for digital download on any of the online music services.",
        "mention_ids": []
      },
      "16": {
        "orig_text": "Apple Corps was founded by the Beatles in 1968.",
        "text": "Apple Corps was founded by the Beatles in 1968.",
        "mention_ids": [
          "M176"
        ]
      },
      "17": {
        "orig_text": "Apple Computer was started by Steve Jobs and Steve Wozniak in 1976, and launched the iTunes Music Store in 2003.",
        "text": "Apple Computer was started by Steve Jobs and Steve Wozniak in 1976, and launched the iTunes Music Store in 2003.",
        "mention_ids": [
          "M180",
          "M181"
        ]
      }
    },
