    },
    "sentences": {
      "00": {
        "orig_text": "Airbus wins Qatar Airways order worth $15bn",
        "text": "Airbus wins Qatar Airways order worth $15bn",
        "mention_ids": [
          "M007",
          "M011"
        ]
      },
      "01": {
        "orig_text": "June 13, 2005",
        "text": "June 13, 2005",
        "mention_ids": []
      },
      "02": {
        "orig_text": "European aircraft manufacturer, Airbus, has won an order from Qatar Airways for up to 60 of its new mid-sized A350 jets.",
        "text": "European aircraft manufacturer, Airbus, has won an order from Qatar Airways for up to 60 of its new mid-sized A350 jets.",
        "mention_ids": [
          "M001",
          "M021",
          "M022"
        ]
      },
      "03": {
        "orig_text": "The order, which is believed to be worth $15 billion, has given a boost to an otherwise disappointing Paris Air Show, which had been dominated by a state aid dispute between Airbus and Boeing.",
        "text": "The order, which is believed to be worth $15 billion, has given a boost to an otherwise disappointing Paris Air Show, which had been dominated by a state aid dispute between Airbus and Boeing.",
        "mention_ids": [
          "M003",
          "M005"
        ]
      },
      "04": {
        "orig_text": "Qatar Airways, which choose the Airbus A350 over Boeing's 787, will also purchase 20 of Boeing's larger 777 planes for $4.6 billion.",
        "text": "Qatar Airways, which choose the Airbus A350 over Boeing's 787, will also purchase 20 of Boeing's larger 777 planes for $4.6 billion.",
        "mention_ids": [
          "M006",
          "M008",
          "M009",
          "M010",
          "M014"
        ]
      },
      "05": {
        "orig_text": "Qatar has a previous order for 2 of Airbus' massive new A380's which it will begin to fly in 2009 out of London Heathrow.",
        "text": "Qatar has a previous order for 2 of Airbus' massive new A380's which it will begin to fly in 2009 out of London Heathrow.",
        "mention_ids": [
          "M002",
          "M004",
          "M016",
          "M019",
          "M020"
        ]
      },
      "06": {
        "orig_text": "In 2004, Airbus sold more planes than Boeing for the first time ever, sparking questions whether the US company had lost its once seemingly impregnable grip on the aviation market.",
        "text": "In 2004, Airbus sold more planes than Boeing for the first time ever, sparking questions whether the US company had lost its once seemingly impregnable grip on the aviation market.",
        "mention_ids": [
          "M089",
          "M095",
          "M128",
          "M153"
        ]
      },
      "07": {
        "orig_text": "However thus far, Boeing has received far more orders than Airbus in 2005.",
        "text": "However thus far, Boeing has received far more orders than Airbus in 2005.",
        "mention_ids": [
          "M100",
          "M116"
        ]
      }
    },
