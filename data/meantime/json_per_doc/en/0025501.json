    "sentences": {
      "00": {
        "orig_text": "Engine troubles delay Airbus superjumbo tour",
        "text": "Engine troubles delay Airbus superjumbo tour",
        "mention_ids": [
          "M007",
          "M009"
        ]
      },
      "01": {
        "orig_text": "November 05, 2005",
        "text": "November 05, 2005",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The departure from France of the new Airbus A380 superjumbo airliner on a tour of Asia and Australia has been delayed, leading to a rearrangement of its public appearances.",
        "text": "The departure from France of the new Airbus A380 superjumbo airliner on a tour of Asia and Australia has been delayed, leading to a rearrangement of its public appearances.",
        "mention_ids": [
          "M016",
          "M017",
          "M114"
        ]
      },
      "03": {
        "orig_text": "Citing ``a precautionary measure``, Airbus announced that two of the Rolls-Royce Trent 900 engines on the prototype airliner would be replaced before the tour commenced.",
        "text": "Citing ``a precautionary measure``, Airbus announced that two of the Rolls-Royce Trent 900 engines on the prototype airliner would be replaced before the tour commenced.",
        "mention_ids": [
          "M020",
          "M021"
        ]
      },
      "04": {
        "orig_text": "Singapore will remain the airliner's first stop, where Singapore Airlines will display the aircraft they intend to launch into passenger-carrying service in November 2006.",
        "text": "Singapore will remain the airliner's first stop, where Singapore Airlines will display the aircraft they intend to launch into passenger-carrying service in November 2006.",
        "mention_ids": [
          "M002",
          "M003",
          "M005",
          "M023",
          "M024"
        ]
      },
      "05": {
        "orig_text": "It is expected that the Airbus 380 will wear the livery of Singapore Airlines when it arrives on November 11, three days later than originally planned.",
        "text": "It is expected that the Airbus 380 will wear the livery of Singapore Airlines when it arrives on November 11, three days later than originally planned.",
        "mention_ids": [
          "M006",
          "M010",
          "M012"
        ]
      },
      "06": {
        "orig_text": "The massive airliner will then bypass the planned stop in Malaysia, and fly directly to Sydney, then Melbourne and Brisbane as part of the leadup to Qantas' 85th birthday celebrations on November 16.",
        "text": "The massive airliner will then bypass the planned stop in Malaysia, and fly directly to Sydney, then Melbourne and Brisbane as part of the leadup to Qantas' 85th birthday celebrations on November 16.",
        "mention_ids": [
          "M088"
        ]
      },
      "07": {
        "orig_text": "Qantas plans to commence A380 operations starting in April 2007.",
        "text": "Qantas plans to commence A380 operations starting in April 2007.",
        "mention_ids": [
          "M121"
        ]
      },
      "08": {
        "orig_text": "The Malaysia visit will be rescheduled for November 17, occurring during the return flight to France.",
        "text": "The Malaysia visit will be rescheduled for November 17, occurring during the return flight to France.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "It is unclear whether the originally planned Australian tour dates can be kept, and sources indicate that the airliner's first stop in Brisbane, on November 11, maybe eliminated in favour of a brief appearance on November 16.",
        "text": "It is unclear whether the originally planned Australian tour dates can be kept, and sources indicate that the airliner's first stop in Brisbane, on November 11, maybe eliminated in favour of a brief appearance on November 16.",
        "mention_ids": [
          "M118"
        ]
      }
    },
