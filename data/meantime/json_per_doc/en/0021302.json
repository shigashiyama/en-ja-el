    },
    "sentences": {
      "00": {
        "orig_text": "Apple unveils iPod nano",
        "text": "Apple unveils iPod nano",
        "mention_ids": [
          "M006",
          "M008"
        ]
      },
      "01": {
        "orig_text": "September 07, 2005",
        "text": "September 07, 2005",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Apple today unveiled its latest addition to the iPod line, the iPod nano.",
        "text": "Apple today unveiled its latest addition to the iPod line, the iPod nano.",
        "mention_ids": [
          "M004",
          "M010"
        ]
      },
      "03": {
        "orig_text": "The announcement came today at a special music event hosted by Apple, held in the Moscone West Convention Center in San Francisco.",
        "text": "The announcement came today at a special music event hosted by Apple, held in the Moscone West Convention Center in San Francisco.",
        "mention_ids": [
          "M011",
          "M012"
        ]
      },
      "04": {
        "orig_text": "The iPod Nano comes in two colors, white and black, as well as two sizes, a 2GB and a 4GB, capable of holding 500 and 1,000 songs and will cost $199 and $249 USD respectively.",
        "text": "The iPod Nano comes in two colors, white and black, as well as two sizes, a 2GB and a 4GB, capable of holding 500 and 1,000 songs and will cost $199 and $249 USD respectively.",
        "mention_ids": [
          "M015",
          "M018"
        ]
      },
      "05": {
        "orig_text": "The iPod nano is the second model of the iPod family to utilize flash memory, after the Shuffle was introduced in January.",
        "text": "The iPod nano is the second model of the iPod family to utilize flash memory, after the Shuffle was introduced in January.",
        "mention_ids": [
          "M003",
          "M019",
          "M020"
        ]
      },
      "06": {
        "orig_text": "The iPod nano replaces the similar iPod Mini in the iPod family.",
        "text": "The iPod nano replaces the similar iPod Mini in the iPod family.",
        "mention_ids": [
          "M112"
        ]
      },
      "07": {
        "orig_text": "The iPod nano's maximum capacity is 2GB smaller than that of the last version of the iPod Mini, which held 6GB for the same price of $249.",
        "text": "The iPod nano's maximum capacity is 2GB smaller than that of the last version of the iPod Mini, which held 6GB for the same price of $249.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "Apple also announced an iTunes-compatible cell phone, the Motorola ROKR E1, which will be available exclusively from Cingular Wireless in the United States, Virgin will be in the UK.",
        "text": "Apple also announced an iTunes-compatible cell phone, the Motorola ROKR E1, which will be available exclusively from Cingular Wireless in the United States, Virgin will be in the UK.",
        "mention_ids": [
          "M109"
        ]
      },
      "09": {
        "orig_text": "The ROKR has a color display for viewing album art and features built-in stereo speakers, as well as stereo headphones that also serve as a mobile headset with microphone.",
        "text": "The ROKR has a color display for viewing album art and features built-in stereo speakers, as well as stereo headphones that also serve as a mobile headset with microphone.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "Users can randomly autofill or manually fill the mobile phone with playlists of music, audiobooks and Podcasts from their iTunes library via a USB 1.1 connection.",
        "text": "Users can randomly autofill or manually fill the mobile phone with playlists of music, audiobooks and Podcasts from their iTunes library via a USB 1.1 connection.",
        "mention_ids": [
          "M115"
        ]
      },
      "11": {
        "orig_text": "The Motorola ROKR pauses music automatically when users take a call and offers the ability to listen to music while messaging or taking a photo.",
        "text": "The Motorola ROKR pauses music automatically when users take a call and offers the ability to listen to music while messaging or taking a photo.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "It will cost $250 with a two-year cell service contract.",
        "text": "It will cost $250 with a two-year cell service contract.",
        "mention_ids": []
      },
      "13": {
        "orig_text": "A new version of iTunes was also announced at the event, bringing it to version 5.",
        "text": "A new version of iTunes was also announced at the event, bringing it to version 5.",
        "mention_ids": [
          "M110",
          "M111"
        ]
      },
      "14": {
        "orig_text": "Along with a new look, it also features a customizable randomization, improved search for the Library and Music Store, and greater parental controls on songs with explicit lyrics in the iTunes Music Store as well as controlling content of Podcasts.",
        "text": "Along with a new look, it also features a customizable randomization, improved search for the Library and Music Store, and greater parental controls on songs with explicit lyrics in the iTunes Music Store as well as controlling content of Podcasts.",
        "mention_ids": [
          "M114",
          "M116"
        ]
      }
    },
