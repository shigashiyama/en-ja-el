    "sentences": {
      "00": {
        "orig_text": "Apple releases Macbook",
        "text": "Apple releases Macbook",
        "mention_ids": [
          "M022",
          "M024"
        ]
      },
      "01": {
        "orig_text": "May 17, 2006",
        "text": "May 17, 2006",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Apple Computer today introduced the new MacBook line, which includes the Macbook and Macbook Pro.",
        "text": "Apple Computer today introduced the new MacBook line, which includes the Macbook and Macbook Pro.",
        "mention_ids": [
          "M004",
          "M012"
        ]
      },
      "03": {
        "orig_text": "It is the successor to the iBook line and contains Intel Core Duo processors and a host of features, and starting at a price of $1,099.",
        "text": "It is the successor to the iBook line and contains Intel Core Duo processors and a host of features, and starting at a price of $1,099.",
        "mention_ids": [
          "M001",
          "M002",
          "M011",
          "M015"
        ]
      },
      "04": {
        "orig_text": "The Macbook features a 13.3`` widescreen display, while the Pro can be purchased with either 15`` or 17`` displays.",
        "text": "The Macbook features a 13.3`` widescreen display, while the Pro can be purchased with either 15`` or 17`` displays.",
        "mention_ids": [
          "M014",
          "M021",
          "M023",
          "M025"
        ]
      },
      "05": {
        "orig_text": "It comes in two colors: Black (2 GHz model only) and White (1.83 and 2 GHz models).",
        "text": "It comes in two colors: Black (2 GHz model only) and White (1.83 and 2 GHz models).",
        "mention_ids": [
          "M003",
          "M005",
          "M007"
        ]
      },
      "06": {
        "orig_text": "This release leaves only one PowerPC processor computer that has not made the transition to Intel chips, the PowerMac G5.",
        "text": "This release leaves only one PowerPC processor computer that has not made the transition to Intel chips, the PowerMac G5.",
        "mention_ids": []
      }
