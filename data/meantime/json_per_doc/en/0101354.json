    },
    "sentences": {
      "00": {
        "orig_text": "Apple releases iPhone SDK, announces upcoming update",
        "text": "Apple releases iPhone SDK, announces upcoming update",
        "mention_ids": [
          "M016",
          "M018"
        ]
      },
      "01": {
        "orig_text": "March 06, 2008",
        "text": "March 06, 2008",
        "mention_ids": []
      },
      "02": {
        "orig_text": "During a media event held at its Cupertino headquarters, Apple released the long awaited iPhones Software Development Kit (SDK) and the associated iTunes ``App Store`` and iPhone Developer Program.",
        "text": "During a media event held at its Cupertino headquarters, Apple released the long awaited iPhones Software Development Kit (SDK) and the associated iTunes ``App Store`` and iPhone Developer Program.",
        "mention_ids": [
          "M008",
          "M009",
          "M010",
          "M011",
          "M012",
          "M068"
        ]
      },
      "03": {
        "orig_text": "The SDK will allow third party software developers to create applications for the iPhone and then distribute them on the iTunes Apps Stores.",
        "text": "The SDK will allow third party software developers to create applications for the iPhone and then distribute them on the iTunes Apps Stores.",
        "mention_ids": [
          "M013",
          "M014",
          "M015",
          "M017",
          "M019",
          "M020"
        ]
      },
      "04": {
        "orig_text": "Any members of the iPhone Developer Program can add applications to the store for free; the program costs $99 (USD) to join.",
        "text": "Any members of the iPhone Developer Program can add applications to the store for free; the program costs $99 (USD) to join.",
        "mention_ids": [
          "M001",
          "M021",
          "M022",
          "M023"
        ]
      },
      "05": {
        "orig_text": "Developers will be able to set the price of the applications or release them for free.",
        "text": "Developers will be able to set the price of the applications or release them for free.",
        "mention_ids": [
          "M002",
          "M004",
          "M006",
          "M007"
        ]
      },
      "06": {
        "orig_text": "All applications will have to be approved by Apple before being allowed on the store.",
        "text": "All applications will have to be approved by Apple before being allowed on the store.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Some of the disallowed categories are``porn``, excessive bandwidth users and anything malicious or illegal.",
        "text": "Some of the disallowed categories are``porn``, excessive bandwidth users and anything malicious or illegal.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "VoIP applications would be allowed over Wi-Fi only.",
        "text": "VoIP applications would be allowed over Wi-Fi only.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "Enterprises can setup private pages on the store to distribute internal applications.",
        "text": "Enterprises can setup private pages on the store to distribute internal applications.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "At the event several third party developers that had been given early access to the SDK demonstrated their applications.",
        "text": "At the event several third party developers that had been given early access to the SDK demonstrated their applications.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "Among the applications demonstrated were EA's Spore, AOL's AIM instant messenger and a client for epocrates, a drug reference service.",
        "text": "Among the applications demonstrated were EA's Spore, AOL's AIM instant messenger and a client for epocrates, a drug reference service.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "The company also announced the iPhone 2.0 update that was released as a closed beta for developers and is scheduled for public release in June.",
        "text": "The company also announced the iPhone 2.0 update that was released as a closed beta for developers and is scheduled for public release in June.",
        "mention_ids": []
      },
      "13": {
        "orig_text": "In the new version Apple has been focusing on enterprise features in an attempt to compete with rivals like RIM's BlackBerry and Microsoft's Windows Mobile.",
        "text": "In the new version Apple has been focusing on enterprise features in an attempt to compete with rivals like RIM's BlackBerry and Microsoft's Windows Mobile.",
        "mention_ids": []
      },
      "14": {
        "orig_text": "The update adds support for ActiveSync, which Apple has licensed from rival Microsoft, WPA2 and virtual private network support.",
        "text": "The update adds support for ActiveSync, which Apple has licensed from rival Microsoft, WPA2 and virtual private network support.",
        "mention_ids": []
      },
      "15": {
        "orig_text": "The update will also be available to iPod Touch users for what Steve Jobs described as ``a nominal charge``, as Apple have done in the past with the previous iPod Touch and MacBook Wi-Fi updates.",
        "text": "The update will also be available to iPod Touch users for what Steve Jobs described as ``a nominal charge``, as Apple have done in the past with the previous iPod Touch and MacBook Wi-Fi updates.",
        "mention_ids": [
          "M115"
        ]
      }
    },
