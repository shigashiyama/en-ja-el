    },
    "sentences": {
      "00": {
        "orig_text": "Aeroflot negotiates purchase of 22 new Boeing 787 Dreamliner aircraft",
        "text": "Aeroflot negotiates purchase of 22 new Boeing 787 Dreamliner aircraft",
        "mention_ids": [
          "M003",
          "M006"
        ]
      },
      "01": {
        "orig_text": "June 10, 2007",
        "text": "June 10, 2007",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Russian flag carrier Aeroflot has negotiated the purchase of 22 new Boeing 787 Dreamliners, an aircraft Boeing currently has under development.",
        "text": "Russian flag carrier Aeroflot has negotiated the purchase of 22 new Boeing 787 Dreamliners, an aircraft Boeing currently has under development.",
        "mention_ids": [
          "M004",
          "M065"
        ]
      },
      "03": {
        "orig_text": "The first jets will be delivered in 2014.",
        "text": "The first jets will be delivered in 2014.",
        "mention_ids": [
          "M010"
        ]
      },
      "04": {
        "orig_text": "The Dreamliner will be the first commercial jet to be manufactured mainly with carbon-fiber composites instead of the aluminium previously used.",
        "text": "The Dreamliner will be the first commercial jet to be manufactured mainly with carbon-fiber composites instead of the aluminium previously used.",
        "mention_ids": [
          "M011",
          "M012"
        ]
      },
      "05": {
        "orig_text": "The deal was announced during a large business forum that President Vladimir Putin had set up to showcase his country's role in the international market.",
        "text": "The deal was announced during a large business forum that President Vladimir Putin had set up to showcase his country's role in the international market.",
        "mention_ids": [
          "M014",
          "M016",
          "M018"
        ]
      },
      "06": {
        "orig_text": "The deal was the centrepiece of the event.",
        "text": "The deal was the centrepiece of the event.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The sale is the result of a deal begun between Aeroflot and Chicago-based Boeing last year, but negotiations had stalled due to worsening relations between the US and Russia.",
        "text": "The sale is the result of a deal begun between Aeroflot and Chicago-based Boeing last year, but negotiations had stalled due to worsening relations between the US and Russia.",
        "mention_ids": [
          "M071"
        ]
      },
      "08": {
        "orig_text": "Aeroflot has suffered as a result of the delay.",
        "text": "Aeroflot has suffered as a result of the delay.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "If had the sale gone through last year, the aircraft would have been delivered by 2011.",
        "text": "If had the sale gone through last year, the aircraft would have been delivered by 2011.",
        "mention_ids": [
          "M067"
        ]
      },
      "10": {
        "orig_text": "The aircraft have a pre-modification catalogue value of US$3.5 billion, although neither of the companies involved would reveal the actual agreement reached between the two firms.",
        "text": "The aircraft have a pre-modification catalogue value of US$3.5 billion, although neither of the companies involved would reveal the actual agreement reached between the two firms.",
        "mention_ids": [
          "M112"
        ]
      },
      "11": {
        "orig_text": "When asked whether the deal should be seen as a step towards a resolution of the rift between Russia and the US, Boeing Executive Vice President Scott Carson replied, ``It represents a business relationship between our Company and Aeroflot, and I'm really proud of it,``.",
        "text": "When asked whether the deal should be seen as a step towards a resolution of the rift between Russia and the US, Boeing Executive Vice President Scott Carson replied, ``It represents a business relationship between our Company and Aeroflot, and I'm really proud of it,``.",
        "mention_ids": [
          "M097",
          "M105",
          "M115"
        ]
      },
      "12": {
        "orig_text": "Boeing has had a policy of Russian involvement dating back to the early 1990s.",
        "text": "Boeing has had a policy of Russian involvement dating back to the early 1990s.",
        "mention_ids": [
          "M077"
        ]
      },
      "13": {
        "orig_text": "More recently, Boeing has entered into an agreement with Russian manufacturer Sukhoi for worldwide marketing and after-sales support for their regional aircraft, the Superjet, and have reached a deal with Kremlin-owned Unified Aircraft for general co-operation.",
        "text": "More recently, Boeing has entered into an agreement with Russian manufacturer Sukhoi for worldwide marketing and after-sales support for their regional aircraft, the Superjet, and have reached a deal with Kremlin-owned Unified Aircraft for general co-operation.",
        "mention_ids": [
          "M108",
          "M157"
        ]
      },
      "14": {
        "orig_text": "They also have a Moscow design centre that has 1,400 employees and a 30-year contract for supply of titanium parts with Russia's VSMPO-Avisma, which Boeing estimates to be worth $18 million.",
        "text": "They also have a Moscow design centre that has 1,400 employees and a 30-year contract for supply of titanium parts with Russia's VSMPO-Avisma, which Boeing estimates to be worth $18 million.",
        "mention_ids": [
          "M080",
          "M113"
        ]
      },
      "15": {
        "orig_text": "It has been reported that the deal between Boeing and Aeroflot was revived after Boeing promised that technical assistance would be provided to Aeroflot for the new aircraft.",
        "text": "It has been reported that the deal between Boeing and Aeroflot was revived after Boeing promised that technical assistance would be provided to Aeroflot for the new aircraft.",
        "mention_ids": [
          "M088",
          "M117"
        ]
      },
      "16": {
        "orig_text": "Aeroflot's current fleet of Boeing and Airbus aircraft was built during the Soviet era.",
        "text": "Aeroflot's current fleet of Boeing and Airbus aircraft was built during the Soviet era.",
        "mention_ids": [
          "M082",
          "M086"
        ]
      },
      "17": {
        "orig_text": "Aeroflot say that the deal does not affect plans to purchase new Airbuses at a later date for later delivery.",
        "text": "Aeroflot say that the deal does not affect plans to purchase new Airbuses at a later date for later delivery.",
        "mention_ids": []
      }
    },
