    },
    "sentences": {
      "00": {
        "orig_text": "Singapore Airlines to be compensated for A380 delays",
        "text": "Singapore Airlines to be compensated for A380 delays",
        "mention_ids": [
          "M003",
          "M021"
        ]
      },
      "01": {
        "orig_text": "October 05, 2006",
        "text": "October 05, 2006",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Singapore Airlines says that it will be compensated by Airbus for the additional delay in delivery of its new superjumbo A380's.",
        "text": "Singapore Airlines says that it will be compensated by Airbus for the additional delay in delivery of its new superjumbo A380's.",
        "mention_ids": [
          "M006",
          "M008",
          "M010",
          "M012"
        ]
      },
      "03": {
        "orig_text": "The double-deck commercial aircrafts' delivery, originally due later this year, will be delayed until October 2007.",
        "text": "The double-deck commercial aircrafts' delivery, originally due later this year, will be delayed until October 2007.",
        "mention_ids": [
          "M004"
        ]
      },
      "04": {
        "orig_text": "The main fault has been found to be problems in the aircraft's 500km of wiring.",
        "text": "The main fault has been found to be problems in the aircraft's 500km of wiring.",
        "mention_ids": [
          "M015"
        ]
      },
      "05": {
        "orig_text": "In a statement on the Airbus website, the company said that ``the root cause of the problem is the fact that the 3D Digital mock-up, which facilitates the design of the electrical harnesses installation, was implemented late and that the people working on it were in their learning curve``.",
        "text": "In a statement on the Airbus website, the company said that ``the root cause of the problem is the fact that the 3D Digital mock-up, which facilitates the design of the electrical harnesses installation, was implemented late and that the people working on it were in their learning curve``.",
        "mention_ids": [
          "M002",
          "M005",
          "M017",
          "M018",
          "M019"
        ]
      },
      "06": {
        "orig_text": "Airbus is currently two years behind schedule.",
        "text": "Airbus is currently two years behind schedule.",
        "mention_ids": [
          "M092"
        ]
      },
      "07": {
        "orig_text": "In a statement, SIA said that, ``The delays are disappointing; all the more so because the flight test and certification programme is proceeding well, and the delays are down to production issues.``",
        "text": "In a statement, SIA said that, ``The delays are disappointing; all the more so because the flight test and certification programme is proceeding well, and the delays are down to production issues.``",
        "mention_ids": [
          "M111"
        ]
      },
      "08": {
        "orig_text": "Analysts are predicting that the compensations could well run into a few hundred million US dollars.",
        "text": "Analysts are predicting that the compensations could well run into a few hundred million US dollars.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "SIA has stated that the compensation details will be kept confidential Singapore Airlines is the first airline to fly the A380 and will be receiving the first four aircraft.",
        "text": "SIA has stated that the compensation details will be kept confidential Singapore Airlines is the first airline to fly the A380 and will be receiving the first four aircraft.",
        "mention_ids": [
          "M081",
          "M083",
          "M088",
          "M091"
        ]
      }
    },
