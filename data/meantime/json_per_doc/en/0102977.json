    },
    "sentences": {
      "00": {
        "orig_text": "Airbus parent EADS wins #13 billion UK RAF airtanker contract",
        "text": "Airbus parent EADS wins #13 billion UK RAF airtanker contract",
        "mention_ids": [
          "M024",
          "M029"
        ]
      },
      "01": {
        "orig_text": "March 27, 2008",
        "text": "March 27, 2008",
        "mention_ids": []
      },
      "02": {
        "orig_text": "European Aeronautic Defence & Space NV (EADS), the parent company of European airframer Airbus, has won a #13 billion contract to supply the United Kingdom's Royal Air Force (RAF) with aerial refueling tankers to replace the nation's current ageing fleet.",
        "text": "European Aeronautic Defence & Space NV (EADS), the parent company of European airframer Airbus, has won a #13 billion contract to supply the United Kingdom's Royal Air Force (RAF) with aerial refueling tankers to replace the nation's current ageing fleet.",
        "mention_ids": [
          "M018",
          "M019",
          "M021",
          "M022",
          "M082"
        ]
      },
      "03": {
        "orig_text": "AirTanker Ltd., an EADS-led consortium, have signed a 27-year contract with the Defense Ministry to supply 14 new Airbus A330-200 passenger airliner converted for the task.",
        "text": "AirTanker Ltd., an EADS-led consortium, have signed a 27-year contract with the Defense Ministry to supply 14 new Airbus A330-200 passenger airliner converted for the task.",
        "mention_ids": [
          "M001",
          "M002",
          "M004",
          "M078"
        ]
      },
      "04": {
        "orig_text": "They will be owned by AirTanker, who retains commercial leasing rights to five which can carry 290 passengers plus cargo, but will fly in RAF livery.",
        "text": "They will be owned by AirTanker, who retains commercial leasing rights to five which can carry 290 passengers plus cargo, but will fly in RAF livery.",
        "mention_ids": [
          "M006",
          "M008",
          "M010",
          "M011",
          "M012",
          "M013",
          "M014"
        ]
      },
      "05": {
        "orig_text": "They replace existing Lockheed Tristar and Vickers VC-10 aircraft.",
        "text": "They replace existing Lockheed Tristar and Vickers VC-10 aircraft.",
        "mention_ids": [
          "M015",
          "M016"
        ]
      },
      "06": {
        "orig_text": "The first aircraft will be in service by 2011 and all by 2016.",
        "text": "The first aircraft will be in service by 2011 and all by 2016.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Rolls-Royce, part of the consortium, will supply engines.",
        "text": "Rolls-Royce, part of the consortium, will supply engines.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "France's Thales will supply electronics, Wimborne, UK's Cobham will manufacture refueling equipment and Southampton, UK's VT Group will provide service management.",
        "text": "France's Thales will supply electronics, Wimborne, UK's Cobham will manufacture refueling equipment and Southampton, UK's VT Group will provide service management.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "Last month, Northrop Grumman and EADS defeated Boeing to win a massive order for 179 tankers from the United States Air Force.",
        "text": "Last month, Northrop Grumman and EADS defeated Boeing to win a massive order for 179 tankers from the United States Air Force.",
        "mention_ids": [
          "M084",
          "M106",
          "M122",
          "M157"
        ]
      },
      "10": {
        "orig_text": "Airbus has also inked recent deals with the Royal Australian Air Force, the Royal Saudi Air Force and the UAE Air Force.",
        "text": "Airbus has also inked recent deals with the Royal Australian Air Force, the Royal Saudi Air Force and the UAE Air Force.",
        "mention_ids": [
          "M100"
        ]
      }
    },
