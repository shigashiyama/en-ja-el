    },
    "sentences": {
      "00": {
        "orig_text": "Automobile sales in the United States down sharply",
        "text": "Automobile sales in the United States down sharply",
        "mention_ids": [
          "M043",
          "M044"
        ]
      },
      "01": {
        "orig_text": "February 03, 2009",
        "text": "February 03, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Sales of automobiles in the United States fell as much as 35% in January, compared to the same month a year ago, as manufacturers reported their January sales.",
        "text": "Sales of automobiles in the United States fell as much as 35% in January, compared to the same month a year ago, as manufacturers reported their January sales.",
        "mention_ids": [
          "M001",
          "M002",
          "M003",
          "M004"
        ]
      },
      "03": {
        "orig_text": "Ford Motor Company sales dropped 42% last month from the previous year.",
        "text": "Ford Motor Company sales dropped 42% last month from the previous year.",
        "mention_ids": [
          "M015"
        ]
      },
      "04": {
        "orig_text": "General Motors reported a 49% decline.",
        "text": "General Motors reported a 49% decline.",
        "mention_ids": [
          "M022"
        ]
      },
      "05": {
        "orig_text": "Chrysler was hardest hit among the domestically owned manufacturers with a plunge of 55%.",
        "text": "Chrysler was hardest hit among the domestically owned manufacturers with a plunge of 55%.",
        "mention_ids": [
          "M026",
          "M027"
        ]
      },
      "06": {
        "orig_text": "The decline in sales has not been limited to just US manufacturers.",
        "text": "The decline in sales has not been limited to just US manufacturers.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Japanese automaker Toyota reported a 32% drop in US sales from a year earlier, Nissan Motors dropped 30%, and Honda sales dropped 28%.",
        "text": "Japanese automaker Toyota reported a 32% drop in US sales from a year earlier, Nissan Motors dropped 30%, and Honda sales dropped 28%.",
        "mention_ids": [
          "M068",
          "M070"
        ]
      },
      "08": {
        "orig_text": "Subaru and South Korean Hyundai Motor Company were two of the few auto firms that reported an increase in sales.",
        "text": "Subaru and South Korean Hyundai Motor Company were two of the few auto firms that reported an increase in sales.",
        "mention_ids": [
          "M049"
        ]
      },
      "09": {
        "orig_text": "They posted gains of eight and fourteen percent, respectively.",
        "text": "They posted gains of eight and fourteen percent, respectively.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "On an annual basis, sales overall have also been plummeting.",
        "text": "On an annual basis, sales overall have also been plummeting.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "Industry-wide US car sales dropped 18% last year to 13.2 million automobiles.",
        "text": "Industry-wide US car sales dropped 18% last year to 13.2 million automobiles.",
        "mention_ids": [
          "M053"
        ]
      },
      "12": {
        "orig_text": "GM announced today that it will offer voluntary buyouts to 22,000 employees in the US, in an effort to reduce its expenditures.",
        "text": "GM announced today that it will offer voluntary buyouts to 22,000 employees in the US, in an effort to reduce its expenditures.",
        "mention_ids": [
          "M061",
          "M067",
          "M072"
        ]
      },
      "13": {
        "orig_text": "Last month, GM and Chrysler were given loans worth US$17.4 billion from the government after they warned of imminent bankruptcy.",
        "text": "Last month, GM and Chrysler were given loans worth US$17.4 billion from the government after they warned of imminent bankruptcy.",
        "mention_ids": [
          "M055",
          "M063"
        ]
      }
    },
