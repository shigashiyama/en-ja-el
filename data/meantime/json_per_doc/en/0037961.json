    "sentences": {
      "00": {
        "orig_text": "Apple releases program to limit iPod volume",
        "text": "Apple releases program to limit iPod volume",
        "mention_ids": [
          "M003",
          "M010"
        ]
      },
      "01": {
        "orig_text": "April 05, 2006",
        "text": "April 05, 2006",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Apple has released an application that limits the volume produced from its iPod portable music players.",
        "text": "Apple has released an application that limits the volume produced from its iPod portable music players.",
        "mention_ids": [
          "M013",
          "M014",
          "M018"
        ]
      },
      "03": {
        "orig_text": "The software update, iPod Software 1.1.1, works with the iPod Nano and the Video version.",
        "text": "The software update, iPod Software 1.1.1, works with the iPod Nano and the Video version.",
        "mention_ids": [
          "M002",
          "M025"
        ]
      },
      "04": {
        "orig_text": "Apple released the program in response to complaints that it had not taken necessary measures to prevent hearing loss in iPod users.",
        "text": "Apple released the program in response to complaints that it had not taken necessary measures to prevent hearing loss in iPod users.",
        "mention_ids": [
          "M004",
          "M006",
          "M007",
          "M008"
        ]
      },
      "05": {
        "orig_text": "In California, a class-action lawsuit was filed against Apple in January over claims of hearing loss from iPod use.",
        "text": "In California, a class-action lawsuit was filed against Apple in January over claims of hearing loss from iPod use.",
        "mention_ids": [
          "M011",
          "M012",
          "M015"
        ]
      },
      "06": {
        "orig_text": "Apple's worldwide vice-president of marketing for the iPod, Greg Joswiak, indicated that this patch is available for people who want ``an easy-to-use option to set their own personal volume limit.``",
        "text": "Apple's worldwide vice-president of marketing for the iPod, Greg Joswiak, indicated that this patch is available for people who want ``an easy-to-use option to set their own personal volume limit.``",
        "mention_ids": [
          "M113"
        ]
      },
      "07": {
        "orig_text": "It is available for free download.",
        "text": "It is available for free download.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "The software also has the option of being used with parental controls to prevent children from modifying the volume.",
        "text": "The software also has the option of being used with parental controls to prevent children from modifying the volume.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "iPods sold in France already have the volume limited, as the law forbids a sound louder than 100 decibels (dB).",
        "text": "iPods sold in France already have the volume limited, as the law forbids a sound louder than 100 decibels (dB).",
        "mention_ids": [
          "M112"
        ]
      },
      "10": {
        "orig_text": "The American Speech Language Hearing Association published a study where it appears that iPod's (and mp3 players's) power is 120 dB, comparable to a live concert.",
        "text": "The American Speech Language Hearing Association published a study where it appears that iPod's (and mp3 players's) power is 120 dB, comparable to a live concert.",
        "mention_ids": [
          "M111"
        ]
      },
      "11": {
        "orig_text": "A gunshot is 140 dB.",
        "text": "A gunshot is 140 dB.",
        "mention_ids": []
      }
    },
