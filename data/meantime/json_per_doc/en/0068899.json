    },
    "sentences": {
      "00": {
        "orig_text": "DaimlerChrysler to sell Chrysler Group for $7.4 Billion",
        "text": "DaimlerChrysler to sell Chrysler Group for $7.4 Billion",
        "mention_ids": [
          "M053",
          "M054"
        ]
      },
      "01": {
        "orig_text": "April 14, 2007",
        "text": "April 14, 2007",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The DaimlerChrysler Corporation has announced that the corporation is divesting itself of the Chrysler Group by selling it to New York City-based private equity firm Cerberus Capital Management for US$7.4 billion.",
        "text": "The DaimlerChrysler Corporation has announced that the corporation is divesting itself of the Chrysler Group by selling it to New York City-based private equity firm Cerberus Capital Management for US$7.4 billion.",
        "mention_ids": [
          "M001",
          "M002",
          "M003",
          "M004",
          "M005",
          "M009"
        ]
      },
      "03": {
        "orig_text": "The companies' names are announced to be changed to Daimler AG and Chrysler Holding LLC.",
        "text": "The companies' names are announced to be changed to Daimler AG and Chrysler Holding LLC.",
        "mention_ids": [
          "M015"
        ]
      },
      "04": {
        "orig_text": "DaimlerChrysler has sold 80.1% of its stake in Chrysler, retaining the other 19.9%, for joint ventures and other agreements and partnerships.",
        "text": "DaimlerChrysler has sold 80.1% of its stake in Chrysler, retaining the other 19.9%, for joint ventures and other agreements and partnerships.",
        "mention_ids": [
          "M018",
          "M049"
        ]
      },
      "05": {
        "orig_text": "Cerberus has agreed to take on billions of dollars in pension and retiree health care costs at Chrysler.",
        "text": "Cerberus has agreed to take on billions of dollars in pension and retiree health care costs at Chrysler.",
        "mention_ids": [
          "M028",
          "M029"
        ]
      },
      "06": {
        "orig_text": "Cerberus also led a consortium that acquired the majority stake in April of last year in GMAC, the financing arm of GM, and is said to planning to invest in auto parts giant Delphi.",
        "text": "Cerberus also led a consortium that acquired the majority stake in April of last year in GMAC, the financing arm of GM, and is said to planning to invest in auto parts giant Delphi.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Buzz Hargrove, the president of Canadian Auto Workers said he had been assured that the collective bargaining agreement with Chrysler would remain and that jobs would not be eliminated.",
        "text": "Buzz Hargrove, the president of Canadian Auto Workers said he had been assured that the collective bargaining agreement with Chrysler would remain and that jobs would not be eliminated.",
        "mention_ids": [
          "M071"
        ]
      },
      "08": {
        "orig_text": "The name change of the German company DaimlerChrysler to Daimler AG is pending shareholder approval at the next meeting, scheduled for fall 2007.",
        "text": "The name change of the German company DaimlerChrysler to Daimler AG is pending shareholder approval at the next meeting, scheduled for fall 2007.",
        "mention_ids": [
          "M058"
        ]
      }
    },
