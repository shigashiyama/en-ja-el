    "sentences": {
      "00": {
        "orig_text": "Global stock markets plunge",
        "text": "Global stock markets plunge",
        "mention_ids": [
          "M043"
        ]
      },
      "01": {
        "orig_text": "October 24, 2008",
        "text": "October 24, 2008",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Stock markets around the world have fallen dramatically today.",
        "text": "Stock markets around the world have fallen dramatically today.",
        "mention_ids": [
          "M002"
        ]
      },
      "03": {
        "orig_text": "This is following the ongoing events in the financial world and related statements by several United Kingdom politicians, including Prime Minister Gordon Brown.",
        "text": "This is following the ongoing events in the financial world and related statements by several United Kingdom politicians, including Prime Minister Gordon Brown.",
        "mention_ids": [
          "M011",
          "M013",
          "M015"
        ]
      },
      "04": {
        "orig_text": "The British FTSE 100 index was one the indices that today suffered from large falls.",
        "text": "The British FTSE 100 index was one the indices that today suffered from large falls.",
        "mention_ids": [
          "M016",
          "M018",
          "M042"
        ]
      },
      "05": {
        "orig_text": "It fell by 5.00% to slip below the four thousand mark, at 3,883.36 points.",
        "text": "It fell by 5.00% to slip below the four thousand mark, at 3,883.36 points.",
        "mention_ids": [
          "M025"
        ]
      },
      "06": {
        "orig_text": "Two of the major United States indices saw falls of over two per cent.",
        "text": "Two of the major United States indices saw falls of over two per cent.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The Dow Jones Industrial Average fell by 252.01 points (2.90%) to 8,439.24 by 17:30 UTC, while the Nasdaq Composite index experienced a 2.30% drop.",
        "text": "The Dow Jones Industrial Average fell by 252.01 points (2.90%) to 8,439.24 by 17:30 UTC, while the Nasdaq Composite index experienced a 2.30% drop.",
        "mention_ids": [
          "M060",
          "M061"
        ]
      },
      "08": {
        "orig_text": "South American markets performed particularly badly today.",
        "text": "South American markets performed particularly badly today.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "By 17:30 UTC, the Argentinean MERVAL index had fallen by almost six per cent to 906.160 points, while the Brazilian Bovespa fell by 6.89% (2,328.45 points) to a value of 31,490.04.",
        "text": "By 17:30 UTC, the Argentinean MERVAL index had fallen by almost six per cent to 906.160 points, while the Brazilian Bovespa fell by 6.89% (2,328.45 points) to a value of 31,490.04.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "David Buik of BGC Partners commented on today's falls.",
        "text": "David Buik of BGC Partners commented on today's falls.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "``It's carnage,`` he said.",
        "text": "``It's carnage,`` he said.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "``Confidence is shot to ribbons.",
        "text": "``Confidence is shot to ribbons.",
        "mention_ids": []
      },
      "13": {
        "orig_text": "We need action now.",
        "text": "We need action now.",
        "mention_ids": []
      },
      "14": {
        "orig_text": "It is a catastrophe that we have got to recession, but we have got to learn to live with it, and that means saving the markets.``",
        "text": "It is a catastrophe that we have got to recession, but we have got to learn to live with it, and that means saving the markets.``",
        "mention_ids": []
      }
    },
