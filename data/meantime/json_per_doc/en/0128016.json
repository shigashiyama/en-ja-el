    "sentences": {
      "00": {
        "orig_text": "China Eastern Airlines to purchase twenty new Airbus A320 jets",
        "text": "China Eastern Airlines to purchase twenty new Airbus A320 jets",
        "mention_ids": [
          "M009",
          "M010"
        ]
      },
      "01": {
        "orig_text": "June 18, 2009",
        "text": "June 18, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "China Eastern Airlines stated on Thursday that it had signed a contract to purchase a score of Airbus A320 passenger jets for 9.9 billion yuan (about US$1.4 billion).",
        "text": "China Eastern Airlines stated on Thursday that it had signed a contract to purchase a score of Airbus A320 passenger jets for 9.9 billion yuan (about US$1.4 billion).",
        "mention_ids": [
          "M003",
          "M004",
          "M008",
          "M012"
        ]
      },
      "03": {
        "orig_text": "The airline, which is the third largest in China, signed the agreement in Paris on June 15.",
        "text": "The airline, which is the third largest in China, signed the agreement in Paris on June 15.",
        "mention_ids": [
          "M007",
          "M014",
          "M015",
          "M016",
          "M067"
        ]
      },
      "04": {
        "orig_text": "The company said that the planes would meet additional demand for domestic passenger routes.",
        "text": "The company said that the planes would meet additional demand for domestic passenger routes.",
        "mention_ids": [
          "M001",
          "M005",
          "M017"
        ]
      },
      "05": {
        "orig_text": "The deal was approved unanimously by China Eastern's board of directors.",
        "text": "The deal was approved unanimously by China Eastern's board of directors.",
        "mention_ids": [
          "M063"
        ]
      },
      "06": {
        "orig_text": "The China Daily newspaper reported that the deal is waiting for an approval by Chinese aviation authorities.",
        "text": "The China Daily newspaper reported that the deal is waiting for an approval by Chinese aviation authorities.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The planes are expected to start delivering in 2011 and end two years later.",
        "text": "The planes are expected to start delivering in 2011 and end two years later.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "The Airbus A320 is a single-aisle airplane, and Airbus' best selling jet.",
        "text": "The Airbus A320 is a single-aisle airplane, and Airbus' best selling jet.",
        "mention_ids": [
          "M060",
          "M076"
        ]
      },
      "09": {
        "orig_text": "It can seat up to 150 people and is used primarily for medium- and short-haul routes.",
        "text": "It can seat up to 150 people and is used primarily for medium- and short-haul routes.",
        "mention_ids": []
      }
