    },
    "sentences": {
      "00": {
        "orig_text": "GM posts first annual loss since 1992",
        "text": "GM posts first annual loss since 1992",
        "mention_ids": [
          "M008"
        ]
      },
      "01": {
        "orig_text": "January 27, 2006",
        "text": "January 27, 2006",
        "mention_ids": []
      },
      "02": {
        "orig_text": "General Motors Corporation (GM) has posted its first annual loss since 1992.",
        "text": "General Motors Corporation (GM) has posted its first annual loss since 1992.",
        "mention_ids": [
          "M011",
          "M012"
        ]
      },
      "03": {
        "orig_text": "GM reported losing US$4.8 billion in the fourth quarter of 2005 and a total loss of $8.6 billion for the entire year.",
        "text": "GM reported losing US$4.8 billion in the fourth quarter of 2005 and a total loss of $8.6 billion for the entire year.",
        "mention_ids": [
          "M002"
        ]
      },
      "04": {
        "orig_text": "GM admitted Thursday night that the loss could swell further as it pays pensions and healthcare costs to thousands of former workers.",
        "text": "GM admitted Thursday night that the loss could swell further as it pays pensions and healthcare costs to thousands of former workers.",
        "mention_ids": [
          "M003",
          "M004",
          "M005"
        ]
      },
      "05": {
        "orig_text": "GM warned that the amount calculated for last year is preliminary and could rise before it is officially reported to the US securities and exchanges commission in March.",
        "text": "GM warned that the amount calculated for last year is preliminary and could rise before it is officially reported to the US securities and exchanges commission in March.",
        "mention_ids": [
          "M051"
        ]
      },
      "06": {
        "orig_text": "The loss was far greater than analysts predicted.",
        "text": "The loss was far greater than analysts predicted.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Ford, the second of the big three American car manufacturers, beat predictions earlier in the week.",
        "text": "Ford, the second of the big three American car manufacturers, beat predictions earlier in the week.",
        "mention_ids": [
          "M052",
          "M078"
        ]
      },
      "08": {
        "orig_text": "In contrast, Toyota is expected to report that it will beat last year's profit of $11 billion.",
        "text": "In contrast, Toyota is expected to report that it will beat last year's profit of $11 billion.",
        "mention_ids": [
          "M055",
          "M069"
        ]
      },
      "09": {
        "orig_text": "GM's automotive division lost $1.5 billion in the fourth quarter, driven by losses in North America.",
        "text": "GM's automotive division lost $1.5 billion in the fourth quarter, driven by losses in North America.",
        "mention_ids": [
          "M060"
        ]
      },
      "10": {
        "orig_text": "This has been attributed to GM's shrinking market share, which has been taken by Japanese manufacturers Toyota and Nissan.",
        "text": "This has been attributed to GM's shrinking market share, which has been taken by Japanese manufacturers Toyota and Nissan.",
        "mention_ids": [
          "M057",
          "M082"
        ]
      },
      "11": {
        "orig_text": "A further $1.3 billion was lost in restructuring charges.",
        "text": "A further $1.3 billion was lost in restructuring charges.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "As part of the restructure, GM plans to cut 30,000 jobs and close 12 facilities by 2008.",
        "text": "As part of the restructure, GM plans to cut 30,000 jobs and close 12 facilities by 2008.",
        "mention_ids": [
          "M054"
        ]
      },
      "13": {
        "orig_text": "An aide for Kirk Kerkorian, GMs largest individual investor (at 9.9%), has called on the company to halve its $1.1 billion annual dividend, cut executive pay and sell Saab.",
        "text": "An aide for Kirk Kerkorian, GMs largest individual investor (at 9.9%), has called on the company to halve its $1.1 billion annual dividend, cut executive pay and sell Saab.",
        "mention_ids": [
          "M071",
          "M086",
          "M087"
        ]
      }
    },
