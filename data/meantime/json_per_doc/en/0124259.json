    },
    "sentences": {
      "00": {
        "orig_text": "US stocks log gains for fourth week in a row",
        "text": "US stocks log gains for fourth week in a row",
        "mention_ids": [
          "M001"
        ]
      },
      "01": {
        "orig_text": "April 04, 2009",
        "text": "April 04, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "United States stock indexes posted gains on Friday, marking a four-week long positive streak.",
        "text": "United States stock indexes posted gains on Friday, marking a four-week long positive streak.",
        "mention_ids": [
          "M007"
        ]
      },
      "03": {
        "orig_text": "This comes despite a report released by the US Labor Department on the same day, which said that the US economy shed 663,000 jobs in March, with the unemployment rate reaching 8.5%, the highest since 1983.",
        "text": "This comes despite a report released by the US Labor Department on the same day, which said that the US economy shed 663,000 jobs in March, with the unemployment rate reaching 8.5%, the highest since 1983.",
        "mention_ids": [
          "M020",
          "M025"
        ]
      },
      "04": {
        "orig_text": "The Dow Jones Industrial Average posted a modest gain of half a percent or 39 points, ending the day at 8,017.",
        "text": "The Dow Jones Industrial Average posted a modest gain of half a percent or 39 points, ending the day at 8,017.",
        "mention_ids": [
          "M035"
        ]
      },
      "05": {
        "orig_text": "The index has not closed above 8,000 since February.",
        "text": "The index has not closed above 8,000 since February.",
        "mention_ids": [
          "M043"
        ]
      },
      "06": {
        "orig_text": "The last time the Dow had risen for four weeks in a row was between September and October of 2007, when the stock index reached its all-time high of over 14,000.",
        "text": "The last time the Dow had risen for four weeks in a row was between September and October of 2007, when the stock index reached its all-time high of over 14,000.",
        "mention_ids": [
          "M077",
          "M082",
          "M094"
        ]
      },
      "07": {
        "orig_text": "The streak is the index's best since 1993.",
        "text": "The streak is the index's best since 1993.",
        "mention_ids": [
          "M087"
        ]
      },
      "08": {
        "orig_text": "The broader S&P 500 gained eight points, or one percent, by the closing bell.",
        "text": "The broader S&P 500 gained eight points, or one percent, by the closing bell.",
        "mention_ids": [
          "M089"
        ]
      },
      "09": {
        "orig_text": "The Nasdaq Composite had the largest gains of the three indexes, up 1.2% to 1,622.",
        "text": "The Nasdaq Composite had the largest gains of the three indexes, up 1.2% to 1,622.",
        "mention_ids": [
          "M071"
        ]
      },
      "10": {
        "orig_text": "Both the S&P and the Dow gained over 3% over the course of the week, while the Nasdaq was up almost 5%.",
        "text": "Both the S&P and the Dow gained over 3% over the course of the week, while the Nasdaq was up almost 5%.",
        "mention_ids": [
          "M074",
          "M079",
          "M093"
        ]
      },
      "11": {
        "orig_text": "In other markets, both the German DAX and the Japanese Nikkei 225 indexes remained even, whilst the British FTSE 100 slipped 2.3%.",
        "text": "In other markets, both the German DAX and the Japanese Nikkei 225 indexes remained even, whilst the British FTSE 100 slipped 2.3%.",
        "mention_ids": [
          "M073"
        ]
      }
    },
