    "sentences": {
      "00": {
        "orig_text": "Apple Inc., Apple Corps resolve trademark dispute",
        "text": "Apple Inc., Apple Corps resolve trademark dispute",
        "mention_ids": [
          "M013",
          "M015"
        ]
      },
      "01": {
        "orig_text": "February 06, 2007",
        "text": "February 06, 2007",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Apple Inc. announced Monday it has settled its decades-long trademark dispute with the Beatles' record company, Apple Corps.",
        "text": "Apple Inc. announced Monday it has settled its decades-long trademark dispute with the Beatles' record company, Apple Corps.",
        "mention_ids": [
          "M005",
          "M006",
          "M007",
          "M011",
          "M024"
        ]
      },
      "03": {
        "orig_text": "Under this new agreement, Apple Inc. will own all Apple trademarks and will license some to Apple Corps, and will continue to use its name and logo on its iTunes music service.",
        "text": "Under this new agreement, Apple Inc. will own all Apple trademarks and will license some to Apple Corps, and will continue to use its name and logo on its iTunes music service.",
        "mention_ids": [
          "M004",
          "M026",
          "M030",
          "M032",
          "M033",
          "M087"
        ]
      },
      "04": {
        "orig_text": "This new settlement replaces the 1991 agreement between the two companies, which defined the context in which the two companies could use the Apple trademark.",
        "text": "This new settlement replaces the 1991 agreement between the two companies, which defined the context in which the two companies could use the Apple trademark.",
        "mention_ids": [
          "M009",
          "M012",
          "M016"
        ]
      },
      "05": {
        "orig_text": "``We love the Beatles, and it has been painful being at odds with them over these trademarks,`` said Apple Inc. CEO Steve Jobs.",
        "text": "``We love the Beatles, and it has been painful being at odds with them over these trademarks,`` said Apple Inc. CEO Steve Jobs.",
        "mention_ids": [
          "M020",
          "M021",
          "M022",
          "M023",
          "M031"
        ]
      },
      "06": {
        "orig_text": "``It feels great to resolve this in a positive manner, and in a way that should remove the potential of further disagreements in the future.``",
        "text": "``It feels great to resolve this in a positive manner, and in a way that should remove the potential of further disagreements in the future.``",
        "mention_ids": []
      },
      "07": {
        "orig_text": "``The years ahead are going to be very exciting times for us,`` said Neil Aspinall, manager of Apple Corps.",
        "text": "``The years ahead are going to be very exciting times for us,`` said Neil Aspinall, manager of Apple Corps.",
        "mention_ids": [
          "M142",
          "M143"
        ]
      },
      "08": {
        "orig_text": "``We wish Apple Inc. every success and look forward to many years of peaceful co-operation with them.``",
        "text": "``We wish Apple Inc. every success and look forward to many years of peaceful co-operation with them.``",
        "mention_ids": [
          "M141"
        ]
      },
      "09": {
        "orig_text": "It was not disclosed whether the Beatles' music catalog was part of the deal.",
        "text": "It was not disclosed whether the Beatles' music catalog was part of the deal.",
        "mention_ids": []
      }
    },
