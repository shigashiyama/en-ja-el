  "0127461": {
    "doc_info": {
      "title": "127461_Penske_Auto_selected_to_buy_General_Motors",
      "url": "http://en.wikinews.org/wiki/Penske_Auto_selected_to_buy_General_Motors'_Saturn_unit"
    },
    "sentences": {
      "00": {
        "orig_text": "Penske Auto selected to buy General Motors' Saturn unit",
        "text": "Penske Auto selected to buy General Motors' Saturn unit",
        "mention_ids": [
          "M056",
          "M058"
        ]
      },
      "01": {
        "orig_text": "June 05, 2009",
        "text": "June 05, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "General Motors Corporation (GM), an American automaker which has filed for bankruptcy protection, announced on Friday that the Penske Automotive Group (PAG) was selected to purchase Saturn Corporation.",
        "text": "General Motors Corporation (GM), an American automaker which has filed for bankruptcy protection, announced on Friday that the Penske Automotive Group (PAG) was selected to purchase Saturn Corporation.",
        "mention_ids": [
          "M006",
          "M009",
          "M010"
        ]
      },
      "03": {
        "orig_text": "The transaction should be completed in October.",
        "text": "The transaction should be completed in October.",
        "mention_ids": []
      },
      "04": {
        "orig_text": "The purchase includes rights to the Saturn brand, its five current models and its dealership network.",
        "text": "The purchase includes rights to the Saturn brand, its five current models and its dealership network.",
        "mention_ids": [
          "M028",
          "M078"
        ]
      },
      "05": {
        "orig_text": "Two models would be discontinued, the Sky and Astra.",
        "text": "Two models would be discontinued, the Sky and Astra.",
        "mention_ids": [
          "M031",
          "M034"
        ]
      },
      "06": {
        "orig_text": "GM would continue building the Aura sedan, the SUV's, Vue and Outlook for at least two more years.",
        "text": "GM would continue building the Aura sedan, the SUV's, Vue and Outlook for at least two more years.",
        "mention_ids": [
          "M069",
          "M076"
        ]
      },
      "07": {
        "orig_text": "Saturn has 350 dealers across the United States.",
        "text": "Saturn has 350 dealers across the United States.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "The dealers employ more than 13,000 jobs and sell only the Saturn autos.",
        "text": "The dealers employ more than 13,000 jobs and sell only the Saturn autos.",
        "mention_ids": [
          "M070"
        ]
      },
      "09": {
        "orig_text": "Canadian Saturn dealers are not included in the deal.",
        "text": "Canadian Saturn dealers are not included in the deal.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "According to Penske future Saturn vehicles will be fuel economy focused.",
        "text": "According to Penske future Saturn vehicles will be fuel economy focused.",
        "mention_ids": [
          "M075"
        ]
      },
      "11": {
        "orig_text": "An expert indicated that this would move Saturn back to its roots of a entry level car company.",
        "text": "An expert indicated that this would move Saturn back to its roots of a entry level car company.",
        "mention_ids": [
          "M066"
        ]
      },
      "12": {
        "orig_text": "PAG is in talks with several international automakers to replace GM after 2011.",
        "text": "PAG is in talks with several international automakers to replace GM after 2011.",
        "mention_ids": [
          "M088"
        ]
      },
      "13": {
        "orig_text": "Automotive News reports that Renault Samsung Motors of Korea is the most likely candidate.",
        "text": "Automotive News reports that Renault Samsung Motors of Korea is the most likely candidate.",
        "mention_ids": []
      },
      "14": {
        "orig_text": "Penske wants Jill Lajdziak, Saturn's general manager, and Tom LaSorda, former Chrysler President to head up the company when it is independent of GM.",
        "text": "Penske wants Jill Lajdziak, Saturn's general manager, and Tom LaSorda, former Chrysler President to head up the company when it is independent of GM.",
        "mention_ids": [
          "M065",
          "M080"
        ]
      },
      "15": {
        "orig_text": "Serra Automotive in Grand Blanc Township, Genesee County, Michigan, is in talks to take a partial ownership in a new Saturn lead by Penske.",
        "text": "Serra Automotive in Grand Blanc Township, Genesee County, Michigan, is in talks to take a partial ownership in a new Saturn lead by Penske.",
        "mention_ids": []
      }
    },
