    "sentences": {
      "00": {
        "orig_text": "First Airbus A380 delivered",
        "text": "First Airbus A380 delivered",
        "mention_ids": [
          "M008"
        ]
      },
      "01": {
        "orig_text": "October 15, 2007",
        "text": "October 15, 2007",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The first A380 superjumbo, made by Airbus, was delivered today to Singapore Airlines (SIA) 18 months behind schedule.",
        "text": "The first A380 superjumbo, made by Airbus, was delivered today to Singapore Airlines (SIA) 18 months behind schedule.",
        "mention_ids": [
          "M006",
          "M011",
          "M012"
        ]
      },
      "03": {
        "orig_text": "After the plane was delivered in Singapore, it was flown to Toulouse, France for the ceremony of about 500 guests.",
        "text": "After the plane was delivered in Singapore, it was flown to Toulouse, France for the ceremony of about 500 guests.",
        "mention_ids": [
          "M005",
          "M007",
          "M013",
          "M014",
          "M015"
        ]
      },
      "04": {
        "orig_text": "The plane is built with four Rolls-Royce Trent 900 engines.",
        "text": "The plane is built with four Rolls-Royce Trent 900 engines.",
        "mention_ids": [
          "M001",
          "M004"
        ]
      },
      "05": {
        "orig_text": "The first flight will take off for a ``special charity-flight`` from Singapore and land in Sydney on October 25.",
        "text": "The first flight will take off for a ``special charity-flight`` from Singapore and land in Sydney on October 25.",
        "mention_ids": [
          "M002",
          "M003"
        ]
      },
      "06": {
        "orig_text": "Special services will continue from October 28.",
        "text": "Special services will continue from October 28.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "``This is a major milestone for the A380 programme, and I would like to express my deepest appreciation to all parties concerned.",
        "text": "``This is a major milestone for the A380 programme, and I would like to express my deepest appreciation to all parties concerned.",
        "mention_ids": [
          "M062"
        ]
      },
      "08": {
        "orig_text": "I want to thank our teams and all those who worked hard, days, nights, and week-ends, for their dedication and commitment,`` said Airbus President and Chief Executive Officer, Tom Enders.",
        "text": "I want to thank our teams and all those who worked hard, days, nights, and week-ends, for their dedication and commitment,`` said Airbus President and Chief Executive Officer, Tom Enders.",
        "mention_ids": [
          "M074",
          "M077"
        ]
      }
    },
