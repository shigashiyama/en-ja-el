  "0008983": {
    "doc_info": {
      "title": "8983_Boeing_secures_11bn_of_aircraft_deals",
      "url": "http://en.wikinews.org/wiki/Boeing_secures_$11bn_of_aircraft_deals"
    },
    "sentences": {
      "00": {
        "orig_text": "Boeing secures $11bn of aircraft deals",
        "text": "Boeing secures $11bn of aircraft deals",
        "mention_ids": [
          "M017"
        ]
      },
      "01": {
        "orig_text": "April 27, 2005",
        "text": "April 27, 2005",
        "mention_ids": []
      },
      "02": {
        "orig_text": "An American Airlines 777 landing at Heathrow airport Boeing has secured two large orders for new aircraft.",
        "text": "An American Airlines 777 landing at Heathrow airport Boeing has secured two large orders for new aircraft.",
        "mention_ids": [
          "M001",
          "M002",
          "M018",
          "M019"
        ]
      },
      "03": {
        "orig_text": "Air Canada has ordered US$6 billion in new airliners, while Air India has signed a deal for a further US$7 billion.",
        "text": "Air Canada has ordered US$6 billion in new airliners, while Air India has signed a deal for a further US$7 billion.",
        "mention_ids": [
          "M003",
          "M004",
          "M005"
        ]
      },
      "04": {
        "orig_text": "The Air Canada deal includes firm orders for 18 777s and 14 of the new mid-size, long-range 787 Dreamliner.",
        "text": "The Air Canada deal includes firm orders for 18 777s and 14 of the new mid-size, long-range 787 Dreamliner.",
        "mention_ids": [
          "M006",
          "M014"
        ]
      },
      "05": {
        "orig_text": "Options are included for 18 more 777s and 46 more 787s.",
        "text": "Options are included for 18 more 777s and 46 more 787s.",
        "mention_ids": [
          "M013",
          "M015"
        ]
      },
      "06": {
        "orig_text": "The Air India order is worth US$6.8 billion for 50 787s.",
        "text": "The Air India order is worth US$6.8 billion for 50 787s.",
        "mention_ids": [
          "M010",
          "M016"
        ]
      },
      "07": {
        "orig_text": "Air India says the new fuel-efficient airliner will save US$300 million in fuel a year, while Air Canada has estimated a 30% fuel saving over their existing 767s.",
        "text": "Air India says the new fuel-efficient airliner will save US$300 million in fuel a year, while Air Canada has estimated a 30% fuel saving over their existing 767s.",
        "mention_ids": [
          "M101"
        ]
      },
      "08": {
        "orig_text": "Air India have also said that the 20 extra seats the 787 offers over a similarly-sized Airbus A330 was a decisive factor.",
        "text": "Air India have also said that the 20 extra seats the 787 offers over a similarly-sized Airbus A330 was a decisive factor.",
        "mention_ids": [
          "M108"
        ]
      },
      "09": {
        "orig_text": "Korean Air also bought 20 Dreamliners earlier this month.",
        "text": "Korean Air also bought 20 Dreamliners earlier this month.",
        "mention_ids": [
          "M093"
        ]
      },
      "10": {
        "orig_text": "Boeing now has a total of 237 orders and commitments for the 787.",
        "text": "Boeing now has a total of 237 orders and commitments for the 787.",
        "mention_ids": [
          "M090",
          "M121"
        ]
      },
      "11": {
        "orig_text": "The Boeing 777 is a family of long range widebody twin engine airliners built by Boeing Commercial Airplanes.",
        "text": "The Boeing 777 is a family of long range widebody twin engine airliners built by Boeing Commercial Airplanes.",
        "mention_ids": [
          "M094",
          "M167"
        ]
      },
      "12": {
        "orig_text": "It carries between 305 and 550 passengers and has a range from 5,600 to 8,870 nautical miles (10,400 to 16,400 km).",
        "text": "It carries between 305 and 550 passengers and has a range from 5,600 to 8,870 nautical miles (10,400 to 16,400 km).",
        "mention_ids": [
          "M081"
        ]
      },
      "13": {
        "orig_text": "The first flight of the 777 was in 1994.",
        "text": "The first flight of the 777 was in 1994.",
        "mention_ids": [
          "M112"
        ]
      },
      "14": {
        "orig_text": "The Boeing 787, or Dreamliner, is a mid-sized passenger airliner currently under development by Boeing Commercial Airplanes and scheduled to enter service in 2008.",
        "text": "The Boeing 787, or Dreamliner, is a mid-sized passenger airliner currently under development by Boeing Commercial Airplanes and scheduled to enter service in 2008.",
        "mention_ids": [
          "M084",
          "M122"
        ]
      },
      "15": {
        "orig_text": "It will carry between 200 and 350 passengers depending on the seating configuration, and be more fuel-efficient than earlier airliners.",
        "text": "It will carry between 200 and 350 passengers depending on the seating configuration, and be more fuel-efficient than earlier airliners.",
        "mention_ids": [
          "M103"
        ]
      },
      "16": {
        "orig_text": "In addition, it will be the first major airliner to use composite material in the majority of its construction.",
        "text": "In addition, it will be the first major airliner to use composite material in the majority of its construction.",
        "mention_ids": [
          "M078",
          "M086"
        ]
      }
    },
