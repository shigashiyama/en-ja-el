    "sentences": {
      "00": {
        "orig_text": "Apple announces new iPod range",
        "text": "Apple announces new iPod range",
        "mention_ids": [
          "M011",
          "M013"
        ]
      },
      "01": {
        "orig_text": "September 05, 2007",
        "text": "September 05, 2007",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Apple have released several re-designs of their popular iPod range.",
        "text": "Apple have released several re-designs of their popular iPod range.",
        "mention_ids": [
          "M002",
          "M003"
        ]
      },
      "03": {
        "orig_text": "In addition to the iPod Classic, Nano and Shuffle, they have announced the brand new iPod touch, a media player which mimics the iPhone's touch-screen capabilities.",
        "text": "In addition to the iPod Classic, Nano and Shuffle, they have announced the brand new iPod touch, a media player which mimics the iPhone's touch-screen capabilities.",
        "mention_ids": [
          "M004",
          "M005",
          "M006",
          "M008",
          "M009"
        ]
      },
      "04": {
        "orig_text": "The new iPod Classic design features a larger memory space of up to 160GB or 40,000 songs, and has a new all-metal design.",
        "text": "The new iPod Classic design features a larger memory space of up to 160GB or 40,000 songs, and has a new all-metal design.",
        "mention_ids": [
          "M019"
        ]
      },
      "05": {
        "orig_text": "The iPod nano is now smaller and squarer, adding video playback to its features for the first time.",
        "text": "The iPod nano is now smaller and squarer, adding video playback to its features for the first time.",
        "mention_ids": [
          "M001",
          "M020",
          "M022"
        ]
      },
      "06": {
        "orig_text": "The 4GB model is cheaper than its predecessor at $149/#99, but the 8GB is available in five colours at $199/#129.",
        "text": "The 4GB model is cheaper than its predecessor at $149/#99, but the 8GB is available in five colours at $199/#129.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The iPod Shuffle is identical in design to the previous model, but is now available in a new range of five colours, with the same price of $79/#49.",
        "text": "The iPod Shuffle is identical in design to the previous model, but is now available in a new range of five colours, with the same price of $79/#49.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "The iPod touch also features in-built wi-fi technology, enabling users to access the internet and download music from Apple's market-leading on line iTunes music store to their handset.",
        "text": "The iPod touch also features in-built wi-fi technology, enabling users to access the internet and download music from Apple's market-leading on line iTunes music store to their handset.",
        "mention_ids": [
          "M110"
        ]
      },
      "09": {
        "orig_text": "It was announced that Starbucks would offer free wi-fi access to all users of the iPod touch in its cafes.",
        "text": "It was announced that Starbucks would offer free wi-fi access to all users of the iPod touch in its cafes.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "The screen layout, similar to that of the iPhone, offers access to music, video, photos and the iTunes store.",
        "text": "The screen layout, similar to that of the iPhone, offers access to music, video, photos and the iTunes store.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "There are currently two models, at 8GB and 16GB of memory capacity.",
        "text": "There are currently two models, at 8GB and 16GB of memory capacity.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "Apple CEO Steve Jobs also announced that the price of Apple's iPhone, currently only on sale in the US, will be cut by $200 to $399.",
        "text": "Apple CEO Steve Jobs also announced that the price of Apple's iPhone, currently only on sale in the US, will be cut by $200 to $399.",
        "mention_ids": [
          "M108"
        ]
      },
      "13": {
        "orig_text": "He described the new iPod range as 'one of the seven wonders of the world'.",
        "text": "He described the new iPod range as 'one of the seven wonders of the world'.",
        "mention_ids": [
          "M107",
          "M112"
        ]
      }
    },
