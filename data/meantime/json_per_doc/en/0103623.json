    },
    "sentences": {
      "00": {
        "orig_text": "Apple's iTunes replaces Wal-Mart as No. 1 U.S. music seller",
        "text": "Apple's iTunes replaces Wal-Mart as No. 1 U.S. music seller",
        "mention_ids": [
          "M025",
          "M027",
          "M029"
        ]
      },
      "01": {
        "orig_text": "April 09, 2008",
        "text": "April 09, 2008",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Five years after its launch, Apple Computer's iTunes downloadable music service in February passed Wal-Mart to become the No. 1 music seller in the United States, according to figures released April 3 by NPD Group, a market research firm.",
        "text": "Five years after its launch, Apple Computer's iTunes downloadable music service in February passed Wal-Mart to become the No. 1 music seller in the United States, according to figures released April 3 by NPD Group, a market research firm.",
        "mention_ids": [
          "M001",
          "M005",
          "M009",
          "M030",
          "M032",
          "M036"
        ]
      },
      "03": {
        "orig_text": "The firm said more than 4 billion songs had been downloaded from the iTunes store since its launch in 2003 and that as of February, the iTunes store accounts for 70% of all digital music sales.",
        "text": "The firm said more than 4 billion songs had been downloaded from the iTunes store since its launch in 2003 and that as of February, the iTunes store accounts for 70% of all digital music sales.",
        "mention_ids": [
          "M012",
          "M014",
          "M016",
          "M018",
          "M022",
          "M023"
        ]
      },
      "04": {
        "orig_text": "NPD said according to those figures, Apple's iTunes store passes U.S. electronics retailer Best Buy for the No. 2 U.S. music retailer in 2007.",
        "text": "NPD said according to those figures, Apple's iTunes store passes U.S. electronics retailer Best Buy for the No. 2 U.S. music retailer in 2007.",
        "mention_ids": [
          "M003",
          "M007",
          "M028",
          "M035"
        ]
      },
      "05": {
        "orig_text": "According to an April 2nd report in Ars Technica, an internal memo at Apple showed the company passed Wal-Mart as the top U.S. music seller Wal-Mart at the beginning of the year.",
        "text": "According to an April 2nd report in Ars Technica, an internal memo at Apple showed the company passed Wal-Mart as the top U.S. music seller Wal-Mart at the beginning of the year.",
        "mention_ids": [
          "M011",
          "M015",
          "M017",
          "M037",
          "M039"
        ]
      },
      "06": {
        "orig_text": "Their projections showed that as of January 2008, Apple's iTunes held 19 percent of the U.S. music market with Wal-Mart at 15 percent, followed by Best Buy at 13 percent.",
        "text": "Their projections showed that as of January 2008, Apple's iTunes held 19 percent of the U.S. music market with Wal-Mart at 15 percent, followed by Best Buy at 13 percent.",
        "mention_ids": [
          "M158"
        ]
      },
      "07": {
        "orig_text": "Rounding out the top five were Amazon.com and Target at 6 percent each.",
        "text": "Rounding out the top five were Amazon.com and Target at 6 percent each.",
        "mention_ids": []
      }
    },
