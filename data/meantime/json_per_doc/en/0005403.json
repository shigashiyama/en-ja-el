    },
    "sentences": {
      "00": {
        "orig_text": "Boeing allowed to bid for U.S. space launch contracts again",
        "text": "Boeing allowed to bid for U.S. space launch contracts again",
        "mention_ids": [
          "M013",
          "M015"
        ]
      },
      "01": {
        "orig_text": "March 06, 2005",
        "text": "March 06, 2005",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Boeing's Launch Systems, Launch Services and Delta Program business units are to be allowed to bid once more for U.S. space launch contracts, the USAF has announced.",
        "text": "Boeing's Launch Systems, Launch Services and Delta Program business units are to be allowed to bid once more for U.S. space launch contracts, the USAF has announced.",
        "mention_ids": [
          "M012",
          "M014",
          "M064"
        ]
      },
      "03": {
        "orig_text": "Boeing had been suspended since July 24, 2003 after violating federal law.",
        "text": "Boeing had been suspended since July 24, 2003 after violating federal law.",
        "mention_ids": [
          "M016",
          "M017"
        ]
      },
      "04": {
        "orig_text": "The company was discovered to have been in possession of thousands of pages of documentation from Lockheed Martin, Boeing's rival during the 1998 evolved expendable launch vehicle source selection process.",
        "text": "The company was discovered to have been in possession of thousands of pages of documentation from Lockheed Martin, Boeing's rival during the 1998 evolved expendable launch vehicle source selection process.",
        "mention_ids": [
          "M004",
          "M018"
        ]
      },
      "05": {
        "orig_text": "The USAF and Boeing have signed an interim agreement to protect the Air Force's interests.",
        "text": "The USAF and Boeing have signed an interim agreement to protect the Air Force's interests.",
        "mention_ids": [
          "M002",
          "M006",
          "M020"
        ]
      },
      "06": {
        "orig_text": "The agreement allows for the reinstating of the suspension if Boeing is indicted or convicted, or if new evidence is found.",
        "text": "The agreement allows for the reinstating of the suspension if Boeing is indicted or convicted, or if new evidence is found.",
        "mention_ids": [
          "M097"
        ]
      },
      "07": {
        "orig_text": "Boeing will have to submit to outside verification of the remedial steps it has taken, and cannot charge the Air Force for associated costs.",
        "text": "Boeing will have to submit to outside verification of the remedial steps it has taken, and cannot charge the Air Force for associated costs.",
        "mention_ids": [
          "M063",
          "M067",
          "M083"
        ]
      },
      "08": {
        "orig_text": "Internal management responsibilities for ethical practice have been changed.",
        "text": "Internal management responsibilities for ethical practice have been changed.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "Lockheed Martin's Atlas V and Boeing's Delta IV are two types of expendable rocket, designed to reduce the cost of space launches.",
        "text": "Lockheed Martin's Atlas V and Boeing's Delta IV are two types of expendable rocket, designed to reduce the cost of space launches.",
        "mention_ids": [
          "M070"
        ]
      }
    },
