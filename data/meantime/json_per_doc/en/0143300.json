    "sentences": {
      "00": {
        "orig_text": "Automobile manufacturer Ford posts unexpected profits",
        "text": "Automobile manufacturer Ford posts unexpected profits",
        "mention_ids": [
          "M075"
        ]
      },
      "01": {
        "orig_text": "November 03, 2009",
        "text": "November 03, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The US automobile manufacturer Ford has said that it posted a net profit of US$997 million in the third quarter of this year, and predicted that it would be ``solidly profitable`` by 2011.",
        "text": "The US automobile manufacturer Ford has said that it posted a net profit of US$997 million in the third quarter of this year, and predicted that it would be ``solidly profitable`` by 2011.",
        "mention_ids": [
          "M002",
          "M003",
          "M004"
        ]
      },
      "03": {
        "orig_text": "The reports surprised most analysts, who had forecast that the auto company would lose money.",
        "text": "The reports surprised most analysts, who had forecast that the auto company would lose money.",
        "mention_ids": [
          "M020"
        ]
      },
      "04": {
        "orig_text": "Ford's profits equate to 26 cents per share, a large difference to the 12 cent per share lost that had been anticipated by most economic analysts.",
        "text": "Ford's profits equate to 26 cents per share, a large difference to the 12 cent per share lost that had been anticipated by most economic analysts.",
        "mention_ids": [
          "M027",
          "M028",
          "M036",
          "M037"
        ]
      },
      "05": {
        "orig_text": "``Our third-quarter results clearly show Ford is making tremendous progress despite the prolonged economic slump,`` said the firm's chief executive officer, Alan Mulally.",
        "text": "``Our third-quarter results clearly show Ford is making tremendous progress despite the prolonged economic slump,`` said the firm's chief executive officer, Alan Mulally.",
        "mention_ids": [
          "M038",
          "M039",
          "M043"
        ]
      },
      "06": {
        "orig_text": "``While we still face a challenging road ahead, our transformation is working and our underlying business continues to grow stronger.``",
        "text": "``While we still face a challenging road ahead, our transformation is working and our underlying business continues to grow stronger.``",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Mulally predicted that his company would meet or even surpass all of its financial targets, and suggested that the fourth quarter of this year could see it post an operating profit.",
        "text": "Mulally predicted that his company would meet or even surpass all of its financial targets, and suggested that the fourth quarter of this year could see it post an operating profit.",
        "mention_ids": [
          "M093",
          "M104"
        ]
      }
    },
