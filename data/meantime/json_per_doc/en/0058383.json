    "sentences": {
      "00": {
        "orig_text": "Cisco sues Apple for iPhone trademark",
        "text": "Cisco sues Apple for iPhone trademark",
        "mention_ids": [
          "M007",
          "M009",
          "M011"
        ]
      },
      "01": {
        "orig_text": "January 12, 2007",
        "text": "January 12, 2007",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The iPhone only made its appearance as a prototype and there have been controversies aroused.",
        "text": "The iPhone only made its appearance as a prototype and there have been controversies aroused.",
        "mention_ids": [
          "M015",
          "M016",
          "M017"
        ]
      },
      "03": {
        "orig_text": "The dispute has come up between the manufacturer of the iPhone (which was resented on Wednesday for the first time) - Apple Inc. - and a leader in network and communication systems, based in San Jose - Cisco.",
        "text": "The dispute has come up between the manufacturer of the iPhone (which was resented on Wednesday for the first time) - Apple Inc. - and a leader in network and communication systems, based in San Jose - Cisco.",
        "mention_ids": [
          "M075"
        ]
      },
      "04": {
        "orig_text": "The company claims to possess the trademark for iPhone, and moreover, that it sells devices under the same brand through one of its divisions.",
        "text": "The company claims to possess the trademark for iPhone, and moreover, that it sells devices under the same brand through one of its divisions.",
        "mention_ids": [
          "M003",
          "M004",
          "M006",
          "M008",
          "M010",
          "M012"
        ]
      },
      "05": {
        "orig_text": "This became the reason for Cisco to file a lawsuit against Apple Inc. so that the latter would stop selling the device.",
        "text": "This became the reason for Cisco to file a lawsuit against Apple Inc. so that the latter would stop selling the device.",
        "mention_ids": [
          "M018",
          "M019",
          "M020",
          "M021"
        ]
      },
      "06": {
        "orig_text": "Cisco states that it has received the trademark in 2000, when the company overtook Infogear Technology Corp., which took place in 1996.",
        "text": "Cisco states that it has received the trademark in 2000, when the company overtook Infogear Technology Corp., which took place in 1996.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The Vice President and general counsel of the company, Mark Chandler, explained that there was no doubt about the excitement of the new device from Apple, but they should not use a trademark, which belongs to Cisco.",
        "text": "The Vice President and general counsel of the company, Mark Chandler, explained that there was no doubt about the excitement of the new device from Apple, but they should not use a trademark, which belongs to Cisco.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "The iPhone developed by Cisco is a device which allows users to make phone calls over the voice over Internet protocol (VoIP).",
        "text": "The iPhone developed by Cisco is a device which allows users to make phone calls over the voice over Internet protocol (VoIP).",
        "mention_ids": []
      }
    },
