    "sentences": {
      "00": {
        "orig_text": "Apple introduces iPhone and Apple TV",
        "text": "Apple introduces iPhone and Apple TV",
        "mention_ids": [
          "M014",
          "M020"
        ]
      },
      "01": {
        "orig_text": "January 10, 2007",
        "text": "January 10, 2007",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Apple Inc. today has introduced the much-anticipated iPhone at the Macworld Conference in San Francisco.",
        "text": "Apple Inc. today has introduced the much-anticipated iPhone at the Macworld Conference in San Francisco.",
        "mention_ids": [
          "M002",
          "M006",
          "M008"
        ]
      },
      "03": {
        "orig_text": "The iPhone is claimed to be ``a revolutionary mobile phone`` as stated on the Apple website.",
        "text": "The iPhone is claimed to be ``a revolutionary mobile phone`` as stated on the Apple website.",
        "mention_ids": [
          "M010",
          "M011",
          "M012"
        ]
      },
      "04": {
        "orig_text": "The device appears to be running a mobile version of the Apple operating system Mac OSX.",
        "text": "The device appears to be running a mobile version of the Apple operating system Mac OSX.",
        "mention_ids": [
          "M013",
          "M015"
        ]
      },
      "05": {
        "orig_text": "It is approximately the same size as a 5th generation iPod, it has a 3.5-inch LCD touchscreen display that is used to access all features of the phone including number dial, as well as making phone calls.",
        "text": "It is approximately the same size as a 5th generation iPod, it has a 3.5-inch LCD touchscreen display that is used to access all features of the phone including number dial, as well as making phone calls.",
        "mention_ids": [
          "M003",
          "M021",
          "M022",
          "M024"
        ]
      },
      "06": {
        "orig_text": "The iPhone plays music, movies, displays pictures and is able to connect to a wireless network.",
        "text": "The iPhone plays music, movies, displays pictures and is able to connect to a wireless network.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Apple CEO Steve Jobs unveiled the device by walking onto the stage and taking the iPhone out of his jeans pocket.",
        "text": "Apple CEO Steve Jobs unveiled the device by walking onto the stage and taking the iPhone out of his jeans pocket.",
        "mention_ids": [
          "M127",
          "M128"
        ]
      },
      "08": {
        "orig_text": "During his 2 hour speech he stated that ``Today Apple is going to reinvent the phone, We are going to make history today``.",
        "text": "During his 2 hour speech he stated that ``Today Apple is going to reinvent the phone, We are going to make history today``.",
        "mention_ids": [
          "M123",
          "M124"
        ]
      },
      "09": {
        "orig_text": "Today Apple also released their Media Center device - Apple TV.",
        "text": "Today Apple also released their Media Center device - Apple TV.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "It will directly compete with Microsoft's Media Center operating system.",
        "text": "It will directly compete with Microsoft's Media Center operating system.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "Apple has taken a different approach to the media center market; rather than storing content (such as movies, music and photos) on the device, Apple TV connects to a computer (Mac and Windows) over a wirless network connection and plays all content stored on that computer.",
        "text": "Apple has taken a different approach to the media center market; rather than storing content (such as movies, music and photos) on the device, Apple TV connects to a computer (Mac and Windows) over a wirless network connection and plays all content stored on that computer.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "This makes it substantially easier for users to organize their media content.",
        "text": "This makes it substantially easier for users to organize their media content.",
        "mention_ids": []
      }
    },
