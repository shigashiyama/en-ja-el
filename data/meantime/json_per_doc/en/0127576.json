    "sentences": {
      "00": {
        "orig_text": "Apple launches new, faster iPhone",
        "text": "Apple launches new, faster iPhone",
        "mention_ids": [
          "M011",
          "M013"
        ]
      },
      "01": {
        "orig_text": "June 09, 2009",
        "text": "June 09, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Apple Computer today announced its latest iteration of their popular iPhone, the iPhone 3GS.",
        "text": "Apple Computer today announced its latest iteration of their popular iPhone, the iPhone 3GS.",
        "mention_ids": [
          "M023",
          "M030"
        ]
      },
      "03": {
        "orig_text": "The new hardware, revealed at the company's annual Worldwide Developers Conference drew attention from the media and iPhone owners alike.",
        "text": "The new hardware, revealed at the company's annual Worldwide Developers Conference drew attention from the media and iPhone owners alike.",
        "mention_ids": [
          "M006",
          "M033"
        ]
      },
      "04": {
        "orig_text": "Features new to the iPhone include an upgraded camera, which also allows users to record video and sound, as well as ``voice control``, which will allow users to control most features of the iPhone with their voice.",
        "text": "Features new to the iPhone include an upgraded camera, which also allows users to record video and sound, as well as ``voice control``, which will allow users to control most features of the iPhone with their voice.",
        "mention_ids": [
          "M017",
          "M018",
          "M020",
          "M024",
          "M028",
          "M032",
          "M034"
        ]
      },
      "05": {
        "orig_text": "The new phone will also come pre-loaded with the new iPhone 3.0 software, which will be available on June 17, two days before the phone launches.",
        "text": "The new phone will also come pre-loaded with the new iPhone 3.0 software, which will be available on June 17, two days before the phone launches.",
        "mention_ids": [
          "M008",
          "M036"
        ]
      },
      "06": {
        "orig_text": "In addition to the new iPhone hardware, Apple also demonstrated some of the features of its new desktop operating system, Snow Leopard, and highlighted the new Safari 4.0 web browser.",
        "text": "In addition to the new iPhone hardware, Apple also demonstrated some of the features of its new desktop operating system, Snow Leopard, and highlighted the new Safari 4.0 web browser.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "The iPhone 3GS will be available in a 16 GB and 32 GB models, retailing for US$199 and $299 respectively.",
        "text": "The iPhone 3GS will be available in a 16 GB and 32 GB models, retailing for US$199 and $299 respectively.",
        "mention_ids": []
      }
    },
