    "sentences": {
      "00": {
        "orig_text": "Japan enters recession",
        "text": "Japan enters recession",
        "mention_ids": [
          "M001"
        ]
      },
      "01": {
        "orig_text": "November 18, 2008",
        "text": "November 18, 2008",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Japan, which accounts for 7% of world output, has entered a recession after posting two consecutive quarters of negative growth.",
        "text": "Japan, which accounts for 7% of world output, has entered a recession after posting two consecutive quarters of negative growth.",
        "mention_ids": [
          "M005",
          "M006",
          "M053"
        ]
      },
      "03": {
        "orig_text": "The Japanese economy contracted by 0.9% between April and June and a further 0.1% since September.",
        "text": "The Japanese economy contracted by 0.9% between April and June and a further 0.1% since September.",
        "mention_ids": [
          "M014"
        ]
      },
      "04": {
        "orig_text": "Kaoru Yosano, the Japanese minister of fiscal policy, said ``The downtrend in the economy will continue for the time being as global growth slows down.",
        "text": "Kaoru Yosano, the Japanese minister of fiscal policy, said ``The downtrend in the economy will continue for the time being as global growth slows down.",
        "mention_ids": [
          "M023",
          "M029"
        ]
      },
      "05": {
        "orig_text": "We need to bear in mind that economic conditions could worsen further as the US and European financial crisis deepens, worries of economic downturn heighten and stock and foreign exchange markets make big swings.``",
        "text": "We need to bear in mind that economic conditions could worsen further as the US and European financial crisis deepens, worries of economic downturn heighten and stock and foreign exchange markets make big swings.``",
        "mention_ids": [
          "M034",
          "M041",
          "M046",
          "M090"
        ]
      },
      "06": {
        "orig_text": "The Nikkei 225 stock index has fallen 40% since the beginning of the year, and has lost a quarter of its value during this month.",
        "text": "The Nikkei 225 stock index has fallen 40% since the beginning of the year, and has lost a quarter of its value during this month.",
        "mention_ids": [
          "M091"
        ]
      },
      "07": {
        "orig_text": "After the news was announced the index dipped, then rose to close up 0.7%.",
        "text": "After the news was announced the index dipped, then rose to close up 0.7%.",
        "mention_ids": [
          "M110"
        ]
      },
      "08": {
        "orig_text": "Japanese companies lose large sums whenever the yen's value surges.",
        "text": "Japanese companies lose large sums whenever the yen's value surges.",
        "mention_ids": []
      },
      "09": {
        "orig_text": "With the yen viewed as relatively stable during financial turmoil, the value against the US dollar has risen greatly.",
        "text": "With the yen viewed as relatively stable during financial turmoil, the value against the US dollar has risen greatly.",
        "mention_ids": [
          "M109"
        ]
      },
      "10": {
        "orig_text": "JPMorgan Chase economist Masamichi Adachi said, ``It's only going to get worse.",
        "text": "JPMorgan Chase economist Masamichi Adachi said, ``It's only going to get worse.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "Japan may be entering its deepest recession in a decade as the global financial crisis cools demand.``",
        "text": "Japan may be entering its deepest recession in a decade as the global financial crisis cools demand.``",
        "mention_ids": []
      },
      "12": {
        "orig_text": "Japan has not been in recession since 2001.",
        "text": "Japan has not been in recession since 2001.",
        "mention_ids": []
      }
    },
