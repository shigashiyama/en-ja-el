    "sentences": {
      "00": {
        "orig_text": "Boeing 787 ``Dreamliner`` makes maiden flight",
        "text": "Boeing 787 ``Dreamliner`` makes maiden flight",
        "mention_ids": [
          "M003"
        ]
      },
      "01": {
        "orig_text": "December 16, 2009",
        "text": "December 16, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Boeing's newest commercial aircraft, the 787 ``Dreamliner``, made its first test flight Tuesday in Everett, Washington at 18:00 UTC.",
        "text": "Boeing's newest commercial aircraft, the 787 ``Dreamliner``, made its first test flight Tuesday in Everett, Washington at 18:00 UTC.",
        "mention_ids": [
          "M001",
          "M002",
          "M005"
        ]
      },
      "03": {
        "orig_text": "Boeing claims that the new 290+ seat wide-body, twin engine jetliner is more fuel efficient than previous Boeing models.",
        "text": "Boeing claims that the new 290+ seat wide-body, twin engine jetliner is more fuel efficient than previous Boeing models.",
        "mention_ids": [
          "M007",
          "M008",
          "M010"
        ]
      },
      "04": {
        "orig_text": "The release of the ``Dreamliner`` is almost two and a half years behind schedule.",
        "text": "The release of the ``Dreamliner`` is almost two and a half years behind schedule.",
        "mention_ids": [
          "M011"
        ]
      },
      "05": {
        "orig_text": "The project has been plagued by various issues, including strike action, parts shortages and design problems.",
        "text": "The project has been plagued by various issues, including strike action, parts shortages and design problems.",
        "mention_ids": []
      },
      "06": {
        "orig_text": "The aircraft is thought to have attracted 840 orders to date, though some have been cancelled due to the delays, making the 787 worth in the region of US$140bn ($96bn, #86bn) for Boeing.",
        "text": "The aircraft is thought to have attracted 840 orders to date, though some have been cancelled due to the delays, making the 787 worth in the region of US$140bn ($96bn, #86bn) for Boeing.",
        "mention_ids": [
          "M058",
          "M064",
          "M102"
        ]
      },
      "07": {
        "orig_text": "An aerospace analyst, Richard Aboulafia, said that ``[The test flight] will provide a badly needed perception that the program is on some kind of schedule again, but it's still a long way from the ultimate result.``",
        "text": "An aerospace analyst, Richard Aboulafia, said that ``[The test flight] will provide a badly needed perception that the program is on some kind of schedule again, but it's still a long way from the ultimate result.``",
        "mention_ids": []
      },
      "08": {
        "orig_text": "Boeing's European rival, Airbus, is preparing to release a similar aircraft, the A350, in 2013.",
        "text": "Boeing's European rival, Airbus, is preparing to release a similar aircraft, the A350, in 2013.",
        "mention_ids": [
          "M070",
          "M075"
        ]
      }
    },
