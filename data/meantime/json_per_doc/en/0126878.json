    "sentences": {
      "00": {
        "orig_text": "Bankruptcy for U.S. automaker GM becomes almost certain after bondholder offers fail",
        "text": "Bankruptcy for U.S. automaker GM becomes almost certain after bondholder offers fail",
        "mention_ids": [
          "M098",
          "M099"
        ]
      },
      "01": {
        "orig_text": "May 28, 2009",
        "text": "May 28, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "The United States automobile manufacturing firm General Motors announced on Wednesday that most of its bondholders did not exchange GM's US$27 billion debt for a ten percent share in the company's stock.",
        "text": "The United States automobile manufacturing firm General Motors announced on Wednesday that most of its bondholders did not exchange GM's US$27 billion debt for a ten percent share in the company's stock.",
        "mention_ids": [
          "M004",
          "M006",
          "M008",
          "M011"
        ]
      },
      "03": {
        "orig_text": "The automaker, in financial straits, has a June 1 deadline to finish a government restructuring plan that includes plant closures and other debt reduction measures.",
        "text": "The automaker, in financial straits, has a June 1 deadline to finish a government restructuring plan that includes plant closures and other debt reduction measures.",
        "mention_ids": [
          "M019",
          "M020",
          "M021",
          "M029"
        ]
      },
      "04": {
        "orig_text": "U.S. President Barack Obama's administration said it would not give more financial aid to the firm unless 90% of GM's bondholders would agree on compromises that would significantly reduce the firm's costs.",
        "text": "U.S. President Barack Obama's administration said it would not give more financial aid to the firm unless 90% of GM's bondholders would agree on compromises that would significantly reduce the firm's costs.",
        "mention_ids": [
          "M035",
          "M036",
          "M037",
          "M038",
          "M040",
          "M041"
        ]
      },
      "05": {
        "orig_text": "``The principal amount of notes tendered was substantially less than the amount required by GM to satisfy the debt reduction requirement,`` GM said in a statement.",
        "text": "``The principal amount of notes tendered was substantially less than the amount required by GM to satisfy the debt reduction requirement,`` GM said in a statement.",
        "mention_ids": [
          "M050",
          "M051",
          "M052",
          "M053"
        ]
      },
      "06": {
        "orig_text": "``They said no.",
        "text": "``They said no.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "That's it.",
        "text": "That's it.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "They tried.",
        "text": "They tried.",
        "mention_ids": [
          "M115"
        ]
      },
      "09": {
        "orig_text": "That's why they're going to have to file for bankruptcy,`` said a university professor from the University of Michigan who specializes in bankruptcy.",
        "text": "That's why they're going to have to file for bankruptcy,`` said a university professor from the University of Michigan who specializes in bankruptcy.",
        "mention_ids": [
          "M139"
        ]
      }
    },
