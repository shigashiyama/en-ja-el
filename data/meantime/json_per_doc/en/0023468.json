    },
    "sentences": {
      "00": {
        "orig_text": "Apple plans another special event",
        "text": "Apple plans another special event",
        "mention_ids": [
          "M005"
        ]
      },
      "01": {
        "orig_text": "October 04, 2005",
        "text": "October 04, 2005",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Within one month of the iPod nano and iTunes phone special event, Apple Computer announced today another special event to be held on October 12.",
        "text": "Within one month of the iPod nano and iTunes phone special event, Apple Computer announced today another special event to be held on October 12.",
        "mention_ids": [
          "M004",
          "M006"
        ]
      },
      "03": {
        "orig_text": "It is to be held at the California Theater in downtown San Jose, California.",
        "text": "It is to be held at the California Theater in downtown San Jose, California.",
        "mention_ids": [
          "M007"
        ]
      },
      "04": {
        "orig_text": "The invitation reads, ``One more thing...``, the teasing tagline of Steve Jobs.",
        "text": "The invitation reads, ``One more thing...``, the teasing tagline of Steve Jobs.",
        "mention_ids": [
          "M010",
          "M011"
        ]
      },
      "05": {
        "orig_text": "The words were atop a picture of closed theater curtains.",
        "text": "The words were atop a picture of closed theater curtains.",
        "mention_ids": [
          "M001"
        ]
      },
      "06": {
        "orig_text": "Recently Apple has been gaining rights to sell music videos from record labels.",
        "text": "Recently Apple has been gaining rights to sell music videos from record labels.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "This has increased rumors of a video iPod(vPod) along with this event.",
        "text": "This has increased rumors of a video iPod(vPod) along with this event.",
        "mention_ids": [
          "M070"
        ]
      },
      "08": {
        "orig_text": "AppleInsider is claiming that there are sources that have seen the new iPod.",
        "text": "AppleInsider is claiming that there are sources that have seen the new iPod.",
        "mention_ids": [
          "M071"
        ]
      },
      "09": {
        "orig_text": "Alongside this rumor, AppleInsider claims to have seen a wireless device similar to Apple's Airport Express that supports video.",
        "text": "Alongside this rumor, AppleInsider claims to have seen a wireless device similar to Apple's Airport Express that supports video.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "Other rumors for this event include a major iTunes Music Store update and a Macintosh with a Intel processor.",
        "text": "Other rumors for this event include a major iTunes Music Store update and a Macintosh with a Intel processor.",
        "mention_ids": [
          "M072"
        ]
      }
    },
