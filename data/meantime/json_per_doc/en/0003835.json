  "0003835": {
    "doc_info": {
      "title": "3835_Chinese_airlines_agree_purchase_of_Boeing_787_Dreamliners",
      "url": "http://en.wikinews.org/wiki/Chinese_airlines_agree_purchase_of_Boeing_787_Dreamliners_worth_US$7.2bn"
    },
    "sentences": {
      "00": {
        "orig_text": "Chinese airlines agree purchase of Boeing 787 Dreamliners worth US$7.2bn",
        "text": "Chinese airlines agree purchase of Boeing 787 Dreamliners worth US$7.2bn",
        "mention_ids": [
          "M016",
          "M018"
        ]
      },
      "01": {
        "orig_text": "January 29, 2005",
        "text": "January 29, 2005",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Officials from the People's Republic of China have agreed to purchase 60 Boeing 787 Dreamliner aircraft in a deal worth US$7.2bn.",
        "text": "Officials from the People's Republic of China have agreed to purchase 60 Boeing 787 Dreamliner aircraft in a deal worth US$7.2bn.",
        "mention_ids": [
          "M011",
          "M013"
        ]
      },
      "03": {
        "orig_text": "In China, airliners are selected and purchased by the government, which then distributes them to different airlines.",
        "text": "In China, airliners are selected and purchased by the government, which then distributes them to different airlines.",
        "mention_ids": [
          "M015",
          "M017",
          "M019"
        ]
      },
      "04": {
        "orig_text": "The preliminary agreement will see Dreamliners delivered to six airlines - Air China, China Eastern Airlines, China Southern Airlines, Hainan Airlines, Shanghai Airlines and Xiamen Airlines.",
        "text": "The preliminary agreement will see Dreamliners delivered to six airlines - Air China, China Eastern Airlines, China Southern Airlines, Hainan Airlines, Shanghai Airlines and Xiamen Airlines.",
        "mention_ids": [
          "M002",
          "M003"
        ]
      },
      "05": {
        "orig_text": "The deliveries are scheduled to be in time for the 2008 Beijing Olympic Games.",
        "text": "The deliveries are scheduled to be in time for the 2008 Beijing Olympic Games.",
        "mention_ids": []
      },
      "06": {
        "orig_text": "The aircraft was once named the 7E7 but was renamed at the same time the deal was announced.",
        "text": "The aircraft was once named the 7E7 but was renamed at the same time the deal was announced.",
        "mention_ids": [
          "M082"
        ]
      },
      "07": {
        "orig_text": "Boeing will begin production in 2006 and the aircraft should enter airline service in 2008.",
        "text": "Boeing will begin production in 2006 and the aircraft should enter airline service in 2008.",
        "mention_ids": [
          "M094",
          "M108"
        ]
      },
      "08": {
        "orig_text": "It will seat 200 to 300 people and will have a range of 3,500 to 8,500 nautical miles.",
        "text": "It will seat 200 to 300 people and will have a range of 3,500 to 8,500 nautical miles.",
        "mention_ids": [
          "M091"
        ]
      },
      "09": {
        "orig_text": "Through the use of the latest materials and designs it will be 20% more fuel efficient than the equivalent aircraft today.",
        "text": "Through the use of the latest materials and designs it will be 20% more fuel efficient than the equivalent aircraft today.",
        "mention_ids": [
          "M081"
        ]
      }
    },
