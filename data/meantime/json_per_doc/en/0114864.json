    "sentences": {
      "00": {
        "orig_text": "Global markets plunge",
        "text": "Global markets plunge",
        "mention_ids": [
          "M047"
        ]
      },
      "01": {
        "orig_text": "October 10, 2008",
        "text": "October 10, 2008",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Stock markets across the world have fallen sharply with several seeing the biggest drop in their history.",
        "text": "Stock markets across the world have fallen sharply with several seeing the biggest drop in their history.",
        "mention_ids": [
          "M002",
          "M004",
          "M007"
        ]
      },
      "03": {
        "orig_text": "Asian markets saw the biggest sell-off.",
        "text": "Asian markets saw the biggest sell-off.",
        "mention_ids": [
          "M008"
        ]
      },
      "04": {
        "orig_text": "The Nikkei dropped 9.62% to reach a 20 year low.",
        "text": "The Nikkei dropped 9.62% to reach a 20 year low.",
        "mention_ids": [
          "M011"
        ]
      },
      "05": {
        "orig_text": "Japan also saw a collapse of a mid-size insurance company, Yamato Life Insurance Company, which declared bankruptcy.",
        "text": "Japan also saw a collapse of a mid-size insurance company, Yamato Life Insurance Company, which declared bankruptcy.",
        "mention_ids": [
          "M020",
          "M021",
          "M038"
        ]
      },
      "06": {
        "orig_text": "The Hang Seng, which was one of the few markets that was positive yesterday, fell 7.19%.",
        "text": "The Hang Seng, which was one of the few markets that was positive yesterday, fell 7.19%.",
        "mention_ids": []
      },
      "07": {
        "orig_text": "Australia dropped by 8.4% and South Korea saw a 9% fall.",
        "text": "Australia dropped by 8.4% and South Korea saw a 9% fall.",
        "mention_ids": []
      },
      "08": {
        "orig_text": "In Europe, markets dropped at the open with the FTSE losing 11%.",
        "text": "In Europe, markets dropped at the open with the FTSE losing 11%.",
        "mention_ids": [
          "M058"
        ]
      },
      "09": {
        "orig_text": "They have recovered only sightly with all European markets losing more than 5%.",
        "text": "They have recovered only sightly with all European markets losing more than 5%.",
        "mention_ids": []
      },
      "10": {
        "orig_text": "The European sell off was more about the Asian lows then any specific news.",
        "text": "The European sell off was more about the Asian lows then any specific news.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "European banks and financial institutes saw the most selling.",
        "text": "European banks and financial institutes saw the most selling.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "Also, oil related companies saw large drops as an result of an expected decrease in oil consumption.",
        "text": "Also, oil related companies saw large drops as an result of an expected decrease in oil consumption.",
        "mention_ids": []
      },
      "13": {
        "orig_text": "The U.S. markets opened lower with the Dow Jones Industrial Average falling below 8,000, before recovering slightly.",
        "text": "The U.S. markets opened lower with the Dow Jones Industrial Average falling below 8,000, before recovering slightly.",
        "mention_ids": [
          "M059",
          "M073"
        ]
      },
      "14": {
        "orig_text": "President George W. Bush made an address on the economy and said markets were being ``driven by uncertainty and fear.``",
        "text": "President George W. Bush made an address on the economy and said markets were being ``driven by uncertainty and fear.``",
        "mention_ids": [
          "M063"
        ]
      },
      "15": {
        "orig_text": "Oil has seen losses of more than US$6 in trading with the current price of a barrel of oil less than $80.",
        "text": "Oil has seen losses of more than US$6 in trading with the current price of a barrel of oil less than $80.",
        "mention_ids": []
      },
      "16": {
        "orig_text": "This is a year low for oil.",
        "text": "This is a year low for oil.",
        "mention_ids": []
      },
      "17": {
        "orig_text": "News also came out that OPEC will hold an emergency meeting on November 18 to discuss the falling price of oil.",
        "text": "News also came out that OPEC will hold an emergency meeting on November 18 to discuss the falling price of oil.",
        "mention_ids": []
      },
      "18": {
        "orig_text": "Charities, such as Cats Protection, today said that they have lost much of their funds in collapsing banks.",
        "text": "Charities, such as Cats Protection, today said that they have lost much of their funds in collapsing banks.",
        "mention_ids": []
      },
      "19": {
        "orig_text": "Cats Protection had a total of #11.2 million saved in the now-collapsed Kaupthing bank.",
        "text": "Cats Protection had a total of #11.2 million saved in the now-collapsed Kaupthing bank.",
        "mention_ids": []
      },
      "20": {
        "orig_text": "The British National Council for Voluntary Organisations said that 60 of its 6,500 have lost money due to the collapse of banks.",
        "text": "The British National Council for Voluntary Organisations said that 60 of its 6,500 have lost money due to the collapse of banks.",
        "mention_ids": []
      }
    },
