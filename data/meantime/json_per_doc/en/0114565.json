    "sentences": {
      "00": {
        "orig_text": "Worldwide markets fall precipitously",
        "text": "Worldwide markets fall precipitously",
        "mention_ids": [
          "M046"
        ]
      },
      "01": {
        "orig_text": "October 06, 2008",
        "text": "October 06, 2008",
        "mention_ids": []
      },
      "02": {
        "orig_text": "Stock markets around the world have fallen dramatically today.",
        "text": "Stock markets around the world have fallen dramatically today.",
        "mention_ids": [
          "M002"
        ]
      },
      "03": {
        "orig_text": "This is following the ongoing events in the financial world, including the US Government's $700 billion bail out of the financial sector.",
        "text": "This is following the ongoing events in the financial world, including the US Government's $700 billion bail out of the financial sector.",
        "mention_ids": [
          "M008",
          "M009",
          "M011"
        ]
      },
      "04": {
        "orig_text": "As of 14:48 UTC, the primary UK index, the FTSE 100, dropped in value by 6.50% (323.65 points) to a point even further below the 5000 mark at 4656.60.",
        "text": "As of 14:48 UTC, the primary UK index, the FTSE 100, dropped in value by 6.50% (323.65 points) to a point even further below the 5000 mark at 4656.60.",
        "mention_ids": [
          "M016"
        ]
      },
      "05": {
        "orig_text": "The Dow Jones, was down 3.76% at 16:08 UTC, a slight increase from earlier today.",
        "text": "The Dow Jones, was down 3.76% at 16:08 UTC, a slight increase from earlier today.",
        "mention_ids": [
          "M022"
        ]
      },
      "06": {
        "orig_text": "The Dow Jones currently has a value of 9936.94 points, below the ten thousand mark.",
        "text": "The Dow Jones currently has a value of 9936.94 points, below the ten thousand mark.",
        "mention_ids": [
          "M058"
        ]
      },
      "07": {
        "orig_text": "The Nasdaq index has fallen by 100.12 points to 1847.27, while the DAX was 6.62% lower than the start of the day as of 16:08 UTC.",
        "text": "The Nasdaq index has fallen by 100.12 points to 1847.27, while the DAX was 6.62% lower than the start of the day as of 16:08 UTC.",
        "mention_ids": [
          "M065"
        ]
      },
      "08": {
        "orig_text": "The Dow Jones index was one that fared particularly poorly today, as not one of its companies increased its share price.",
        "text": "The Dow Jones index was one that fared particularly poorly today, as not one of its companies increased its share price.",
        "mention_ids": [
          "M060",
          "M072"
        ]
      },
      "09": {
        "orig_text": "The same is true for the CAC 40 index.",
        "text": "The same is true for the CAC 40 index.",
        "mention_ids": [
          "M055"
        ]
      },
      "10": {
        "orig_text": "The Merval index is another one that fared badly today.",
        "text": "The Merval index is another one that fared badly today.",
        "mention_ids": []
      },
      "11": {
        "orig_text": "It dropped in value by 10.12%, while the affiliated Merval 25 index dropped by nearly as much, 10.03%.",
        "text": "It dropped in value by 10.12%, while the affiliated Merval 25 index dropped by nearly as much, 10.03%.",
        "mention_ids": []
      },
      "12": {
        "orig_text": "One of the worst faring indexes of the day was the Brazilian Bovespa index.",
        "text": "One of the worst faring indexes of the day was the Brazilian Bovespa index.",
        "mention_ids": []
      },
      "13": {
        "orig_text": "It has already fallen by 14.45% today, despite the fact that it is not even half way through the trading day.",
        "text": "It has already fallen by 14.45% today, despite the fact that it is not even half way through the trading day.",
        "mention_ids": []
      }
