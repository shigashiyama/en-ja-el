    },
    "sentences": {
      "00": {
        "orig_text": "GM, Chrysler offer buyouts and early retirement to workers",
        "text": "GM, Chrysler offer buyouts and early retirement to workers",
        "mention_ids": [
          "M069",
          "M070",
          "M072"
        ]
      },
      "01": {
        "orig_text": "February 03, 2009",
        "text": "February 03, 2009",
        "mention_ids": []
      },
      "02": {
        "orig_text": "General Motors (GM) and Chrysler have both begun to offer layoff packages to their workforces.",
        "text": "General Motors (GM) and Chrysler have both begun to offer layoff packages to their workforces.",
        "mention_ids": [
          "M005",
          "M006",
          "M052",
          "M054"
        ]
      },
      "03": {
        "orig_text": "The automobile manufacturers have been hard hit in the recent economic downturn and have been forced to seek federal aid from the U.S. government.",
        "text": "The automobile manufacturers have been hard hit in the recent economic downturn and have been forced to seek federal aid from the U.S. government.",
        "mention_ids": [
          "M010",
          "M011",
          "M055"
        ]
      },
      "04": {
        "orig_text": "Reports say that GM's package includes a $20,000 cash payment and a $25,000 new vehicle voucher.",
        "text": "Reports say that GM's package includes a $20,000 cash payment and a $25,000 new vehicle voucher.",
        "mention_ids": [
          "M019",
          "M020",
          "M058"
        ]
      },
      "05": {
        "orig_text": "Chrysler will offer a $25,000 vehicle voucher and $50,000 with healthcare and $75,000 without.",
        "text": "Chrysler will offer a $25,000 vehicle voucher and $50,000 with healthcare and $75,000 without.",
        "mention_ids": [
          "M027",
          "M063"
        ]
      },
      "06": {
        "orig_text": "Both will offer the deal to most United Auto Workers (UAW) union members - 62,000 at GM, which is seeking to cut 31,500 jobs by 2012.",
        "text": "Both will offer the deal to most United Auto Workers (UAW) union members - 62,000 at GM, which is seeking to cut 31,500 jobs by 2012.",
        "mention_ids": [
          "M075",
          "M077",
          "M126",
          "M137"
        ]
      },
      "07": {
        "orig_text": "The two companies have received $13.4 billion in federal loans to keep them operating, but Congress required them to produce viability plans to demonstrate they were making significant cost cuts and labor concessions in return for the money.",
        "text": "The two companies have received $13.4 billion in federal loans to keep them operating, but Congress required them to produce viability plans to demonstrate they were making significant cost cuts and labor concessions in return for the money.",
        "mention_ids": [
          "M085",
          "M106",
          "M125"
        ]
      },
      "08": {
        "orig_text": "UAW workers in Detroit earn $28 an hour; their replacements will earn about half that.",
        "text": "UAW workers in Detroit earn $28 an hour; their replacements will earn about half that.",
        "mention_ids": [
          "M095"
        ]
      },
      "09": {
        "orig_text": "The UAW's ``jobs bank``, a system where workers without duties are still paid, has stopped at both companies.",
        "text": "The UAW's ``jobs bank``, a system where workers without duties are still paid, has stopped at both companies.",
        "mention_ids": [
          "M134"
        ]
      },
      "10": {
        "orig_text": "GM is also attempting to engineer a debt-for-equity swap, reducing its liabilities from $27.5 billion in unsecured debt to $9.2 billion.",
        "text": "GM is also attempting to engineer a debt-for-equity swap, reducing its liabilities from $27.5 billion in unsecured debt to $9.2 billion.",
        "mention_ids": [
          "M094",
          "M122"
        ]
      },
      "11": {
        "orig_text": "It is also seeking to sell a truck manufacturer, the Delco Electronics parts group and the Hummer and Saab Automobile vehicle brands.",
        "text": "It is also seeking to sell a truck manufacturer, the Delco Electronics parts group and the Hummer and Saab Automobile vehicle brands.",
        "mention_ids": [
          "M103"
        ]
      },
      "12": {
        "orig_text": "The entire motor manufacturing sector has suffered under the economic downturn, with the Ford Motor Company announcing a $14.6 billion annual loss, although it has not sought federal aid.",
        "text": "The entire motor manufacturing sector has suffered under the economic downturn, with the Ford Motor Company announcing a $14.6 billion annual loss, although it has not sought federal aid.",
        "mention_ids": [
          "M082",
          "M130"
        ]
      },
      "13": {
        "orig_text": "GM and Chrysler both ran out of operating funds in December, leading to the federal bailout.",
        "text": "GM and Chrysler both ran out of operating funds in December, leading to the federal bailout.",
        "mention_ids": [
          "M113",
          "M128"
        ]
      }
    },
